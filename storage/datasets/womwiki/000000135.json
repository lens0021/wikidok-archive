#xcc2c;(&#x8d0a;)&#xd3ec;&#xb974;&#xb178;&#xadf8;&#xb798;&#xd53c; &#xd398;&#xbbf8;&#xb2c8;&#xc2a4;&#xd2b8;&#xb4e4; &#xc774; &#xad00;&#xc810;&#xc5d0;&#xc11c; &#xd3ec;&#xb974;&#xb178;&#xadf8;&#xb798;&#xd53c;&#xb294; &#xc5ec;&#xc131;&#xc758; &#xc131;&#xc801; &#xd45c;&#xd604;&#xc758; &#xb9e4;&#xccb4;&#xb85c; &#xbcf4;&#xc5ec;&#xc9c4;&#xb2e4;. &#xc131;(&#x6027;) &#xae0d;&#xc815;&#xc801; &#xd398;&#xbbf8;&#xb2c8;&#xc2a4;&#xd2b8;&#xb4e4;&#xc740; <a href=\"/wp-d/5e64da452eac37ae36f87897/View\" class=\"pjax-link\" data-mce-href=\"/wp-d/5e64da452eac37ae36f87897/View\">&#xb798;&#xb514;&#xceec; &#xd398;&#xbbf8;&#xb2c8;&#xc2a4;&#xd2b8;</a>&#xc758; &#xc139;&#xc288;&#xc5bc;&#xb9ac;&#xd2f0;&#xc5d0; &#xb300;&#xd55c; &#xad00;&#xc810;&#xc774; &#xac00;&#xbd80;&#xc7a5;&#xc801; &#xc885;&#xad50;&#xc640; &#xc774;&#xb370;&#xc62c;&#xb85c;&#xae30;&#xb9cc;&#xd07c;&#xc774;&#xb098; &#xc5b5;&#xc555;&#xc801;&#xc774;&#xb77c;&#xace0; &#xc8fc;&#xc7a5;&#xd558;&#xba70; &#xbc18;<span>(&#x53cd;)</span>&#xd3ec;&#xb974;&#xb178;&#xadf8;&#xb798;&#xd53c; &#xd398;&#xbbf8;&#xb2c8;&#xc2a4;&#xd2b8; &#xb2f4;&#xb860;&#xb4e4;&#xc774; &#xc5ec;&#xc131;&#xc758; &#xc131; &#xc8fc;&#xccb4;&#xc131;(agency)&#xc744; &#xbb34;&#xc2dc;&#xd558;&#xac70;&#xb098; &#xc0ac;&#xc18c;&#xd558;&#xac8c; &#xb9cc;&#xb4e0;&#xb2e4;&#xace0; &#xc8fc;&#xc7a5;&#xd55c;&#xb2e4;. &#xb2e4;&#xc218;&#xc758; <span>&#xc131;(&#x6027;) &#xae0d;&#xc815;&#xc801; &#xd398;&#xbbf8;&#xb2c8;&#xc2a4;&#xd2b8;</span>&#xb4e4;&#xc740; &#xd3ec;&#xb974;&#xb178;&#xadf8;&#xb798;&#xd53c;&#xac00; &#xadf8;&#xb4e4;&#xc774; &#xbc18;&#xb300;&#xd558;&#xb294; &#xc804;&#xd1b5;&#xc801; &#xc5ec;&#xc131;&#xc5d0; &#xb300;&#xd55c; &#xc0dd;&#xac01;&#xb4e4;&#xc744; &#xc804;&#xbcf5;&#xd558;&#xb294; &#xac83;&#xc73c;&#xb85c; &#xbcf8;&#xb2e4;. &#xc608;&#xb97c; &#xb4e4;&#xc5b4; &#xd3ec;&#xb974;&#xb178;&#xadf8;&#xb798;&#xd53c;&#xb294; &#xc885;&#xc885; &#xc5ec;&#xc131;&#xc774; &#xc131;&#xc801;&#xc73c;&#xb85c; &#xc9c0;&#xbc30;&#xc801;&#xc778; &#xc5ed;&#xd560;&#xc744; &#xd558;&#xb294; &#xac83;&#xc744; &#xbcf4;&#xc5ec;&#xc8fc;&#xac70;&#xb098; &#xc8fc;&#xb958; &#xc5d4;&#xd130;&#xd14c;&#xc778;&#xba3c;&#xd2b8;&#xc640; &#xd328;&#xc158;&#xc5d0; &#xbe44;&#xd574; &#xb2e4;&#xc591;&#xd55c; &#xc2e0;&#xccb4;&#xb97c; &#xbcf4;&#xc5ec;&#xc900;&#xb2e4;&#xb294; &#xc8fc;&#xc7a5;&#xc744; &#xd3bc;&#xce5c;&#xb2e4;.</p><p><br></p><p>&#xadf8;&#xb7ec;&#xb098; &#xc5ec;&#xc131;&#xc774; &#xc131;&#xc801;&#xc73c;&#xb85c; &#xc9c0;&#xbc30;&#xc801;&#xc778; &#xc5ed;&#xd560;&#xc744; &#xd558;&#xb294; &#xb3c4;&#xbbf8;&#xb124;&#xc774;&#xd2b8;&#xb9ad;&#xc2a4;[dominatrix]<span data-cls=\"foot\" class=\"wiki-fnote\" id=\"#wk_ft1\" contenteditable=\"false\"><button class=\"wiki-fnotepop\" data-container=\".wiki-fnote\" data-placement=\"auto bottom\" data-content=\"(%ED%9D%94%ED%9E%88%20%EC%84%B1%EC%A0%81%20%EC%BE%8C%EA%B0%90%EC%9D%84%20%EC%9C%84%ED%95%B4%20%ED%8F%AD%EB%A0%A5%EC%9D%84%20%ED%9C%98%EB%91%90%EB%A5%B4%EB%A9%B0)%20%EC%84%B1%ED%96%89%EC%9C%84%EB%A5%BC%20%EC%A3%BC%EB%8F%84%ED%95%98%EB%8A%94%20%EC%97%AC%EC%9E%90%2C%20BDSM%EC%97%90%EC%84%9C%20%ED%8E%A8%EB%8F%94(Female%20Dominant)%EB%9D%BC%EA%B3%A0%20%ED%91%9C%ED%98%84\">1</button></span>&#xa0;&#xa0; &#xc5ed;&#xc2dc; &#xc5ec;&#xc131;&#xc758; &#xbab8;&#xc774; &#xb4dc;&#xb7ec;&#xb098;&#xb294; &#xc637;&#xacfc; &#xd558;&#xc774;&#xd790;&#xc744; &#xcc29;&#xc6a9;&#xd558;&#xace0; &#xc788;&#xace0;, &#xc5ec;&#xc131;&#xc774; &#xc9c0;&#xbc30;&#xc801;&#xc778; &#xc704;&#xce58;&#xc640; &#xc5ed;&#xd560;&#xc744; &#xd558;&#xb294; &#xb4ef; &#xbcf4;&#xc774;&#xb098; &#xacb0;&#xad6d;&#xc5d0;&#xb294; &#xb0a8;&#xc131;&#xc758; &#xc131;&#xc801; &#xc695;&#xb9dd;&#xc744; &#xcda9;&#xc871;&#xc2dc;&#xd0a4;&#xae30; &#xc704;&#xd55c; &#xd2b9;&#xc815;&#xd55c; &#xc5ed;&#xd560;&#xc774;&#xb77c;&#xb294; &#xc810;&#xacfc;, &#xc2e0;&#xccb4;&#xc758; &#xb2e4;&#xc591;&#xc131;&#xbcf4;&#xb2e4;&#xb294; &#xc774;&#xc0c1;&#xc801;&#xc778; &#xc5ec;&#xc131;&#xc758; &#xbab8;&#xc5d0; &#xbc18;&#xd558;&#xb294; <a data-mce-href=\"/dok/&#xd0c0;&#xc790;&#xd654;\" href=\"/dok/&#xd0c0;&#xc790;&#xd654;\" class=\"pjax-link\">&#xd0c0;&#xc790;&#xd654;</a>/<a data-mce-href=\"/dok/&#xac1d;&#xccb4;&#xd654;\" href=\"/dok/&#xac1d;&#xccb4;&#xd654;\" class=\"pjax-link\">&#xac1d;&#xccb4;&#xd654;</a>&#xb41c; &#xbaa8;&#xc2b5; &#xb54c;&#xbb38;&#xc5d0; &#xc804;&#xd1b5;&#xc801; &#xc5ec;&#xc131;&#xc131;&#xc744; &#xc804;&#xbcf5;&#xd560; &#xc218; &#xc788;&#xb2e4;&#xb294; &#xadf8;&#xb4e4;&#xc758; &#xc8fc;&#xc7a5;&#xacfc;&#xb294; &#xb2ec;&#xb9ac; &#xc5ec;&#xc804;&#xd788; <a href=\"/wp-d/5e6387d9f2dd6e4a2c45684e/View\" class=\"pjax-link\" data-mce-href=\"/wp-d/5e6387d9f2dd6e4a2c45684e/View\">&#xc131;&#xc801; &#xb300;&#xc0c1;&#xd654;</a>&#xb418;&#xc5b4; &#xc788;&#xb2e4;.&#xa0;&#xb610;&#xd55c; &#xd2b9;&#xc815; &#xc778;&#xc885;(&#xb77c;&#xd2f4;&#xacc4;, &#xd751;&#xc778;, &#xbc31;&#xc778;, &#xc544;&#xc2dc;&#xc548; &#xb4f1;)&#xacfc; &#xb098;&#xc774;(&#xace0;&#xb4f1;&#xd559;&#xc0dd;, sweet sixteen, &#xc544;&#xc90c;&#xb9c8; &#xb4f1;), &#xc2e0;&#xccb4; &#xc7a5;&#xc560;(<a data-mce-href=\"/dok/&#xc0ac;&#xc9c0;&#xc808;&#xb2e8;\" href=\"/dok/&#xc0ac;&#xc9c0;&#xc808;&#xb2e8;\" class=\"pjax-link\">&#xc0ac;&#xc9c0;&#xc808;&#xb2e8;</a> &#xb4f1; &#xc2e0;&#xccb4;&#xac1c;&#xc870;)&#xc5d0; &#xb300;&#xd55c; &#xb3c4;&#xcc29;(fetishism)&#xc73c;&#xb85c; &#xc774;&#xc5b4;&#xc9c8; &#xac00;&#xb2a5;&#xc131;&#xc774; &#xb192;&#xb2e4;.</p><p><br></p><p>&#xd3ec;&#xb974;&#xb178;&#xadf8;&#xb798;&#xd53c;&#xc5d0; &#xb300;&#xd55c; &#xad00;&#xc810;&#xc5d0; &#xc0c1;&#xad00;&#xc5c6;&#xc774; &#xb9ce;&#xc740; &#xd398;&#xbbf8;&#xb2c8;&#xc2a4;&#xd2b8;&#xb4e4;&#xc740; &#xac80;&#xc5f4;&#xc758; &#xc6d0;&#xce59;&#xc5d0; &#xbc18;&#xb300;&#xd55c;&#xb2e4;. &#xd3ec;&#xb974;&#xb178;&#xadf8;&#xb798;&#xd53c;&#xb97c; &#xc131;&#xcc28;&#xbcc4;&#xc801; &#xae30;&#xad00;&#xc774;&#xb77c;&#xace0; &#xbcf4;&#xb294; &#xb2e4;&#xc218;&#xc758; &#xd398;&#xbbf8;&#xb2c8;&#xc2a4;&#xd2b8;&#xb4e4; &#xc5ed;&#xc2dc; &#xac80;&#xc5f4;&#xc744; &#xc545;&#xc73c;&#xb85c; &#xaddc;&#xc815;&#xd55c;&#xb2e4;. &#x2018;&#xc790;&#xc720; &#xd45c;&#xd604;&#xc744; &#xc704;&#xd55c; 