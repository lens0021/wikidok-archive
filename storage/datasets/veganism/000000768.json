	"postContents": "<div class=\"col-md-12 col-sm-12\">\r\n\t\t\t\t<p><br data-mce-bogus=\"1\"></p><p><img src=\"/api/File/Real/5abe4422b9d8af166fc703a8\" data-mce-src=\"/api/File/Real/5abe4422b9d8af166fc703a8\" width=\"379\" height=\"379\" style=\"display: block; margin-left: auto; margin-right: auto;\" data-mce-style=\"display: block; margin-left: auto; margin-right: auto;\"></p><p><br data-mce-bogus=\"1\"></p><h2>1. &#xc65c; &#xbe44;&#xac74; &#xd3b8;&#xc758;&#xc810;&#xc778;&#xac00;&#xc694;?</h2><p>&#xac00;&#xc7a5; &#xc811;&#xadfc;&#xc131;&#xc774; &#xc88b;&#xc740; &#xd3b8;&#xc758;&#xc810;&#xc5d0;&#xc11c; <a href=\"http://ko.veganism.wikidok.net/wp-d/595c78521fafad8d03baa9d0/View\" class=\"pjax-link\" data-mce-href=\"http://ko.veganism.wikidok.net/wp-d/595c78521fafad8d03baa9d0/View\">&#xd06c;&#xb8e8;&#xc5bc;&#xd2f0;&#xd504;&#xb9ac;(Cruelty-Free)</a>, &#xbe44;&#xac74; &#xc81c;&#xd488;&#xc744; &#xc2ec;&#xc2ec;&#xce58; &#xc54a;&#xac8c; &#xb9cc;&#xb098;&#xbcf4;&#xace0; &#xc2f6;&#xc740; &#xc5fc;&#xc6d0;&#xc744; &#xb2f4;&#xc544; &#xc774;&#xb807;&#xac8c; &#xc9c0;&#xc5c8;&#xc5b4;&#xc694;. &#xbe44;&#xac74;&#xd3b8;&#xc758;&#xc810; &#xc704;&#xd0a4;&#xb294; &#xac74;&#xac15; &#xcc44;&#xc2dd;&#xc778;&#xc740; &#xbb3c;&#xb860; &#xc815;&#xd06c; &#xd478;&#xb4dc;&#xb97c; &#xba39;&#xc73c;&#xba70; &#xadfc;&#xadfc;&#xd788; &#xc0b4;&#xc544;&#xac00;&#xb294; &#xcc44;&#xc2dd;&#xc778;&#xb4e4;&#xb3c4; &#xd3b8;&#xd558;&#xac8c; &#xbe44;&#xac70;&#xb2c8;&#xc998;&#xc744; &#xc2e4;&#xcc9c;&#xd560; &#xc218; &#xc788;&#xac8c; &#xb3c4;&#xc640;&#xc90d;&#xb2c8;&#xb2e4;. &#xadf8;&#xb7ec;&#xb2e4;&#xbcf4;&#xba74; &#xcc44;&#xc2dd;&#xc744; &#xd5a5;&#xd55c; &#xc9c4;&#xc785;&#xc7a5;&#xbcbd;&#xb3c4; &#xb0ae;&#xc544;&#xc9c0;&#xb9ac;&#xb77c; &#xbbff;&#xc5b4;&#xc694;. &#xc644;&#xbcbd;&#xd55c; &#xbe44;&#xac74;(&#xc774;&#xb780; &#xac74; &#xc788;&#xc744; &#xc218; &#xc5c6;&#xaca0;&#xc9c0;&#xb9cc;)&#xbcf4;&#xb2e4;&#xb3c4; &#xd5c8;&#xc220;&#xd558;&#xace0; &#xc644;&#xbcbd;&#xd574;&#xbcf4;&#xc774;&#xc9c0; &#xc54a;&#xb294; &#xcc44;&#xc2dd;&#xc8fc;&#xc758;&#xc790; &#xc5f4; &#xba85;&#xc774; &#xcd5c;&#xc57d;&#xc790;&#xc778; &#xbe44;&#xc778;&#xac04;&#xb3d9;&#xbb3c; &#xd574;&#xbc29;&#xc5d0; &#xb3c4;&#xc6c0;&#xc774; &#xb420; &#xd14c;&#xb2c8;&#xae4c;&#xc694;.&#xa0;</p><p><br></p><h2>2. &#xbb34;&#xc5c7;&#xc744; &#xc4f0;&#xb098;&#xc694;?</h2><p>&#xc0ac;&#xd68c; &#xbb38;&#xd654; &#xc804;&#xbc18;&#xc5d0; &#xac78;&#xccd0; &#xbe44;&#xac70;&#xb2c8;&#xc998;&#xacfc; &#xad00;&#xb828;&#xb41c; &#xcee8;&#xd150;&#xce20;&#xb77c;&#xba74; &#xbb34;&#xc5c7;&#xc774;&#xb4e0; &#xc88b;&#xc2b5;&#xb2c8;&#xb2e4;. &#xcea0;&#xd398;&#xc778;, &#xd398;&#xc2a4;&#xd2f0;&#xbc8c;, &#xc0ac;&#xac74; &#xc0ac;&#xace0;, &#xbe44;&#xac74; &#xb9db;&#xc9d1; &#xb4f1; &#xacf5;&#xc720;&#xd558;&#xace0; &#xc2f6;&#xc740; &#xc815;&#xbcf4;&#xb97c; &#xc62c;&#xb824;&#xc8fc;&#xc138;&#xc694;. &apos;&#xbe44;&#xac70;&#xb2c8;&#xc998;&apos; &#xce74;&#xd14c;&#xace0;&#xb9ac;&#xc5d0; &#xbe44;&#xac74;&#xd3b8;&#xc758;&#xc810; &#xc704;&#xd0a4;&#xac00; &#xc9c0;&#xd5a5;&#xd558;&#xb294; &#xbc14;&#xb97c; &#xad6c;&#xccb4;&#xc801;&#xc73c;&#xb85c; &#xc801;&#xc5b4;&#xb458; &#xc608;&#xc815;&#xc785;&#xb2c8;&#xb2e4;. &#xcc38;&#xace0;&#xd574;&#xc8fc;&#xc138;&#xc694;. (&#xd398;&#xbbf8;&#xb2c8;&#xc998;&#xacfc; &#xbe44;&#xac70;&#xb2c8;&#xc998;&#xc774; &#xae34;&#xbc00;&#xd558;&#xb2e4;&#xace0; &#xc0dd;&#xac01;&#xd558;&#xbbc0;&#xb85c; &#xbc18;&#xd398;&#xbbf8;&#xb2c8;&#xc998; &#xc131;&#xaca9;&#xc744; &#xb744;&#xb294; &#xae00;&#xc740; &#xbb34;&#xb2e8;&#xc73c;&#xb85c; &#xc9c0;&#xc6cc;&#xc9c8; &#xc218;&#xb3c4; &#xc788;&#xc2b5;&#xb2c8;&#xb2e4;.)</p><p><br></p><h2>3. &#xbe44;&#xac74;&#xb9cc; &#xae00; &#xc4f8; &#xc218; &#xc788;&#xb098;&#xc694;?</h2><p>&#xadf8;&#xb7f4;&#xb9ac;&#xac00;&#xc694;... &#xbe44;&#xac70;&#xb2c8;&#xc998;&#xc5d0; &#xd765;&#xbbf8;&#xac00; &#xc788;&#xb294; &#xbaa8;&#xb4e0; &#xbd84;&#xb4e4;&#xc5d0;&#xac8c; &#xc5f4;&#xb824; &#xc788;&#xc2b5;&#xb2c8;&#xb2e4;. &#xc790;&#xb3d9; &#xc2b9;&#xc778; &#xbc29;&#xc2dd;&#xc73c;&#xb85c; &#xb204;&#xad6c;&#xb098; &#xba64;&#xbc84;&#xac00; &#xb418;&#xc5b4; &#xae00;&#xc744; &#xc4f8; &#xc218; &#xc788;&#xb3c4;&#xb85d; &#xd588;&#xc2b5;&#xb2c8;&#xb2e4;. &#xb204;&#xad6c;&#xb098; &#xc4f8; &#xc218; &#xc788;&#xace0;, &#xc6b4;&#xc601;&#xc9c4;&#xc774; &#xbb38;&#xc11c;&#xd3b8;&#xc9d1;&#xc744; &#xc7a0;&#xae08;&#xd55c; &#xac8c;&#xc2dc;&#xbb3c;&#xc744; &#xc81c;&#xc678;&#xd558;&#xace0; &#xb204;&#xad6c;&#xb098; &#xc5b4;&#xb5a4; &#xbb38;&#xc11c;&#xb4e0; &#xd3b8;&#xc9d1;&#xd560; &#xc218; &#xc788;&#xc2b5;&#xb2c8;&#xb2e4;.&#xa0;</p><p><br></p><h2>4. &#xadf8;&#xb9ac;&#xace0; &#xc5ec;&#xb7ec;&#xbd84;&#xb4e4;&#xaed8;</h2><p>&#xc62c; &#xcd08; &#xc18c;&#xc18c;&#xd558;&#xac8c; &#xbca0;&#xc9c0;&#xd14c;&#xb9ac;&#xc5b8; &#xce5c;&#xad6c;&#xb4e4;&#xacfc; &#xbe44;&#xac70;&#xb2c8;&#xc998; &#xbd07;&#xc744; &#xc6b4;&#xc601;&#xd574;&#xbcf4;&#xc790;&#xace0; &#xd638;&#xae30;&#xb86d;&#xac8c; &#xb9d0;&#xd558;&#xace0; &#xbc18; &#xb144;&#xc774; &#xc9c0;&#xb098;&#xc11c; &#xc704;&#xd0a4;&#xb97c; &#xac1c;&#xc124;&#xd558;&#xac8c; &#xb418;&#xc5c8;&#xc2b5;&#xb2c8;&#xb2e4;. &#xcd5c;&#xadfc; &#xd2b8;&#xc704;&#xd130;&#xc5d0;&#xc11c;&#xb3c4; &#xbe44;&#xac70;&#xb2c8;&#xc998;&#xc744; &#xb9d0;&#xd558;&#xb294; &#xc0ac;&#xb78c;&#xb4e4;&#xc774; &#xb298;&#xc5c8;&#xace0;, &#xc774; &#xc2dc;&#xb958;&#xb97c; &#xd0c0;&#xc11c; &#xc704;&#xd0a4;&#xd53c;&#xb514;&#xc544;&#xb97c; &#xb9cc;&#xb4e4;&#xba74; &#xbaa8;&#xb450;&#xac00; &#xc990;&#xac81;&#xace0; &#xc720;&#xc775;&#xd558;&#xac8c; &#xd65c;&#xc6a9;&#xd560; &#xc218; &#xc788;&#xc744; &#xac83; &#xac19;&#xc558;&#xc5b4;&#xc694;. &#xd2b8;&#xc704;&#xd130;&#xc758; @veganism_bot &#xc744; &#xd1b5;&#xd574; &#xc704;&#xd0a4; &#xb0b4;&#xc6a9;&#xc740; &#xbb3c;&#xb860; &#xb2e4;&#xc591;&#xd55c; &#xcee8;&#xd150;&#xce20;&#xb97c; &#xc62c;&#xb9b4; &#xc608;&#xc815;&#xc774;&#xba70; &#xc6b4;&#xc601;&#xc9c4;&#xc740; &#xc0c1;&#xc2dc; &#xbaa8;&#xc9d1;&#xd558;&#xace0; &#xc788;&#xc2b5;&#xb2c8;&#xb2e4;. &#xbe44;&#xac74; &#xd3b8;&#xc758;&#xc810; &#xc704;&#xd0a4;&#xc640; &#xbe44;&#xac70;&#xb2c8;&#xc998;&#xbd07; &#xc6b4;&#xc601;&#xc9c4;&#xc740; &#xac01;&#xac01; &#xb530;&#xb85c; &#xbaa8;&#xc9d1;&#xd569;&#xb2c8;&#xb2e4;. &#xd574;&#xb2f9; &#xacc4;&#xc815;&#xc744; &#xd1b5;&#xd574; &#xbb38;&#xc758;&#xd574;&#xc8fc;&#xc138;&#xc694;.&#xa0;</p><p><br data-mce-bogus=\"1\"></p><p>(2018-03-30) &#xc0c1;&#xbc18;&#xae30; &#xae30;&#xd68d;&#xd300;&#xc6d0;&#xc774; &#xcda9;&#xc6d0;&#xb418;&#xc5c8;&#xc2b5;&#xb2c8;&#xb2e4;. &#xcf5c;&#xb4dc;&#xbe0c;&#xb8e8;&#xb2d8;, &#xd574;&#xc544;&#xb2d8;&#xacfc; &#xd568;&#xaed8; &#xd558;&#xace0; &#xc788;&#xc2b5;&#xb2c8;&#xb2e4;. &#xc704;&#xd0a4;&#xd53c;&#xb514;&#xc544; &#xbaa8;&#xb2c8;&#xd130;&#xb9c1;&#xacfc; &apos;&#xbe44;&#xac74;&#xc5d0;&#xac8c; &#xbb3b;&#xb294; &#xc9c8;&#xbb38;&#xb4e4;&apos;&#xc744; &#xc218;&#xc9d1;&#xd558;&#xace0; &#xb2f5;&#xbcc0;&#xc744; &#xc791;&#xc131; &#xc911;&#xc785;&#xb2c8;&#xb2e4;. &#xbe44;&#xac70;&#xb2c8;&#xc998; &#xc9c0;&#xd615;&#xb3c4; &#xb9e4;&#xd551; &#xc791;&#xc5c5;&#xc740; &#xcf5c;&#xb4dc;&#xbe0c;&#xb8e8;&#xb2d8;&#xacfc;&#xb3c4; &#xd568;&#xaed8; &#xd560; &#xc608;&#xc815;&#xc785;&#xb2c8;&#xb2e4;.&#xa0;</p><p><br></p><p>&#xb610;&#xd55c; &#xbe44;&#xac74;&#xd3b8;&#xc758;&#xc810; &#xc704;&#xd0a4;&#xb294; &#xc544;&#xb984;&#xb4dc;&#xb9ac;&#xc704;&#xd0a4;&#xc758; &#xb9ce;&#xc740; &#xbd80;&#xbd84;&#xc744; &#xcc38;&#xace0;&#xd558;&#xace0; &#xbe5a;&#xc9c0;&#xace0; &#xc788;&#xc2b5;&#xb2c8;&#xb2e4;. &#xbc88;&#xcc3d;&#xd558;&#xc138;&#xc694; &#xc544;&#xb984;&#xb4dc;&#xb9ac;&#xc704;&#xd0a4;..&#xa0;</p><p><br></p><p><br></p>\r\n\t\t\t</div>"
