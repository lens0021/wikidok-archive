	"postContents": "<div class=\"col-md-12 col-sm-12\">\r\n\t\t\t\t<p><img src=\"/api/File/Real/57979178e1db80c0295ecc79\" data-mce-src=\"/api/File/Real/57979178e1db80c0295ecc79\" width=\"354\" height=\"434\" style=\"display: block; margin-left: auto; margin-right: auto;\" data-mce-style=\"display: block; margin-left: auto; margin-right: auto;\"><span id=\"#wk_ft1\" class=\"wiki-fnote\" contenteditable=\"false\" data-cls=\"foot\"><button class=\"wiki-fnotepop\" data-container=\".wiki-fnote\" data-placement=\"auto bottom\" data-content=\"&#xc81c;&#xc0ac;&#xc7a5;&#xc5d0;&#xc11c; &#xc7ac;&#xd68c;&#xd588;&#xc744;&#xb54c;&#xc758; &#xbaa8;&#xc2b5;.\">1</button></span>&#xa0;</p><p>&#xa0;<br></p><h2>&#xac1c;&#xc694;</h2><p><a class=\"pjax-link\" title=\"&#xb2e4;&#xd06c;&#xc18c;&#xc6b8; 3\" href=\"/wp-d/5794e586e70c5cb308fc2b65/View\" data-mce-href=\"/wp-d/5794e586e70c5cb308fc2b65/View\">&#xb2e4;&#xd06c; &#xc18c;&#xc6b8; 3</a>&#xc758; &#xb4f1;&#xc7a5; NPC&#xc778; &#xc544;&#xc2a4;&#xd1a0;&#xb77c;&#xc758; &#xc559;&#xb9ac;&#xb97c; &#xc18c;&#xac1c;&#xd558;&#xb294; &#xbb38;&#xc11c;.&#xa0;<br></p><h2>&#xc0c1;&#xc138;</h2><p>&#xa0; &#xc0b0; &#xc81c;&#xbb3c;&#xc758; &#xae38;&#xc5d0; &#xc788;&#xb294; &#xae38;&#xc774; &#xb04a;&#xae34; &#xc694;&#xc0c8; &#xd654;&#xd1b3;&#xbd88; &#xadfc;&#xcc98;&#xc5d0;&#xc11c; &#xcc98;&#xc74c;&#xc73c;&#xb85c; &#xb9cc;&#xb098;&#xac8c; &#xb418;&#xb294; NPC. &#xb3d9;&#xb8cc;&#xc778; &#xd638;&#xb808;&#xc774;&#xc2a4;&#xc640; &#xd568;&#xaed8; &#xc788;&#xb2e4;.</p><p>&#xa0; &#xd50c;&#xb808;&#xc774;&#xc5b4;&#xc640; &#xb9c8;&#xcc2c;&#xac00;&#xc9c0;&#xb85c; &#xbd88; &#xaebc;&#xc9c4; &#xc7ac;&#xc774;&#xba70;, &#xc8fc;&#xbcc0; &#xc9c0;&#xc5ed;&#xc758; &#xc18c;&#xac1c;&#xb97c; &#xd574;&#xc8fc;&#xba74;&#xc11c; &#xc790;&#xc2e0;&#xb4e4;&#xc740; &#xc5d8;&#xb4dc;&#xb9ac;&#xce58;&#xc758; &#xace0;&#xd5a5;&#xc774;&#xb77c;&#xb294; &#xae4a;&#xc740; &#xacf3;&#xc758; &#xc131;&#xb2f9;&#xc744; &#xd5a5;&#xd558;&#xace0; &#xc788;&#xb2e4;&#xace0; &#xd55c;&#xb2e4;. &#xc7a5;&#xc791;&#xc758; &#xc655; &#xc0c1;&#xd0dc;&#xb77c;&#xba74; &#xae4a;&#xc740; &#xacf3;&#xc758; &#xc8fc;&#xad50;&#xb4e4; &#xbcf4;&#xc2a4;&#xc804;&#xc5d0;&#xc11c; &#xbc31;&#xb839;&#xc73c;&#xb85c; &#xc18c;&#xd658;&#xd560; &#xc218; &#xc788;&#xb2e4;.</p><p>&#xa0; &#xae4a;&#xc740; &#xacf3;&#xc758; &#xc8fc;&#xad50;&#xb4e4;&#xc744; &#xaca9;&#xd1f4; &#xd6c4; &#xc81c;&#xc0ac;&#xc7a5;&#xc5d0;&#xc11c; &#xd638;&#xb808;&#xc774;&#xc2a4;&#xc640; &#xd568;&#xaed8; &#xb2e4;&#xc2dc; &#xb9cc;&#xb0a0; &#xc218; &#xc788;&#xb294;&#xb370;, &#xc5d8;&#xb4dc;&#xb9ac;&#xce58;&#xb294; &#xadf8; &#xacf3;&#xc5d0; &#xc5c6;&#xc5c8;&#xc73c;&#xba70; &#xd314;&#xb780;&#xc758; &#xc694;&#xc0c8; &#xb108;&#xba38;&#xc5d0; &#xc788;&#xb294; &#xc5d8;&#xb4dc;&#xb9ac;&#xce58;&#xc758; &#xc9c4;&#xc815;&#xd55c; &#xace0;&#xd5a5;, &#xc774;&#xb8e8;&#xc2e4;&#xb85c; &#xd5a5;&#xd558;&#xaca0;&#xb2e4;&#xb294; &#xb9d0;&#xc744; &#xd55c;&#xb2e4;.</p><p>&#xb300;&#xd654; &#xd6c4; &#xce74;&#xc0ac;&#xc2a4;&#xc758; &#xc9c0;&#xd558;&#xbb18;&#xb85c; &#xc9c4;&#xc785;&#xd558;&#xba74; &#xc559;&#xb9ac; &#xd63c;&#xc790; &#xc788;&#xb294; &#xac83;&#xc744; &#xbc1c;&#xacac;&#xd560; &#xc218; &#xc788;&#xb294;&#xb370;, &#xd568;&#xc815;&#xc5d0; &#xbe60;&#xc838; &#xd638;&#xb808;&#xc774;&#xc2a4;&#xc640; &#xd5e4;&#xc5b4;&#xc84c;&#xb2e4;&#xace0; &#xd55c;&#xb2e4;.&#xa0;&#xd50c;&#xb808;&#xc774;&#xc5b4;&#xc5d0;&#xac8c; &#xb9cc;&#xc57d; &#xd638;&#xb808;&#xc774;&#xc2a4;&#xb97c; &#xb9cc;&#xb098;&#xac8c; &#xb41c;&#xb2e4;&#xba74; &#xc790;&#xc2e0;&#xc740; &#xc544;&#xc9c1; &#xc9c0;&#xd558;&#xbb18;&#xc5d0; &#xc788;&#xc73c;&#xba70;, &#xadf8;&#xb97c; &#xc704;&#xd574; &#xce60;&#xc0c9;&#xc11d;&#xc744; &#xb193;&#xc544;&#xb450;&#xaca0;&#xb2e4;&#xb294; &#xb9d0;&#xc744; &#xc804;&#xd574;&#xb2ec;&#xb77c;&#xace0; &#xbd80;&#xd0c1;&#xd55c;&#xb2e4;.</p><p>&#xa0; &#xc774;&#xd6c4;, &#xd328;&#xc655; &#xc6cc;&#xb2d0;&#xc5d0;&#xac8c; &#xd5a5;&#xd558;&#xb294; &#xb0a1;&#xc740; &#xb098;&#xbb34;&#xb2e4;&#xb9ac; &#xadfc;&#xcc98;&#xc5d0;&#xc11c; &#xb2e4;&#xc2dc; &#xc559;&#xb9ac;&#xb97c; &#xb9cc;&#xb0a0; &#xc218; &#xc788;&#xb2e4;.<span id=\"#wk_ft2\" class=\"wiki-fnote\" contenteditable=\"false\" data-cls=\"foot\"><button class=\"wiki-fnotepop\" data-container=\".wiki-fnote\" data-placement=\"auto bottom\" data-content=\"&#xd328;&#xc655; &#xc6cc;&#xb2d0;&#xcabd;&#xc5d0;&#xc11c; &#xac74;&#xb108;&#xc624;&#xba74; &#xbc18;&#xb300;&#xd3b8;&#xc5d0;&#xc11c; &#xac74;&#xb108;&#xc624;&#xb294; &#xac83;&#xbcf4;&#xb2e4; &#xb354; &#xb9ce;&#xc740; &#xd574;&#xace8;&#xbcd1;&#xc0ac;&#xb4e4;&#xc774; &#xd50c;&#xb808;&#xc774;&#xc5b4;&#xb97c; &#xb530;&#xb77c;&#xc624;&#xae30; &#xb54c;&#xbb38;&#xc5d0; &#xd070;&#xc77c;&#xb09c;&#xb2e4;!\">2</button></span>&#xa0;&#xa0;</p><p>&#xa0; &#xb9cc;&#xc57d; &#xd638;&#xb808;&#xc774;&#xc2a4;&#xb97c; &#xbc1c;&#xacac;&#xd558;&#xc9c0; &#xbabb;&#xd588;&#xb2e4;&#xba74; &#xc559;&#xb9ac;&#xc758; &#xc9c8;&#xbb38;&#xc5d0; &apos;&#xbaa8;&#xb978;&#xb2e4;&apos;&#xb77c;&#xb294; &#xc751;&#xb2f5;&#xb9cc; &#xc120;&#xd0dd;&#xd560; &#xc218; &#xc788;&#xc73c;&#xba70;, &#xd638;&#xb808;&#xc774;&#xc2a4;&#xb97c; &#xbc1c;&#xacac;&#xd588;&#xb2e4;&#xba74; &#xc559;&#xb9ac;&#xc758; &#xc9c8;&#xbb38;&#xc5d0; &apos;&#xadf8;&#xc744;&#xb9b0; &#xd638;&#xc218;&#xb97c; &#xac00;&#xb974;&#xccd0; &#xc900;&#xb2e4;&apos;, &apos;&#xbaa8;&#xb978;&#xb2e4;&apos; &#xc911; &#xd558;&#xb098;&#xb97c; &#xc120;&#xd0dd;&#xd560; &#xc218; &#xc788;&#xb2e4;.</p><p>&#xa0;<br></p><p>&#xa0;</p><p><br></p><div contenteditable=\"false\"><div class=\"alert alert-warning\" contenteditable=\"true\" style=\"text-align: center;\" data-mce-style=\"text-align: center;\">&#xa0;&#xa0;<strong>&#xc791; &#xc911; &#xc2a4;&#xd3ec;&#xc77c;&#xb7ec; &#xc8fc;&#xc758;: &#xd018;&#xc2a4;&#xd2b8; &#xad00;&#xb828; &#xc2a4;&#xd3ec;&#xc77c;&#xb7ec;</strong></div></div><p>&#xa0;<br></p><p>&#xa0;</p><h3>&#xbd84;&#xae30;</h3><p>&#xa0; &#xc559;&#xb9ac; &#xd018;&#xc2a4;&#xd2b8;&#xb77c;&#xc778;&#xc758; &#xbd84;&#xae30;&#xb294; &#xd06c;&#xac8c; &#xb450; &#xac00;&#xc9c0; &#xc694;&#xc18c;&#xc5d0; &#xc601;&#xd5a5;&#xc744; &#xbc1b;&#xb294;&#xb370;, &#xd558;&#xb098;&#xb294; &#xb9dd;&#xc790;&#xac00; &#xb41c; &#xd638;&#xb808;&#xc774;&#xc2a4;&#xb97c; (&#xc5b8;&#xc81c;) &#xc7a1;&#xb290;&#xb0d0;, &#xb2e4;&#xb978; &#xd558;&#xb098;&#xb294; &#xc694;&#xb974;&#xc2dc;&#xce74; &#xad50;&#xd68c;&#xc5d0;&#xc11c; &#xb9cc;&#xb0ac;&#xc744; &#xb54c; &#xc228;&#xc5b4; &#xc788;&#xb294; &#xc21c;&#xb840;&#xc790;&#xb97c; &#xc7a1;&#xb290;&#xb0d0;&#xc774;&#xb2e4;.</p><p>&#xa0; &#xc559;&#xb9ac;&#xc5d0;&#xac8c; &#xb9d0;&#xc744; &#xac78;&#xc5c8;&#xb4e0;, &#xac78;&#xc9c0; &#xc54a;&#xc558;&#xb4e0; &#xd328;&#xc655; &#xc6cc;&#xb2d0;&#xc744; &#xc7a1;&#xace0; &#xb098;&#xba74; &#xcc28;&#xac00;&#xc6b4; &#xace8;&#xc9dc;&#xae30;&#xc758; &#xc774;&#xb8e8;&#xc2e4; &#xcabd;&#xc73c;&#xb85c; &#xc774;&#xb3d9;&#xd558;&#xac8c; &#xb41c;&#xb2e4;.&#xa0;&#xd638;&#xb808;&#xc774;&#xc2a4; &#xc2e4;&#xc885; &#xc774;&#xbca4;&#xd2b8;&#xb97c; &#xc9c4;&#xd589;&#xd558;&#xace0; &#xc2f6;&#xc740; &#xc0ac;&#xb78c;&#xc740; &#xd328;&#xc655; &#xc6cc;&#xb2d0;&#xc744; &#xc7a1;&#xc9c0; &#xc54a;&#xb3c4;&#xb85d; &#xc8fc;&#xc758;&#xd560; &#xac83;.</p><h4>1. &#xb9dd;&#xc790;&#xac00; &#xb41c; &#xd638;&#xb808;&#xc774;&#xc2a4;&#xb97c; &#xc4f0;&#xb7ec;&#xb728;&#xb9ac;&#xc9c0; &#xc54a;&#xace0; &#xc704;&#xce58;&#xb97c; &#xb9d0;&#xd574;&#xc900;&#xb2e4;</h4><p>&#xa0; &#xc774; &#xacbd;&#xc6b0; &#xc559;&#xb9ac;&#xb294; &#xace0;&#xb9d9;&#xb2e4;&#xb294; &#xb9d0;&#xacfc; &#xd568;&#xaed8; &#xd50c;&#xb808;&#xc774;&#xc5b4;&#xc5d0;&#xac8c; &#xc0ac;&#xc548;&#xc758; &#xbc18;&#xc9c0;&#xb97c; &#xc8fc;&#xb294;&#xb370;, &#xc774; &#xc0c1;&#xd0dc;&#xc5d0;&#xc11c; &#xd328;&#xc655; &#xc6cc;&#xb2d0;&#xc744; &#xc4f0;&#xb7ec;&#xb728;&#xb9b0; &#xb4a4; &#xd638;&#xb808;&#xc774;&#xc2a4;&#xac00; &#xc788;&#xb358; &#xacf3;&#xc73c;&#xb85c; &#xac00;&#xba74; &#xc559;&#xb9ac;&#xc758; &#xc2dc;&#xccb4;&#xb97c; &#xbc1c;&#xacac;&#xd560; &#xc218; &#xc788;&#xc73c;&#xba70; &#xc2dc;&#xccb4;&#xc5d0;&#xc11c; &#xc559;&#xb9ac;&#xc758; &#xc9c1;&#xac80;&#xc744; &#xc5bb;&#xc744; &#xc218; &#xc788;&#xb2e4;. &#xc774;&#xd6c4; &#xc559;&#xb9ac; &#xad00;&#xb828; &#xd018;&#xc2a4;&#xd2b8;&#xb77c;&#xc778;&#xc744; &#xb354; &#xc774;&#xc0c1; &#xc9c4;&#xd589;&#xd560; &#xc218; &#xc5c6;&#xae30;&#xc5d0; &#xc774;&#xd6c4; &#xb354; &#xc9c4;&#xd589;&#xd558;&#xace0; &#xc2f6;&#xc740; &#xc774;&#xb4e4;&#xc774;&#xb098; &#xd2b9;&#xc815; &#xc5d4;&#xb529;&#xc744; &#xb178;&#xb9ac;&#xb294; &#xc774;&#xb4e4;&#xc774;&#xb77c;&#xba74; &#xc8fc;&#xc758;&#xd574;&#xc57c; &#xd55c;&#xb2e4;.</p><h4>2. &#xc704;&#xce58;&#xb97c; &#xb9d0;&#xd574;&#xc8fc;&#xc9c0; &#xc54a;&#xb294;&#xb2e4; or &#xb9dd;&#xc790;&#xac00; &#xb41c; &#xd638;&#xb808;&#xc774;&#xc2a4;&#xb97c; &#xc4f0;&#xb7ec;&#xb728;&#xb9b0; &#xb4a4; &#xc704;&#xce58;&#xb97c; &#xb9d0;&#xd574;&#xc900;&#xb2e4;</h4><p>&#xa0; &#xce74;&#xc0ac;&#xc2a4;&#xc758; &#xc9c0;&#xd558; &#xbb18;&#xc9c0;&#xc5d0;&#xc11c; &#xb9cc;&#xb0ac;&#xc744; &#xb54c; &#xd50c;&#xb808;&#xc774;&#xc5b4;&#xac00; &#xd638;&#xb808;&#xc774;&#xc2a4;&#xc758; &#xc704;&#xce58;&#xb97c; &#xb9d0;&#xd558;&#xba74; &#xace0;&#xb9d9;&#xb2e4;&#xb294; &#xb9d0;&#xacfc; &#xd568;&#xaed8; &#xc0ac;&#xc548;&#xc758; &#xbc18;&#xc9c0;&#xb97c; &#xbc1b;&#xc744; &#xc218; &#xc788;&#xb2e4;.</p><p>&#xa0; &#xc774;&#xd6c4; &#xcc28;&#xac00;&#xc6b4; &#xace8;&#xc9dc;&#xae30;&#xc758; &#xc774;&#xb8e8;&#xc2e4;&#xc5d0; &#xc788;&#xb294; &#xc694;&#xb974;&#xc2dc;&#xce74; &#xad50;&#xd68c;&#xc5d0;&#xc11c; &#xc7ac;&#xd68c;&#xd560; &#xc218; &#xc788;&#xb2e4;.&#xa0;&#xd50c;&#xb808;&#xc774;&#xc5b4;&#xac00; &#xd638;&#xb808;&#xc774;&#xc2a4;&#xc758; &#xc704;&#xce58;&#xb97c; &#xb9d0;&#xd558;&#xc9c0; &#xc54a;&#xc740; &#xacbd;&#xc6b0;&#xc5d0;&#xb294; &#xd50c;&#xb808;&#xc774;&#xc5b4;&#xc5d0;&#xac8c; &#xbb34;&#xc758;&#xbbf8;&#xd55c; &#xbd80;&#xd0c1;&#xc744; &#xd588;&#xb2e4;&#xba70; &#xc0ac;&#xacfc;&#xc640; &#xae30;&#xc6d0;&#xc758; &#xc758;&#xbbf8;&#xb85c; &#xc0ac;&#xc548;&#xc758; &#xbc18;&#xc9c0;&#xb97c; &#xac74;&#xb128;&#xb2e4;.&#xa0;</p><p>&#xa0; &#xadf8;&#xb9ac;&#xace0; &#xd50c;&#xb808;&#xc774;&#xc5b4;&#xac00; &#xd640;&#xb85c; &#xc0ac;&#xba85;&#xc5d0; &#xb9de;&#xc11c;&#xb294; &#xad73;&