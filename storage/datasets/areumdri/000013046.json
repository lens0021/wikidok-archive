8; &#xc774;&#xc911;&#xc7a3;&#xb300;&#xac00; &#xb124;&#xd2f0;&#xc98c;&#xc758; &#xb3d9;&#xc758;&#xc744; &#xc5bb;&#xc73c;&#xba74;&#xc11c; &#xc5ec;&#xcd08; &#xcee4;&#xbba4;&#xb2c8;&#xd2f0;, SNS&#xb4f1;&#xc5d0;&#xc11c; &#xd654;&#xc81c;&#xac00; &#xb418;&#xc5c8;&#xb2e4;. &#xc774;&#xc5d0; &#xb0a8;&#xcd08; &#xcee4;&#xbba4;&#xb2c8;&#xd2f0;&#xc5d0;&#xc11c;&#xb294; &#xc774;&#xc911;&#xc7a3;&#xb300;&#xac00; &#xc544;&#xb2cc; &#xadfc;&#xac70;&#xb97c; &#xcc3e;&#xb294; &#xb370; &#xc8fc;&#xb825;&#xd558;&#xba70; &#xbb38;&#xc81c;&#xac00; &#xc5c6;&#xb2e4;&#xb294; &#xc785;&#xc7a5;&#xc774;&#xb2e4;.</p><p>&#xa0;<br></p><p>&#xacbd;&#xcc30;&#xc774; &#xd328;&#xce58; &#xc0ac;&#xac74;&#xc5d0; &#xc774;&#xc911;&#xc7a3;&#xb300;&#xb97c; &#xc801;&#xc6a9;&#xd588;&#xb294;&#xc9c0;&#xb294; &#xc815;&#xd655;&#xd788; &#xc54c;&#xae30; &#xc5b4;&#xb835;&#xb2e4;. &#xadf8;&#xb7ec;&#xb098; &#xb2e4;&#xc218; &#xc5ec;&#xc131; &#xb124;&#xd2f0;&#xc98c;&#xc740; &#xc774; &#xc0ac;&#xac74; &#xbfd0;&#xb9cc; &#xc544;&#xb2c8;&#xb77c; &#xc5ed;&#xc0ac;&#xc801;&#xc73c;&#xb85c; &#xacf5;&#xad8c;&#xb825;&#xacfc; &#xc5b8;&#xb860;&#xc774; &#xb0a8;&#xc131;&#xc758; &#xc785;&#xc7a5;&#xc5d0; &#xc11c;&#xc11c; &#xc5ec;&#xc131;&#xc744; &#xd0c0;&#xc790;&#xd654;&#xd574; &#xc654;&#xb2e4;&#xb294; &#xc810;, &#xc5ec;&#xc131;&#xc758; &#xc2e0;&#xc0c1; &#xc815;&#xbcf4;&#xb97c; &#xc720;&#xcd9c;&#xd558;&#xb294; &#xacc4;&#xc815;&#xb4e4;&#xc774; &#xc544;&#xc9c1; &#xc218;&#xc0ac; &#xb300;&#xc0c1;&#xc774; &#xb418;&#xc9c0; &#xc54a;&#xace0; &#xd65c;&#xb3d9;&#xd558;&#xace0; &#xc788;&#xb2e4;&#xb294; &#xc810; &#xb4f1;&#xc744; &#xbb38;&#xc81c;&#xc81c;&#xae30;&#xd55c; &#xac83;&#xc774;&#xb2e4;.</p><h2>2. &#xac15;&#xb0a8;&#xd328;&#xce58;</h2><p>&apos;&#xac15;&#xb0a8;&#xd328;&#xce58;&apos;&#xb294; &#xc778;&#xc2a4;&#xd0c0;&#xadf8;&#xb7a8; &#xacc4;&#xc815;&#xc73c;&#xb85c;, &#xacc4;&#xc815;&#xc8fc;&#xb294; &apos;<strong>&#xc720;&#xd765; &#xc5c5;&#xc18c;&#xb098; &#xadf8; &#xc8fc;&#xbcc0;&#xc5d0;&#xc11c; &#xd65c;&#xb3d9;&#xd558;&#xb294; &#xc0ac;&#xb78c;&#xb4e4;</strong>&#xc758; &#xc2e0;&#xc0c1; &#xc815;&#xbcf4;&apos;&#xb4f1;&#xc744; &#xc778;&#xc2a4;&#xd0c0;&#xadf8;&#xb7a8;&#xc5d0; &#xc5c5;&#xb85c;&#xb4dc;&#xd588;&#xb2e4;.</p><p><em><span style=\"font-size: 10pt;\" data-mce-style=\"font-size: 10pt;\">*&#xc774; &#xacc4;&#xc815;&#xc5d0; &#xc62c;&#xb77c;&#xc628; &#xc0ac;&#xb78c;&#xb4e4;&#xc5d0; &#xb300;&#xd55c;&#xa0;&#xc815;&#xbcf4;&#xc758; &#xc9c4;&#xc704;&#xc5ec;&#xbd80;&#xb294; &#xd655;&#xc778;&#xb418;&#xc9c0; &#xc54a;&#xc558;&#xb2e4;.&#xa0;</span></em></p><p>2016&#xb144; 8&#xc6d4; 30&#xc77c; &#xc11c;&#xc6b8; &#xac15;&#xb0a8;&#xacbd;&#xcc30;&#xc11c;&#xb294; &apos;&#xac15;&#xb0a8;&#xd328;&#xce58;&apos; &#xc6b4;&#xc601;&#xc790;&#xb97c; &#xbd88;&#xad6c;&#xc18d; &#xc785;&#xac74;&#xd588;&#xb2e4;.</p><p>&#xc774; &#xac15;&#xb0a8;&#xd328;&#xce58;&#xc5d0; &#xc5c5;&#xb85c;&#xb4dc;&#xb41c; &#xc774;&#xbbf8;&#xc9c0;&#xb97c; &#xbb34;&#xb2e8;&#xc73c;&#xb85c; &#xc790;&#xc2e0;&#xc758; &#xbe14;&#xb85c;&#xadf8;&#xb85c; &#xc62e;&#xaca8; &#xd53c;&#xd574;&#xc790;&#xb4e4;&#xc5d0;&#xac8c; &#xae08;&#xd488;&#xc744; &#xc694;&#xad6c;&#xd55c; &#xae40;&#xbaa8;&#xc528;(28&#xc138;)&#xb3c4; &#xd568;&#xaed8; &#xac80;&#xac70; &#xb418;&#xc5c8;&#xb2e4;.&#xa0;<br></p><h2>3. &#xd55c;&#xb0a8;&#xd328;&#xce58;</h2><p>&apos;&#xd55c;&#xb0a8;&#xd328;&#xce58;&apos;&#xb294; &#xc778;&#xc2a4;&#xd0c0;&#xadf8;&#xb7a8;&#xacc4;&#xc815;&#xc73c;&#xb85c;, &#xacc4;&#xc815;&#xc8fc;&#xb294; &apos;<strong>&#xc720;&#xd765;&#xc5c5;&#xc18c; &#xb0a8;&#xc131; &#xc885;&#xc5c5;&#xc6d0; &#xbc0f; &#xc720;&#xd765; &#xc5c5;&#xc18c;&#xc5d0; &#xcd9c;&#xc785;&#xd55c; &#xc774;&#xb4e4;</strong>&#xc758; &#xc2e0;&#xc0c1;&#xc815;&#xbcf4;&apos;&#xb4f1;&#xc744; &#xc778;&#xc2a4;&#xd0c0;&#xadf8;&#xb7a8;&#xc5d0; &#xc5c5;&#xb85c;&#xb4dc;&#xd588;&#xb2e4;.</p><p><em>*&#xc774; &#xacc4;&#xc815;&#xc5d0; &#xc62c;&#xb77c;&#xc628; &#xc0ac;&#xb78c;&#xb4e4;&#xc5d0; &#xb300;&#xd55c;&#xa0;&#xc815;&#xbcf4;&#xc758; &#xc9c4;&#xc704;&#xc5ec;&#xbd80;&#xb294; &#xd655;&#xc778;&#xb418;&#xc9c0; &#xc54a;&#xc558;&#xb2e4;.</em></p><p><br data-mce-bogus=\"1\"></p><p>2016&#xb144; 8&#xc6d4; 30&#xc77c; &#xc11c;&#xc6b8; &#xac15;&#xb0a8; &#xacbd;&#xcc30;&#xc11c;&#xb294; &#xc6b4;&#xc601;&#xc790;&#xb97c; &#xbd88;&#xad6c;&#xc18d; &#xc785;&#xac74;&#xd588;&#xb2e4;. &#xadf8;&#xb7f0;&#xb370; &#xc774; &#xac80;&#xac70; &#xacfc;&#xc815;&#xc758; &#xc601;&#xc0c1;&#xc774; 31&#xc77c; &#xb274;&#xc2a4;&#xb85c; &#xacf5;&#xac1c;&#xb418;&#xc5c8;&#xb2e4;.</p><p>&#xb3c4;&#xbc15;&#xd604;&#xc7a5;, &#xc870;&#xc9c1;&#xac04;&#xc758; &#xd328;&#xc2f8;&#xc6c0;, &#xb300;&#xb7c9; &#xc74c;&#xb780;&#xbb3c; &#xc720;&#xd3ec;&#xbc94;&#xc5d0; &#xb300;&#xd55c; &#xc601;&#xc0c1;&#xb9cc; &#xacf5;&#xac1c; &#xb418;&#xc5c8;&#xb358; &#xac83;&#xacfc;&#xb294; &#xb2e4;&#xb978; &#xc774;&#xb840;&#xc801;&#xc778; &#xbcf4;&#xb3c4;&#xc600;&#xb2e4;.</p><p>&#xac15;&#xb0a8;&#xacbd;&#xcc30;&#xc11c; &#xce21;&#xc740; &#xc774;&#xb7ec;&#xd55c; &#xbcf4;&#xb3c4;&#xc5d0; &#xb300;&#xd574; &quot;&#xac80;&#xac70; &#xc601;&#xc0c1;&#xc744; &#xacf5;&#xac1c;&#xd558;&#xb294; &#xac83;&#xc740; &#xacbd;&#xcc30;&#xc758; &#xd310;&#xb2e8; &#xbb38;&#xc81c;&#xc774;&#xace0; &#xc774;&#xbc88; &#xc0ac;&#xc548;&#xc758; &#xc911;&#xb300;&#xc131;&#xc744; &#xac10;&#xc548;&#xd574; &#xacf5;&#xac1c;&#xb97c; &#xacb0;&#xc815;&#xd588;&#xb2e4;.&quot;&#xace0; &#xbc1d;&#xd614;&#xb2e4;.</p><p>&#xc774;&#xc640; &#xac19;&#xc740; &#xac80;&#xac70;&#xc601;&#xc0c1;&#xc5d0; &#xbc18;&#xb300;&#xd558;&#xb294; &#xc0ac;&#xb78c;&#xb4e4;&#xc740; &quot;&#xd749;&#xc545; &#xbc94;&#xc8c4;&#xc790;&#xb97c; &#xac80;&#xac70;&#xd560; &#xb54c;&#xb3c4; &#xc790;&#xc138;&#xd558;&#xac8c; &#xb098;&#xc624;&#xc9c0; &#xc54a;&#xc558;&#xb358; &#xc9d1;&#xad6c;&#xc870; &#xb4f1;&#xc774; &#xbaa8;&#xc790;&#xc774;&#xd06c; &#xc5c6;&#xc774; &#xacf5;&#xac1c;&#xb418;&#xc5c8;&#xb2e4;.&quot;, &quot;&#xc9d1; &#xad6c;&#xc870;&#xb97c; &#xc544;&#xb294; &#xc0ac;&#xb78c;&#xb4e4;&#xc774;&#xb77c;&#xba74; &#xb204;&#xad70;&#xc9c0; &#xcda9;&#xbd84;&#xd788; &#xcd94;&#xce21;&#xc774; &#xac00;&#xb2a5;&#xd558;&#xb2e4;.&quot;,&quot;&#xd53c;&#xc758;&#xc790;&#xac00; &#xb204;&#xad6c;&#xc778;&#xc9c0; &#xc27d;&#xac8c; &#xc54c; &#xc218; &#xc788;&#xb294; &#xc601;&#xc0c1;&#xc744; &#xc5b8;&#xb860;&#xc5d0; &#xacf5;&#xac1c;&#xd558;&#xb294; &#xac83;&#xc740; &#xbb34;&#xc8c4; &#xcd94;&#xc815;&#xc758; &#xc6d0;&#xce59;&#xc5d0; &#xc5b4;&#xae0b;&#xb09c;&#xb2e4;,&quot;&#xba70; &#xacbd;&#xcc30; &#xce21;&#xc774; &#xad6d;&#xbbfc;&#xc73c;&#xb85c;&#xc11c; &#xb204;&#xb824;&#xc57c;&#xd560; &#xad8c;&#xb9ac;&#xb97c; &#xc5ec;&#xc131;&#xc774;&#xae30;&#xc5d0; &#xb0a8;&#xc131;&#xbcf4;&#xb2e4; &#xd604;&#xc800;&#xd788; &#xb0ae;&#xc740; &#xc218;&#xc900;&#xc73c;&#xb85c; &#xc801;&#xc6a9;&#xd55c; &#xac83;&#xc73c;&#xb85c; &#xbcf4;&#xb294; &#xc0ac;&#xb78c;&#xb4e4;&#xc774; &#xc788;&#xb2e4;.</p><h2>4. &#xba54;&#xac08;&#xd328;&#xce58;<br></h2><p>&apos;&#xba54;&#xac08;&#xd328;&#xce58;&apos;&#xb294; &#xc778;&#xc2a4;&#xd0c0;&#xadf8;&#xb7a8; &#xacc4;&#xc815;&#xc73c;&#xb85c;, &#xacc4;&#xc815;&#xc8fc;&#xb294; &apos;&#xc5ec;&#xc131;&#xd610;&#xc624;&#xc131; &#xac8c;&#xc2dc;&#xae00;, &#xc5ec;&#xc131;&#xc758; &#xc678;&#xbaa8;&#xa0;&#xbe44;&#xd558;, &#xc131;&#xc801; &#xb300;&#xc0c1;&#xd654; &#xbc1c;&#xc5b8; &#xb4f1;&#xc774; &#xb2f4;&#xae34; &#xc0ac;&#xc9c4;&#xacfc; &#xae00;&apos;&#xc744; &#xc778;&#xc2a4;&#xd0c0;&#xadf8;&#xb7a8;&#xc5d0; &#xc5c5;&#xb85c;&#xb4dc; &#xd558;&#xace0;&#xc788;&#xb2e4;.</p><p><em><span style=\"font-size: 10pt;\" data-mce-style=\"font-size: 10pt;\">*&#xc774; &#xacc4;&#xc815;&#xc5d0; &#xc62c;&#xb77c;&#xc628; &#xc0ac;&#xb78c;&#xb4e4;&#xc5d0; &#xb300;&#xd55c;&#xa0;&#xc815;&#xbcf4;&#xc758; &#xc9c4;&#xc704;&#xc5ec;&#xbd80;&#xb294; &#xd655;&#xc778;&#xb418;&#xc9c0; &#xc54a;&#xc558;&#xb2e4;.&#xa0;</span></em></p><h2>5. &#xc6cc;&#xb9c8;&#xb4dc;&#xd328;&#xce58;</h2><p>&apos;&#xc6cc;&#xb9c8;&#xb4dc;&#xd328;&#xce58;&apos;&#xb294; &#xc778;&#xc2a4;&#xd0c0;&#xadf8;&#xb7a8; &#xacc4;&#xc815;&#xc73c;&#xb85c;, &#xacc4;&#xc815;&#xc8fc;&#xb294; &apos;&#xc5ec;&#xc131;&#xd610;&#xc624;&#xc131; &#xac8c;&#xc2dc;&#xae00;, &#xc5ec;&#xc131;&#xc758; &#xc678;&#xbaa8;&#xa0;&#xbe44;&#xd55