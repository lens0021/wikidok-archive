	"postContents": "<div class=\"col-md-12 col-sm-12\">\r\n\t\t\t\t<p><span class=\"mce-preview-object mce-object-iframe\" data-mce-object=\"iframe\" data-mce-p-allowfullscreen=\"allowfullscreen\" data-mce-p-src=\"//www.youtube.com/embed/Fg85ggZSHMw\" contenteditable=\"false\"><iframe src=\"//www.youtube.com/embed/Fg85ggZSHMw\" allowfullscreen=\"allowfullscreen\" data-mce-src=\"//www.youtube.com/embed/Fg85ggZSHMw\" frameborder=\"0\" height=\"314\" width=\"560\"></iframe><span class=\"mce-shim\"></span></span></p><p>&#xacf5;&#xc2dd; &#xc601;&#xad6d; &#xac1c;&#xbd09; &#xd2b8;&#xb808;&#xc77c;&#xb7ec;</p><p>&#xa0;<br></p><h2>1. &#xc18c;&#xac1c;</h2><p>&#xc774;&#xbbf8;&#xd14c;&#xc774;&#xc158; &#xac8c;&#xc784;(The Imitation Game)&#xc740; 2014&#xb144;&#xc5d0; &#xac1c;&#xbd09;&#xd55c; &#xc5ed;&#xc0ac; &#xb4dc;&#xb77c;&#xb9c8; &#xc2a4;&#xb9b4;&#xb7ec; &#xc601;&#xd654;&#xc774;&#xb2e4;(&#xd55c;&#xad6d; &#xac1c;&#xbd09;: 2015&#xb144; 2&#xc6d4; 17&#xc77c;). &#xac10;&#xb3c5;&#xc740; &#xbaa8;&#xb974;&#xd150; &#xd288;&#xb460;&#xc774;&#xb2e4;. &#xc564;&#xb4dc;&#xb8e8; &#xd638;&#xc9c0;&#xc2a4;&#xc758; &#xcc45; &lt;&#xc568;&#xb7f0; &#xd29c;&#xb9c1;&#xc758; &#xc774;&#xbbf8;&#xd14c;&#xc774;&#xc158; &#xac8c;&#xc784;&gt;&#xc744; &#xbc14;&#xd0d5;&#xc73c;&#xb85c; &#xc81c;&#xc791;&#xb418;&#xc5c8;&#xb2e4;. &#xbca0;&#xb124;&#xb515;&#xd2b8; &#xcef4;&#xbc84;&#xbc30;&#xce58;&#xac00; &#xc568;&#xb7f0; &#xd29c;&#xb9c1;, &#xd0a4;&#xc774;&#xb77c; &#xb098;&#xc774;&#xd2c0;&#xb9ac;&#xac00; &#xc870;&#xc548; &#xd074;&#xb77c;&#xd06c;&#xb97c; &#xc5f0;&#xae30;&#xd588;&#xb2e4;. &#xc81c;2&#xcc28; &#xc138;&#xacc4;&#xb300;&#xc804; &#xb54c; &#xb3c5;&#xc77c;&#xad70;&#xc758; &#xc5d0;&#xb2c8;&#xadf8;&#xb9c8; &#xc554;&#xd638;&#xb97c; &#xd574;&#xb3c5;&#xd558;&#xc5ec; &#xc5f0;&#xd569;&#xad70;&#xc758; &#xc2b9;&#xb9ac;&#xc5d0; &#xacf5;&#xd5cc;&#xd588;&#xb358; &#xc601;&#xad6d;&#xc758; &#xcc9c;&#xc7ac; &#xc218;&#xd559;&#xc790; &#xc568;&#xb7f0; &#xd29c;&#xb9c1;&#xc758; &#xc0b6;&#xacfc;, &#xad70;&#xc0ac;&#xae30;&#xbc00;&#xb85c; &#xbb36;&#xc5ec; &#xc0ac;&#xd6c4; &#xc218;&#xc2ed; &#xb144; &#xb3d9;&#xc548; &#xc54c;&#xb824;&#xc9c0;&#xc9c0; &#xc54a;&#xc558;&#xb358; &#xadf8;&#xc758; &#xc5c5;&#xc801;&#xc744; &#xb2e4;&#xb8ec; &#xc601;&#xd654;&#xb2e4;. <br></p><p>&#xa0;<br></p><h2>2. &#xb0b4;&#xc6a9;</h2><div contenteditable=\"false\"><div class=\"alert alert-warning\" contenteditable=\"true\"><p>&#xc2a4;&#xd3ec;&#xc77c;&#xb7ec; &#xc8fc;&#xc758;! &#xc601;&#xd654; &#xc804;&#xccb4; &#xc904;&#xac70;&#xb9ac; &#xb0b4;&#xc6a9;&#xc774; &#xc788;&#xc2b5;&#xb2c8;&#xb2e4;.</p><p>&#xc601;&#xd654; &#xb0b4;&#xc6a9;&#xc740; &#xc5ed;&#xc0ac;&#xc801; &#xc0ac;&#xc2e4;&#xacfc; &#xcc28;&#xc774;&#xac00; &#xc788;&#xc2b5;&#xb2c8;&#xb2e4;(3. &#xc5ec;&#xb2f4; &#xcc38;&#xc870;)<br></p></div></div><p>&#xc601;&#xd654; &#xc774;&#xbbf8;&#xd14c;&#xc774;&#xc158; &#xac8c;&#xc784;&#xc5d0;&#xb294; &#xc138; &#xac00;&#xc9c0; &#xc2dc;&#xae30;&#xac00; &#xb4f1;&#xc7a5;&#xd55c;&#xb2e4;. 1928&#xb144; &#xd29c;&#xb9c1;&#xc758; &#xd559;&#xcc3d;&#xc2dc;&#xc808;, 1939-45&#xb144; 2&#xcc28; &#xb300;&#xc804; &#xb2f9;&#xc2dc; &#xbe14;&#xb808;&#xce28;&#xb9ac; &#xd30c;&#xd06c;&#xc5d0;&#xc11c; &#xadfc;&#xbb34;&#xd558;&#xb358; &#xc2dc;&#xc808;, &#xadf8;&#xb9ac;&#xace0; 1951&#xb144; &#xb9e8;&#xccb4;&#xc2a4;&#xd130;&#xc5d0;&#xc11c; &#xb3d9;&#xc131;&#xc560;(gross indecency) &#xd610;&#xc758;&#xb85c; &#xccb4;&#xd3ec;&#xb418;&#xb294; &#xc2dc;&#xae30;&#xb97c; &#xbc88;&#xac08;&#xc544;&#xac00;&#xba70; &#xbcf4;&#xc5ec;&#xc900;&#xb2e4;. &#xd559;&#xcc3d;&#xc2dc;&#xc808;&#xacfc; 1951&#xb144; &#xc598;&#xae30;&#xb294; &#xc7a0;&#xae50;&#xc529; &#xb4f1;&#xc7a5;&#xd558;&#xace0;, &#xba54;&#xc778;&#xc740; &#xbe14;&#xb808;&#xce28;&#xb9ac; &#xd30c;&#xd06c;&#xc5d0;&#xc11c; &#xc5d0;&#xb2c8;&#xadf8;&#xb9c8; &#xc554;&#xd638;&#xb97c; &#xae68;&#xb294; &#xacfc;&#xc815; &#xc774;&#xc57c;&#xae30;&#xc774;&#xb2e4;.<br><br>&#xc601;&#xd654; &#xb3c4;&#xc785;&#xbd80;&#xb294; 1951&#xb144;. &#xd29c;&#xb9c1;&#xc758; &#xc9d1;&#xc5d0; &#xb3c4;&#xb451;&#xc774; &#xb4e0; &#xac83;&#xc774; &#xacbd;&#xcc30;&#xc5d0; &#xc2e0;&#xace0;&#xb418;&#xba74;&#xc11c;, &#xb179; &#xd615;&#xc0ac;&#xac00; &#xcc3e;&#xc544;&#xc628;&#xb2e4;. &#xc880; &#xbb34;&#xb840;&#xd55c; &#xd0dc;&#xb3c4;&#xb85c; &#xb3c4;&#xc6c0; &#xd544;&#xc694; &#xc5c6;&#xb2e4;&#xba70; &#xc218;&#xc0ac;&#xb97c; &#xac70;&#xbd80;&#xd558;&#xb294; &#xd29c;&#xb9c1;&#xc758; &#xd0dc;&#xb3c4;&#xc5d0; &#xbb54;&#xac00; &#xc228;&#xae30;&#xb294; &#xac8c; &#xc788;&#xb294; &#xac74; &#xc544;&#xb2cc;&#xc9c0; &#xc758;&#xc2ec;&#xc774; &#xb4e0; &#xb179; &#xd615;&#xc0ac;. &#xd29c;&#xb9c1;&#xc758; &#xc778;&#xc801;&#xc0ac;&#xd56d;&#xc744; &#xc870;&#xc0ac;&#xd558;&#xb2e4; 2&#xcc28; &#xb300;&#xc804; &#xb2f9;&#xc2dc; &#xadf8;&#xc758; &#xad70; &#xbcf5;&#xbb34; &#xae30;&#xb85d;&#xc774; &#xae30;&#xbc00;&#xc778;&#xb370;&#xb2e4; &#xd30c;&#xc77c;&#xc774; &#xbe44;&#xc5b4;&#xc788;&#xb294; &#xac83;&#xc744; &#xd655;&#xc778;&#xd558;&#xace0; &#xd29c;&#xb9c1;&#xc774; &#xc18c;&#xb828; &#xc2a4;&#xd30c;&#xc774;&#xac00; &#xc544;&#xb2cc;&#xc9c0; &#xc758;&#xc2ec;&#xd558;&#xac8c; &#xb41c;&#xb2e4;. <br><br>&#xd29c;&#xb9c1;&#xc744; &#xbbf8;&#xd589;&#xd558;&#xb358; &#xacbd;&#xcc30;&#xc740; &#xadf8;&#xac00; &#xc5b4;&#xb5a4; &#xb0a8;&#xc790;&#xc5d0;&#xac8c; &#xbd09;&#xd22c;&#xb97c; &#xac74;&#xb124;&#xb294; &#xac83;&#xc744; &#xbaa9;&#xaca9;, &#xadf8; &#xc0c1;&#xb300; &#xb0a8;&#xc790;&#xb97c; &#xcde8;&#xc870;&#xd55c; &#xacb0;&#xacfc; &#xd29c;&#xb9c1;&#xc774; &#xb3d9;&#xc131;&#xc560;&#xc790;&#xb77c;&#xb294; &#xc0ac;&#xc2e4;&#xc744; &#xbc1c;&#xac01;&#xd55c;&#xb2e4;(&#xb2f9;&#xc2dc; &#xc601;&#xad6d;&#xc5d0;&#xc11c; &#xbd88;&#xbc95;&#xc774;&#xc5c8;&#xb2e4;). &#xb3c8;&#xc744; &#xbc1b;&#xace0; &#xd29c;&#xb9c1; &#xc9d1;&#xc5d0;&#xc11c; &#xc131;&#xb9e4;&#xb9e4;&#xd588;&#xb358; &#xb0a8;&#xc790;&#xac00; &#xb098;&#xc911;&#xc5d0; &#xb2e4;&#xc2dc; &#xc640;&#xc11c; &#xc9d1;&#xc744; &#xd138;&#xc5c8;&#xb358; &#xac83;. &#xd29c;&#xb9c1;&#xc774; &#xc18c;&#xb828; &#xc2a4;&#xd30c;&#xc774;&#xc77c;&#xac70;&#xb77c;&#xb294; &#xd655;&#xc2e0;&#xc5d0; &#xc0ac;&#xb85c;&#xc7a1;&#xd78c; &#xb179; &#xd615;&#xc0ac;&#xb294; &#xc774; &#xc218;&#xc0ac;&#xacb0;&#xacfc;&#xc5d0; &#xb9cc;&#xc871;&#xd558;&#xc9c0; &#xc54a;&#xace0;, &#xd29c;&#xb9c1;&#xc744; &#xc9c1;&#xc811; &#xcde8;&#xc870;&#xd560; &#xc218; &#xc788;&#xac8c; &#xd574;&#xb2ec;&#xb77c;&#xace0; &#xc0c1;&#xad00;&#xc5d0;&#xac8c; &#xbd80;&#xd0c1;&#xd55c;&#xb2e4;. &#xd29c;&#xb9c1;&#xacfc; &#xb179; &#xd615;&#xc0ac;&#xac00; &#xb9c8;&#xc8fc; &#xc549;&#xc740; &#xcde8;&#xc870;&#xc2e4;&#xc5d0;&#xc11c; &#xd29c;&#xb9c1;&#xc758; &#xacfc;&#xac70; &#xc774;&#xc57c;&#xae30;&#xac00; &#xc2dc;&#xc791;&#xb41c;&#xb2e4;.</p><p><br>1939&#xb144; 2&#xcc28; &#xb300;&#xc804; &#xbc1c;&#xbc1c; &#xc9c1;&#xd6c4;, &#xd29c;&#xb9c1;&#xc740; &#xbe14;&#xb808;&#xce28;&#xb9ac; &#xd30c;&#xd06c;&#xb77c;&#xb294; &#xbe44;&#xbc00; &#xad70;&#xc0ac;&#xc2dc;&#xc124;(&#xb300;&#xc678;&#xc801;&#xc73c;&#xb85c;&#xb294; &apos;&#xb77c;&#xb514;&#xc624; &#xacf5;&#xc7a5;&apos;)&#xc758; &#xc554;&#xd638; &#xd574;&#xb3c5;&#xd300;&#xc5d0; &#xc9c0;&#xc6d0;&#xd55c;&#xb2e4;. &#xc778;&#xd130;&#xbdf0;&#xb97c; &#xb9e1;&#xc740; &#xc54c;&#xb9ac;&#xc2a4;&#xd130; &#xb370;&#xb2c8;&#xc2a4;&#xd134; &#xd574;&#xad70; &#xc0ac;&#xb839;&#xad00;&#xc740; &#xd29c;&#xb9c1;&#xc744; &#xd0d0;&#xd0c1;&#xc796;&#xac8c; &#xc0dd;&#xac01;&#xd558;&#xace0; &#xb3c5;&#xc77c;&#xc5b4;&#xb3c4; &#xbabb;&#xd558;&#xb294;&#xb370; &#xd544;&#xc694;&#xc5c6;&#xb2e4;&#xba70; &#xd0c8;&#xb77d;&#xc2dc;&#xd0a4;&#xb824;&#xace0; &#xd558;&#xc9c0;&#xb9cc;, &#xd29c;&#xb9c1;&#xc774; &#xb2f9;&#xc2dc; &#xc77c;&#xae09;&#xbe44;&#xbc00;&#xc774;&#xc5c8;&#xb358; &#xc5d0;&#xb2c8;&#xadf8;&#xb9c8;&#xb97c; &#xc5b8;&#xae09;&#xd558;&#xba70; &#xb098; &#xac19;&#xc740; &#xc218;&#xd559;&#xc790;&#xac00; &#xd544;&#xc694;&#xd558;&#xb2e4;&#xace0; &#xc124;&#xb4dd;&#xd574;&#xc11c; &#xd300;&#xc5d0; &#xd3ec;&#xd568;&#xc2dc;&#xd0a8;&#xb2e4;. &#xcd5c;&#xc885; &#xba64;&#xbc84;&#xb294; &#xd734;, &#xc874;, &#xd53c;&#xd130;, &#xadf8;&#xb9ac;&#xace0; &#xace7; &#xc601;&#xd654;&#xc5d0;&#xc11c; &#xd1f4;&#xc7a5;&#xd558;&#xac8c; &#xb418;&#xb294; &#xb450; &#xba85;&#xacfc; &#xd29c;&#xb9c1;&#xae4c;&#xc9c0; &#xcd1d; 6&#xba85;. &#xccab; &#xbbf8;&#xd305;&#xc5d0;&#xc11c; MI6 &#xad6d;&#xc7a5; &#xc2a4;&#xd29c;&#xc5b4;&#xd2b8; &#xba58;&#xc9c0;&#xc2a4;&#xac00; &#xc18c;&#xac1c;&#xb41c;&#xb2e4;.</p><p>&#xa0;<br></p><p>&#xb2e4;&#xb978; &#xd300;&#xc6d0;&#xb4e4;&#xacfc; &#xd611;&#xc870;&#xd558;&#xc9c0; &#xc54a;&#xace0; &#xc0ac;&#xbb34;&#xc2e4; &#xd55c;&#xad6c;&#xc11d;&#xc5d0;&#xc11c; &#xd63c;&#xc790; &#xc554;&#xd638;&#xd574;&#xb3c5;&#xae30;&#xb97c; &#xc124;&#xacc4;&#xd558;&#xb294; &#xd29c;&#xb9c1;. &#xc5d0;&#xb2c8;&#xadf8;&#xb9c8;&#xb77c;&#xb294; &#xae30;&#xacc4;&#xb97c; &#xc0c1;&#xb300;&#xd558;&#xb824;&#xba74; &#xc0c1;&#xc751;&#xd558;&#xb294; &#xae30;&#xacc4;&#xb97c; &#xb9cc;&#xb4e4;&#xc5b4;&#xc57c; &#xd55c;&#xb2e4;&#xace0; &#xbbff;&#xb294;&#xb2e4;. &#xd558;&#xc9c0;&#xb9cc; &#xb370;&#xb2c8;&#xc2a4;&#xd134; &#xc0ac;&#xb839;&#xad00;&#xc740; &#xd29c;&#xb9c1;&#xc774; &#xc124;&#xacc4;&#xd55c; &#xae30;&#xacc4;&#xb97c; &#xb9cc;&#xb4e4; &#xc608;&#xc0b0;&#xc744; &#xc2b9;&#xc778;&#xd574;&#xc8fc;&#xc9c0; &#xc54a;&#xace0;, &#xd29c;&#xb9c1;&#xc740; &#xc0ac;&#xb839;&#xad00;&#xc758; &#xc9c1;&#xc18d;&#xc0c1;&#xad00;&#xc778; &#xc708;&#xc2a4;&#xd134; &#xcc98;&#xce60; &#xc218;&#xc0c1;&#xc5d0;&#xac8c; &#xc9c1;&#xc811; &#xd3b8;&#xc9c0;&#xb97c; &#xc368;&#xc11c; &#xd5c8;&#xac00;&#xb97c; &#xbc1b;&#xc544;&#xb0b4; &#xbc84;&#xb9ac;&#xace0; &#xd300; &#xb9ac;&#xb354;&#xb85c; &#xb4f1;&#xadf9;. &#xb370;&#xb2c8;&#xc2a4;&#xd134; &#xc0ac;&#xb839;&#xad00;&#xc740; &#xbd88;&#xb9cc;&#xc774; &#xac00;&#xb4dd;&#xd558;&#xc9c0;&#xb9cc;, &#xba58;&#xc9c0;&#xc2a4;&#xb294; &#xd29c;&#xb9c1;&#xc5d0;&#xac8c; &#xd798;&#xc744; &#xc2e4;&#xc5b4;&#xc900;&#xb2e4;. &#xadf8; &#xc790;&#xb9ac;&#xc5d0;&#xc11c; &#xd300;&#xc6d0; &#xc911; 2&#xba85;&#xc744; &#xb3c4;&#xc6c0; &#xc548;&#xb41c;&#xb2e4;&#xba70; &#xd574;&#xace0;&#xd574; &#xbc84;&#xb824;&#xc11c; &#xb0a8;&#xc740; &#xd300;&#xc6d0;&#xb4e4;&#xc758; &#xbc18;&#xac10;&#xc744; &#xc0ac;&#xb294; &#xd29c;&#xb9c1;.</p><p>&#xa0;<br></p><p>&#xd300; &#xc778;&#xc6d0;&#xbcf4;&#xac15;&#xc744; &#xc704;&#xd574; &#xc2e0;&#xbb38;&#xc5d0; &#xace0;&#xb09c;&#xb3c4; &#xc2ed;&#xc790;&#xb9d0;&#xd480;&#xc774;&#xb97c; &#xc2e3;&#xace0;, &#xadf8;&#xac78; &#xd480;&#xc5b4;&#xc11c; &#xbcf4;&#xb0b8; &#xc0ac;&#xb78c;&#xb4e4;&#xc744; &#xbaa8;&#xc544; &#xc218;&#xd559;&#xbb38;&#xc81c;&#xb97c; &#xb0b4;&#xace0; 6&#xbd84; &#xc548;&#xc5d0; &#xd480;&#xb77c;&#xace0; &#xd55c;&#xb2e4;. &#xac00;&#xb2a5;&#xd55c; &#xc598;&#xae30;&#xb0d0;&#xb294; &#xc9c8;&#xbb38;&#xc5d0; &#xd29c;&#xb9c1; &#xbcf8;&#xc778;&#xb3c4; &#xd478;&#xb294; &#xb370; 8&#xbd84; &#xac78;&#xb9b0; &#xbb38;&#xc81c;&#xb77c;&#xba70;&#xa0; &#xbd88;&#xac00;&#xb2a5;&#xd560;&#xac70;&#xb77c;&#xace0; &#xb2f5;&#xd558;&#xb294;&#xb370;, &#xc870;&#xc548; &#xd074;&#xb77c;&#xd06c;&#xac00; 6&#xbd84; &#xc774;&#xb0b4;&#xc5d0; &#xc77c;&#xcc29;&#xc73c;&#xb85c; &#xd480;&#xc5b4;&#xbc84;&#xb9b0;&#xb2e4;. &#xadf8;&#xb140;&#xc758; &#xb2a5;&#xb825;&#xc744; &#xb192;&#xac8c; &#xc0b0; &#xd29c;&#xb9c1;&#xc740; &#xc870;&#xc548;&#xc744; &#xd300;&#xc6d0;&#xc73c;&#xb85c; &#xc120;&#xbc1c;&#xd558;&#xace0;, &#xc80a;&#xc740; &#xc5ec;&#xc790;&#xac00; &#xc9d1;&#xc744; &#xb5a0;&#xb098; &#xc77c;&#xd558;&#xb294; &#xc0c1;&#xd669;&#xc774; &#xb9c8;&#xb729;&#xc796;&#xc740; &#xc870;&#xc548; &#xbd80;&#xbaa8;&#xb2d8;&#xc744; &#xc124;&#xb4dd;&#xd558;&#xb294;&#xb370; &#xb3c4;&#xc6c0;&#xc744; &#xc900;&#xb2e4;.<br></p><p>&#xa0;</p><p>&#xc870;&#xc548;&#xacfc; &#xd568;&#xaed8; &#xd14c;&#xc2a4;&#xd2b8;&#xb97c; &#xd1b5;&#xacfc;&#xd55c; &#xc7ad;&#xc740; &#xbc14;&#xb85c; &#xd300;&#xc5d0; &#xd569;&#xb958;&#xd558;&#xc9c0;&#xb9cc;, &#xc870;&#xc548;&#xc740; &#xc5ec;&#xc131;&#xb4e4;&#xacfc; &#xd568;&#xaed8; &#xc0ac;&#xbb34;&#xc9c1;&#xc73c;&#xb85c; &#xc77c;&#xd558;&#xac8c; &#xb41c;&#xb2e4;. &#xd29c;&#xb9c1;&#xc740; &#xb0ae;&#xc5d0; &#xc791;&#xc5c5;&#xd55c; &#xb0b4;&#xc6a9;&#xc744; &#xbc00;&#xbc18;&#xcd9c;&#xd574;&#xc11c; &#xc870;&#xc548;&#xc758; &#xc219;&#xc18c;&#xc5d0; &#xac00;&#xc838;&#xac00; &#xbcf4;&#xc5ec;&#xc8fc;&#xb294; &#xbc29;&#xc2dd;&#xc73c;&#xb85c; &#xadf8;&#xb140;&#xc758; &#xb450;&#xb1cc;&#xb97c; &#xbe4c;&#xb9b0;&#xb2e4;. &#xb610;&#xd55c; &#xc870;&#xc548;&#xc73c;&#xb85c;&#xbd80;&#xd130; &#xae30;&#xacc4;&#xb97c; &#xc644;&#xc131;&#xd558;&#xae30; &#xc704;&#xd574;&#xc11c;&#xb294; &#xd300;&#xc6d0;&#xb4e4;&#xc758; &#xb3c4;&#xc6c0;&#xc774; &#xd544;&#xc694;&#xd558;&#xb2e4;&#xace0; &#xc124;&#xb4dd;&#xbc1b;&#xace0; &#xd300;&#xc6d0;&#xb4e4;&#xacfc;&#xc758; &#xad00;&#xacc4; &#xd68c;&#xbcf5;&#xc744; &#xc704;&#xd574; &#xb178;&#xb825;&#xd55c;&#xb2e4;. &#xd300;&#xc6d0;&#xb4e4;&#xb3c4; &#xb9c8;&#xc74c;&#xc744; &#xb3cc;&#xb824; &#xae30;&#xacc4;&#xc81c;&#xc791;&#xc5d0; &#xd611;&#xc870;&#xd558;&#xac8c; &#xb41c;&#xb2e4;.</p><p>&#xa0;<br></p><p>&#xc9c4;&#xd589; &#xc911;&#xac04;&#xc911;&#xac04;&#xc5d0; &#xd29c;&#xb9c1;&#xc758; &#xd559;&#xcc3d;&#xc2dc;&#xc808; &#xc598;&#xae30;&#xac00; &#xc7a0;&#xae50;&#xc529; &#xb098;&#xc628;&#xb2e4;. &#xd559;&#xad50; &#xc2dd;&#xc0ac; &#xc2dc;&#xac04;&#xc5d0; &#xc5b4;&#xb9b0; &#xd29c;&#xb9c1;&#xc774; &#xc644;&#xb450;&#xcf69;&#xacfc; &#xb2f9;&#xadfc;&#xc744; &#xc11c;&#xb85c; &#xb2ff;&#xc9c0; &#xc54a;&#xac8c; &#xbd84;&#xb9ac;&#xd558;&#xb294;&#xb370; &#xc9d1;&#xcc29;&#xd558;&#xb294; &#xbaa8;&#xc2b5;, &#xb2e4;&#xb978; &#xd559;&#xc0dd;&#xb4e4;&#xc774; &#xadf8;&#xb97c; &#xad50;&#xc2e4; &#xb9c8;&#xb8fb;&#xbc14;&#xb2e5; &#xbc11; &#xacf5;&#xac04;&#xc5d0; &#xac00;&#xb450;&#xace0; &#xbabb;&#xc9c8;&#xd574;&#xbc84;&#xb9ac;&#xba70; &#xad34;&#xb86d;&#xd788;&#xb294; &#xc7a5;&#xba74;, &#xadf8;&#xb54c; &#xadf8;&#xb97c; &#xaebc;&#xb0b4;&#xc900; &#xd06c;&#xb9ac;&#xc2a4;&#xd1a0;&#xd37c;. &#xc720;&#xc77c;&#xd55c; &#xce5c;&#xad6c;&#xc600;&#xb358; &#xd06c;&#xb9ac;&#xc2a4;&#xd1a0;&#xd37c;&#xac00; &#xbe4c;&#xb824;&#xc900; &#xcc45;&#xc73c;&#xb85c; &#xc554;&#xd638;&#xd559;&#xc5d0; &#xad00;&#xc2ec;&#xc744; &#xac16;&#xac8c; &#xb41c; &#xac83;, &#xd06c;&#xb9ac;&#xc2a4;&#xd1a0;&#xd37c;&#xc640; &#xc554;&#xd638; &#xcabd;&#xc9c0;&#xb97c; &#xc8fc;&#xace0; &#xbc1b;&#xc73c;&#xba70; &#xb354;&#xc6b1; &#xac00;&#xae4c;&#xc6c