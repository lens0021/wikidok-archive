2c8;&#xadf8;&#xb9c8;&#xb97c; &#xae6c; &#xae30;&#xacc4; &#xc774;&#xb984;&#xc740; Bombe(&#xb098;&#xc911;&#xc5d0; Victory&#xb85c; &#xbc1c;&#xc804;). Colossus&#xb294; &#xd29c;&#xb9c1;&#xc758; &#xc544;&#xc774;&#xb514;&#xc5b4;&#xb97c; &#xcc44;&#xc6a9;&#xd574; &#xbe14;&#xb808;&#xce28;&#xb9ac; &#xd30c;&#xd06c; &#xc5d4;&#xc9c0;&#xb2c8;&#xc5b4; Tommy Flowers&#xac00; &#xb9cc;&#xb4e0; &#xcd5c;&#xcd08;&#xc758; &#xd504;&#xb85c;&#xadf8;&#xb7a8; &#xac00;&#xb2a5;&#xd55c; &#xb514;&#xc9c0;&#xd138; &#xc77c;&#xb809;&#xd2b8;&#xb85c;&#xb2c9; &#xcef4;&#xd4e8;&#xd130;&#xc758; &#xc774;&#xb984;. <br></li><li>&#xc554;&#xd638;&#xd574;&#xb3c5;&#xc744; &#xc704;&#xd574; &#xc120;&#xbc1c;&#xb41c; 6&#xba85;&#xc774; &#xcc98;&#xc74c; &#xb9cc;&#xb098;&#xb294; &#xc790;&#xb9ac;&#xc5d0;&#xc11c; &#xd0c1;&#xc790; &#xc704;&#xc5d0; &#xc774;&#xb2c8;&#xadf8;&#xb9c8; &#xae30;&#xacc4;&#xac00; &#xc788;&#xace0; &#xd3f4;&#xb780;&#xb4dc;&#xad70;&#xc774; &#xbab0;&#xb798; &#xbe7c; &#xc628; &#xac70;&#xb77c;&#xace0; &#xc5b8;&#xae09;&#xd558;&#xb294;&#xb370;, &#xc2e4;&#xc81c; &#xc601;&#xad6d;&#xad70;&#xc740; &#xc774; &#xc2dc;&#xc810;&#xc5d0;&#xc11c; &#xc5d0;&#xb2c8;&#xadf8;&#xb9c8; &#xae30;&#xacc4;&#xb97c; &#xbcf4;&#xc720;&#xd558;&#xace0; &#xc788;&#xc9c0; &#xc54a;&#xc558;&#xb2e4;. &#xbcf5;&#xc81c;&#xd488;&#xc774;&#xc5c8;&#xc73c;&#xba74; &#xbab0;&#xb77c;&#xb3c4;.<br></li><li>&#xc870;&#xc548;&#xc740; &#xd29c;&#xb9c1;&#xc774; &#xc544;&#xb2c8;&#xb77c; Gordon Welchman&#xc5d0; &#xc758;&#xd574; GC&amp;CS(Government Code and Cypher School)&#xc5d0; &#xb9ac;&#xcfe0;&#xb974;&#xd2b8; &#xb418;&#xc5c8;&#xb2e4;. &#xadf8;&#xb294; Hut 6&#xc5d0;&#xc11c; &#xb3c5;&#xc77c; &#xc721;&#xad70; &#xc5d0;&#xb2c8;&#xadf8;&#xb9c8; &#xc554;&#xd638; &#xd574;&#xb3c5;&#xd300;&#xc744; &#xc774;&#xb04c;&#xc5c8;&#xb358; &#xc778;&#xbb3c;(&#xd29c;&#xb9c1;&#xc740; Hut 8 &#xd574;&#xad70;&#xcabd;).</li><li>&#xc870;&#xc548;&#xc740; &#xc0ac;&#xbb34;&#xc9c1;&#xc774; &#xc544;&#xb2c8;&#xb77c;, &#xd29c;&#xb9c1;&#xacfc; &#xd568;&#xaed8; Hut 8&#xc5d0;&#xc11c; &#xc554;&#xd638;&#xd574;&#xb3c5;&#xc790;&#xb85c; &#xadfc;&#xbb34;&#xd588;&#xb2e4;. 1944&#xb144;&#xc5d0; Hut 8&#xc758; &#xbd80;&#xc218;&#xc11d;(deputy head)&#xc774; &#xb418;&#xc5c8;&#xb2e4;.</li><li>&#xc870;&#xc548;&#xc740; 1954&#xb144; &#xd29c;&#xb9c1;&#xc774; &#xc0ac;&#xb9dd;&#xd560; &#xb54c;&#xae4c;&#xc9c0; &#xac00;&#xae4c;&#xc6b4; &#xce5c;&#xad6c;&#xb85c; &#xc9c0;&#xb0c8;&#xb2e4;. &#xcde8;&#xbbf8;&#xc640; &#xc131;&#xaca9;&#xc774; &#xbe44;&#xc2b7;&#xd588;&#xb2e4;&#xace0;.</li><li>&#xc2a4;&#xd30c;&#xc774; &#xc874;&#xc740; &#xd29c;&#xb9c1;&#xc758; &#xd300;&#xc6d0;&#xc774; &#xc544;&#xb2c8;&#xc5c8;&#xb2e4;. &#xbe14;&#xb808;&#xce28;&#xb9ac;&#xc758; &#xc804;&#xd600; &#xb2e4;&#xb978; &#xc139;&#xc158; &#xc18c;&#xc18d;. &#xbcf4;&#xc548; &#xb54c;&#xbb38;&#xc5d0; &#xb2e4;&#xb978; &#xc139;&#xc158; &#xc18c;&#xc18d;&#xb07c;&#xb9ac; &#xac19;&#xc774; &#xc77c;&#xd558;&#xae30;&#xb294; &#xcee4;&#xb155; &#xb9cc;&#xb0ac;&#xc744; &#xac00;&#xb2a5;&#xc131;&#xb3c4; &#xac70;&#xc758; &#xc5c6;&#xb2e4;&#xace0; &#xd55c;&#xb2e4;.</li><li>&#xc2a4;&#xd30c;&#xc774;&#xb97c; &#xbb35;&#xc778;&#xd558;&#xb294;&#xac74; &#xc804;&#xc2dc;&#xc0c1;&#xd669;&#xc5d0;&#xc11c; &#xc2ec;&#xac01;&#xd55c; &#xbc18;&#xc5ed;&#xd589;&#xc704;&#xb85c;, &#xc874;&#xc774; &#xc18c;&#xb828; &#xc2a4;&#xd30c;&#xc774;&#xc778; &#xac83;&#xc744; &#xc54c;&#xace0;&#xb3c4; &#xbb35;&#xc778;&#xd588;&#xb2e4;&#xb294; &#xc601;&#xd654; &#xb0b4; &#xc124;&#xc815;&#xc740; &#xc880; &#xb108;&#xbb34; &#xb098;&#xac14;&#xb2e4;&#xb294; &#xc9c0;&#xc801;&#xb3c4; &#xc788;&#xb2e4;.</li><li>&#xd29c;&#xb9c1;&#xc758; &#xbd80;&#xac80; &#xb9ac;&#xd3ec;&#xd2b8;&#xc5d0; &#xc758;&#xd558;&#xba74;, 4&#xc628;&#xc2a4; &#xac00;&#xb7c9;&#xc758; &#xc544;&#xbaac;&#xb4dc;&#xd5a5; &#xc561;&#xccb4;(&#xccad;&#xc0b0;&#xac00;&#xb9ac;)&#xac00; &#xc704;&#xc5d0; &#xb0a8;&#xc544; &#xc788;&#xc5c8;&#xb2e4;&#xace0; &#xd55c;&#xb2e4;. &#xc640;&#xc778;&#xc794;&#xc744; &#xc801;&#xb2f9;&#xd788; &#xcc44;&#xc6b8;&#xb9cc;&#xd55c; &#xc591;&#xc758; &#xb3c5;&#xc744; &#xb9c8;&#xc2e0; &#xac83;&#xc73c;&#xb85c; 