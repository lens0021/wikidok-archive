4;. &#xd654;&#xb048;&#xd55c; &#xc131;&#xaca9;&#xc73c;&#xb85c; &#xd06c;&#xb9ac;&#xc2a4;&#xd0c8; &#xc82c;&#xc758; &#xacfc;&#xaca9;&#xd30c;&#xa0;&#xb178;&#xc120;&#xc744; &#xac78;&#xc73c;&#xba70;&#xa0;&#xb85c;&#xc988;&#xc640;&#xb294; &#xc758;&#xacac;&#xc774; &#xcda9;&#xb3cc;&#xb410;&#xb358; &#xac83;&#xc73c;&#xb85c; &#xbcf4;&#xc778;&#xb2e4;.</p><p>&#xd384;&#xc774; &#xb208;&#xbb3c;&#xc744; &#xd758;&#xb9b4; &#xb54c; &#xc27d;&#xac8c; &#xb3d9;&#xd654;&#xb418;&#xc5b4; &#xac19;&#xc774; &#xb208;&#xbb3c;&#xc744; &#xd758;&#xb9ac;&#xae30;&#xb3c4; &#xd55c;&#xb2e4;. &#xd648;&#xc6d4;&#xb4dc;&#xc758; &#xd558;&#xae09; &#xacc4;&#xae09;&#xc73c;&#xb85c; &#xc5d8;&#xb9ac;&#xd2b8; &#xacc4;&#xae09;&#xacfc; &#xd648;&#xc6d4;&#xb4dc; &#xccb4;&#xc81c;&#xc5d0; &#xac15;&#xd55c; &#xbd88;&#xb9cc;&#xacfc; &#xc5fc;&#xc99d;&#xc744; &#xb4dc;&#xb7ec;&#xb0b4;&#xae30;&#xb3c4; &#xd588;&#xb2e4;. &#xc790;&#xc2e0;&#xc758; &#xac00;&#xb2a5;&#xc131;&#xc744; &#xae0d;&#xc815;&#xd558;&#xba70;, &#xb0a8;&#xb4e4;&#xc758; &#xac00;&#xb2a5;&#xc131; &#xb610;&#xd55c; &#xbbff;&#xc5b4;&#xc8fc;&#xace0; &#xc774;&#xb04c;&#xc5b4;&#xb0b4; &#xc900;&#xb2e4;.</p><h2>&#xc791;&#xc911; &#xd589;&#xbcf4;</h2><p>&#xa0;<br></p><p>&#xc6d0;&#xb798;&#xb294; &#xd648;&#xc6d4;&#xb4dc;&#xc758; &#xb300;&#xc7a5;&#xc7a5;&#xc774; &#xacc4;&#xae09;&#xc73c;&#xb85c; &#xc0c1;&#xc704; &#xacc4;&#xae09;&#xb4e4;&#xc774; &#xc0ac;&#xc6a9;&#xd558;&#xb294; &#xac74;&#xcd95;&#xbb3c;&#xc744; &#xc704;&#xd574; &#xc0dd;&#xc0b0;&#xb41c; &#xc82c;&#xb4e4; &#xc911; &#xd558;&#xb098;&#xc600;&#xb2e4;.</p><p>&#xadf8;&#xb7ec;&#xb098; &#xb85c;&#xc988;&#xac00; &#xbe44;&#xc2a4;&#xbb34;&#xd2b8;&#xc5d0;&#xac8c; &apos;&#xbb58; &#xb9cc;&#xb4e4;&#xace0; &#xc2f6;&#xc740;&#xc9c0;&apos; &#xbb3c;&#xc5b4;&#xbd24;&#xb358; &#xac83;&#xc744; &#xacc4;&#xae30;&#xb85c;, &#xbe44;&#xc2a4;&#xbb34;&#xd2b8; &#xb610;&#xd55c; &#xc790;&#xc2e0;&#xc774; &#xc6d0;&#xd558;&#xb294; &#xc790;&#xc2e0;&#xc744; &#xb9cc;&#xb4e4;&#xae30; &#xc704;&#xd574; &#xd06c;&#xb9ac;&#xc2a4;&#xd0c8; &#xc82c;&#xc774; &#xb418;&#xc5b4; &#xadf8;&#xb4e4;&#xc774; &#xc804;&#xc7c1;&#xc5d0; &#xc0ac;&#xc6a9;&#xd560; &#xc5ec;&#xb7ec;&#xac00;&#xc9c0; &#xbb34;&#xae30;&#xb97c; &#xc81c;&#xc791;&#xd558;&#xac8c; &#xb41c;&#xb2e4;. &#xb85c;&#xc988;&#xc758; &#xac80; &#xc5ed;&#xc2dc; &#xbe44;&#xc2a4;&#xbb34;&#xd2b8;&#xac00; &#xc81c;&#xc791;&#xd55c; &#xac83;&#xc774;&#xba70;, &#xadf8; &#xc678;&#xc5d0;&#xb3c4; &#xc82c; &#xbb34;&#xae30;&#xb97c; &#xc5c5;&#xadf8;&#xb808;&#xc774;&#xb4dc; &#xc2dc;&#xd0a4;&#xb294; &#xc5ec;&#xb7ec;&#xac00;&#xc9c0; &#xbd80;&#xd488;&#xb4e4;&#xc744; &#xb9cc;&#xb4e4;&#xc5b4;&#xb0c8;&#xb358; &#xac83; &#xac19;&#xb2e4;.</p><p>&#xa0;<br></p><p>&#xadf8;&#xb7ec;&#xb098; &#xc804;&#xc7c1;&#xc5d0;&#xc11c; &#xc774;&#xae30;&#xace0;&#xc790; &#xd588;&#xb358; &#xbe44;&#xc2a4;&#xbb34;&#xd2b8;&#xc758; &#xc695;&#xb9dd;&#xc740; &apos;&#xbe0c;&#xb808;&#xc774;&#xd0b9; &#xd3ec;&#xc778;&#xd2b8;&apos;&#xb77c;&#xb294; &#xbb34;&#xae30;&#xb97c; &#xb9cc;&#xb4e4;&#xc5b4; &#xb0b4;&#xac8c; &#xb41c;&#xb2e4;. &#xc82c;&#xb4e4;&#xc758; &#xbcf8;&#xccb4;&#xc778; &#xc82c;&#xc744; &#xbe60;&#xb974;&#xace0; &#xd655;&#xc2e4;&#xd558;&#xac8c; &#xd30c;&#xad34;&#xd560; &#xc218; &#xc788;&#xb294; &#xbb34;&#xae30;&#xb85c;&#xc11c;, &#xbe44;&#xc2a4;&#xbb34;&#xd2b8;&#xb294; &#xc774; &#xbb34;&#xae30;&#xb97c; &#xd1b5;&#xd574; &#xc804;&#xc7c1;&#xc744; &#xc720;&#xb9ac;&#xd558;&#xac8c; &#xc774;&#xb04c;&#xc5b4;&#xac00;&#xace0;&#xc790; &#xd588;&#xc9c0;&#xb9cc; &#xc774; &#xbb34;&#xae30;&#xc758; &#xc0ac;&#xc6a9;&#xc744; &#xbc18;&#xb300;&#xd588;&#xb358; &#xb85c;&#xc988;&#xc640; &#xac08;&#xb4f1;&#xc744; &#xbe5a;&#xac8c; &#xb418;&#xace0; &#xacb0;&#xad6d; &#xb85c;&#xc988;&#xb294; &#xbe44;&#xc2a4;&#xbb34;&#xd2b8;&#xc758; &#xbb3c;&#xb9ac;&#xc801; &#xd615;&#xd0dc;&#xb97c; &#xd30c;&#xad34;&#xd558;&#xc5ec; &#xbc84;&#xbe14;&#xc5d0; &#xac00;&#xb454; &#xb4a4; &#xb77c;&#xc774;&#xc5b8;&#xc5d0; &#xbcf4;&#xad00;&#xd558;&#xac8c; &#xb418;&#xc5c8;&#xb2e4;.</p><p>&#xa0;<br></p><p>&#xc774;&#xd6c4; &#xc2a4;&#xd2f0;&#xbe10;&#xc5d0; &#xc758;&#xd574; &#xbe44;&#xc2a4;&#xbb34;&#xd2b8;&#xb294; &#xc774;&#xbbf8; &#xc804;&#xc7c1;&#xc774; &#xb05d;&#xb09c; &#xc138;&#xc0c1;&#xc5d0; &#xb2e4;&#xc2dc; &#xb098;&#xc624;&#xac8c; &#xb41c;&#xb2e4;. &#xc624;&#xb798;&#xac04;&#xb9cc;&#xc5d0; &#xbcf4;&#xac8c; &#xb41c; &#xc624;&#xb9ac;&#xc9c0;&#xb110; &#xd06c;&#xb9ac;&#xc2a4;&#xd0c8; &#xc82c;&#xc2a4; &#xba64;&#xbc84;(&#xac00;&#xb137;, &#xd384;)&#xb4e4;&#xc744; &#xbcf4;&#xace0; &#xb9e4;&#xc6b0; &#xae30;&#xbed0;&#xd558;&#xc9c0;&#xb9cc;, &#xc804;&#xc7c1;&#xc758; &#xc5ec;&#xd30c;&#xb85c; &#xb450;&#xbc88;&#xb2e4;&#xc2dc; &#xbcfc; &#xc218; &#xc5c6;&#xac8c; &#xb41c; &#xce5c;&#xad6c;&#xb4e4;&#xb3c4; &#xb9ce;&#xc558;&#xb2e4;. &#xd558;&#xc9c0;&#xb9cc; &#xc9c0;&#xad6c;&#xb97c; &#xd5a5;&#xd55c; &#xd648;&#xc6d4;&#xb4dc;&#xc758; &#xc704;&#xd611;&#xc740; &#xc544;&#xc9c1; &#xb05d;&#xb098;&#xc9c0; &#xc54a;&#xc558;&#xae30;&#xc5d0;, &#xbe44;&#xc2a4;&#xbb34;&#xd2b8;&#xb294; &#xc624;&#xb798;&#xb41c; &#xb300;&#xc7a5;&#xac04;&#xc744; &#xac1c;&#xbc29;&#xd558;&#xace0; &#xd06c;&#xb9ac;&#xc2a4;&#xd0c8; &#xc82c;&#xc2a4;&#xb97c; &#xc704;&#xd574; &#xbb34;&#xae30;&#xb97c; &#xb9cc;&#xb4e4;&#xba70; &#xadf8;&#xb4e4;&#xacfc; &#xd6c8;&#xb828;&#xd55c;&#xb2e4;. &#xb610;&#xd55c; &#xc2a4;&#xd2f0;&#xbe10; &#xbc29;&#xc2dd;&#xc758; &#xc804;&#xc7c1; &#xc900;&#xbe44;&#xc5d0;&#xb3c4; &#xc120;&#xb73b; &#xcc38;&#xc5ec;&#xd574; &#xd3b8;&#xc548;&#xd55c; &#xc2dc;&#xac04;&#xc744; &#xbcf4;&#xb0b4;&#xae30;&#xb3c4; &#xd558;&#xace0;, &#xc2a4;&#xd2f0;&#xbe10;&#xc758; &#xb85c;&#xc988;&#xc5d0; &#xb300;&#xd55c; &#xc911;&#xc555;&#xac10;&#xc744; &#xc704;&#xb85c;&#xd558;&#xace0; &#xc751;&#xc6d0;&#xd574;&#xc900;&#xb2e4;.</p><p>&#xa0;<br></p><p>&#xadf8; &#xacfc;&#xc815;&#xc5d0;&#xc11c; &#xbe44;&#xc2a4;&#xbb34;&#xd2b8;&#xb294; &#xb2e4;&#xc2dc;&#xae08; &#xc790;&#xc2e0;&#xc758; &#xc804;&#xc7c1;&#xbb34;&#xae30; &apos;&#xbe0c;&#xb808;&#xc774;&#xd0b9; &#xd3ec;&#xc778;&#xd2b8;&apos;&#xac00; &#xd544;&#xc694;&#xd55c; &#xc0c1;&#xd669;&#xc774;&#xb77c;&#xba70; &#xc2a4;&#xd2f0;&#xbe10;&#xc5d0;&#xac8c; &#xbcf4;&#xc5ec;&#xc8fc;&#xc9c0;&#x