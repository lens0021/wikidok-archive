	"postContents": "<div class=\"col-md-12 col-sm-12\">\r\n\t\t\t\t<p><img src=\"/api/File/Real/57a3ac1249cbb9423e9560dd\" data-mce-src=\"/api/File/Real/57a3ac1249cbb9423e9560dd\"></p><p>&#xc560;&#xb35c;&#xb9b0; &#xbc84;&#xc9c0;&#xb2c8;&#xc544; &#xc2a4;&#xd2f0;&#xbe10; &#xc6b8;&#xd504;&#xa0;Adeline Virginia Stephen Woolf (1882. 1. 25~ 1941. 3. 28)</p><p>&#xc601;&#xad6d;&#xc758; &#xc791;&#xac00;.</p><h2>&#xac1c;&#xc694;</h2><p>&#xbc84;&#xc9c0;&#xb2c8;&#xc544; &#xc6b8;&#xd504;(Virginia Woolf)&#xb294; 20&#xc138;&#xae30; &#xc601;&#xad6d;&#xc758; &#xc791;&#xac00;&#xc774;&#xb2e4;. &#xbaa8;&#xb354;&#xb2c8;&#xc998; &#xc2dc;&#xb300;&#xc5d0; &#xc758;&#xc2dd;&#xc758; &#xd750;&#xb984; &#xae30;&#xbc95;&#xc744; &#xc644;&#xc131;&#xc2dc;&#xd0a8; &#xc791;&#xac00; &#xc911; &#xd558;&#xb098;&#xb85c;, &#xc120;&#xad6c;&#xc801;&#xc778; &#xd398;&#xbbf8;&#xb2c8;&#xc2a4;&#xd2b8; &#xc791;&#xac00;&#xb85c;&#xb3c4; &#xc720;&#xba85;&#xd558;&#xb2e4;. &#xb300;&#xd45c;&#xc791;&#xc73c;&#xb85c;&#xb294;&#xa0;&#x300a;&#xc790;&#xae30;&#xb9cc;&#xc758; &#xbc29;&#x300b;,&#xa0;&#x300a;&#xb308;&#xb7ec;&#xc6e8;&#xc774; &#xbd80;&#xc778;&#x300b; &#xb4f1;&#xc774; &#xc788;&#xb2e4;.</p><p>&#xa0;</p><h2>&#xc0dd;&#xc560;</h2><p>1882&#xb144; 1&#xc6d4; 25&#xc77c; &#xb7f0;&#xb358;&#xc5d0;&#xc11c; &#xb808;&#xc2ac;&#xb9ac; &#xc2a4;&#xd14c;&#xd310;&#xacfc; &#xc904;&#xb9ac;&#xc544; &#xb355;&#xc6cc;&#xc2a4; &#xc0ac;&#xc774;&#xc5d0;&#xc11c; &#xd0dc;&#xc5b4;&#xb0ac;&#xc73c;&#xba70;, &#xc720;&#xba85;&#xd55c; &#xbe44;&#xd3c9;&#xac00;&#xc778; &#xc544;&#xbc84;&#xc9c0; &#xbc11;&#xc5d0;&#xc11c; &#xad50;&#xc721;&#xc744; &#xbc1b;&#xc558;&#xb2e4;. 1895&#xb144; &#xc5b4;&#xba38;&#xb2c8; &#xc0ac;&#xb9dd; &#xd6c4; &#xcd5c;&#xcd08;&#xc758; &#xc815;&#xc2e0;&#xc9c8;&#xd658; &#xc99d;&#xc138;&#xb97c; &#xbcf4;&#xc600;&#xc73c;&#xba70; 1904&#xb144; &#xc544;&#xbc84;&#xc9c0;&#xc758; &#xc0ac;&#xb9dd; &#xc774;&#xd6c4; &#xc99d;&#xc138;&#xac00; &#xc545;&#xd654;&#xb418;&#xc5b4; &#xc790;&#xc0b4;&#xc2dc;&#xb3c4;&#xb97c; &#xd558;&#xc600;&#xc73c;&#xb098; &#xbbf8;&#xc218;&#xc5d0; &#xadf8;&#xcce4;&#xb2e4;. &#xc591;&#xce5c;&#xc758; &#xc0ac;&#xb9dd; &#xd6c4; &#xb7f0;&#xb358; &#xbe14;&#xb8f8;&#xc988;&#xbc84;&#xb9ac;&#xb85c; &#xc62e;&#xaca8;&#xac00; &#xbe14;&#xb8f8;&#xc988;&#xbc84;&#xb9ac; &#xadf8;&#xb8f9;&#xc744; &#xb9cc;&#xb4e4;&#xace0; &#xb2f9;&#xb300;&#xc758; &#xc7ac;&#xb2a5;&#xc788;&#xb294; &#xc9c0;&#xc131;&#xb4e4;&#xacfc; &#xad50;&#xb958;&#xd558;&#xc600;&#xb2e4;. 1912&#xb144; &#xbe14;&#xb8f8;&#xc988;&#xbc84;&#xb9ac; &#xadf8;&#xb8f9;<span id=\"#wk_ft1\" class=\"wiki-fnote\" contenteditable=\"false\" data-cls=\"foot\"><button class=\"wiki-fnotepop\" data-container=\".wiki-fnote\" data-placement=\"auto bottom\" data-content=\"Life in Squares (2015) - &#xbe14;&#xb8f8;&#xc988;&#xbc84;&#xb9ac; &#xadf8;&#xb8f9; &#xc774;&#xc57c;&#xae30;&#xb97c; &#xb2e4;&#xb8ec; &#xc601;&#xad6d; TV &#xbbf8;&#xb2c8;&#xc2dc;&#xb9ac;&#xc988;,\">1</button></span>&#xa0;&#xc758; &#xba64;&#xbc84; &#xc911; &#xd55c; &#xc0ac;&#xb78c;&#xc774;&#xc5c8;&#xb358; &#xb808;&#xc624;&#xb098;&#xb4dc; &#xc6b8;&#xd504;&#xc640; &#xacb0;&#xd63c;&#xd558;&#xc600;&#xc73c;&#xba70; 1915&#xb144; &#xccab;&#xc791;&#xa0;&#x300a;&#xcd9c;&#xd56d;&#x300b;&#xc744; &#xcd9c;&#xd310;&#xd558;&#xc600;&#xb2e4;.</p><p>&#xa0;<br></p><p>&#xc774;&#xd6c4; 1919&#xb144;&#xc5d0;&#xb294;&#xa0;&#x300a;&#xbc24;&#xacfc; &#xb0ae;&#x300b;, 1922&#xb144;&#xc5d0;&#xb294; &#x300a;&#xc81c;&#xc774;&#xcf65;&#xc758; &#xbc29;&#x300b;&#xb4f1;&#xc744; &#xbc1c;&#xd45c;&#xd558;&#xc600;&#xc73c;&#xba70; &#x300a;&#xd604;&#xb300; &#xc18c;&#xc124;&#xb860;&#x300b;(1919), &#x300a;&#xbca0;&#xb137; &#xc528;&#xc640; &#xbe0c;&#xb77c;&#xc6b4; &#xbd80;&#xc778;&#x300b;(1924)&#xb4f1;&#xc758; &#xc791;&#xd488;&#xc744; &#xd1b5;&#xd574; &#xc2e4;&#xd5d8;&#xc801; &#xc18c;&#xc124;&#xc758; &#xc694;&#xc18c;&#xc5d0; &#xb300;&#xd558;&#xc5ec; &#xb17c;&#xd588;&#xb2e4;. 1925&#xb144;&#xc5d0;&#xb294;&#xa0;&#x300a;&#xb308;&#xb7ec;&#xc6e8;&#xc774; &#xbd80;&#xc778;&#x300b;, 1927&#xb144;&#xc5d0;&#xb294; &#xc758;&#xc2dd;&#xc758; &#xd750;&#xb984; &#xae30;&#xbc95;&#xc73c;&#xb85c; &#xc720;&#xba85;&#xd55c;&#xa0;&#x300a;&#xb4f1;&#xb300;&#xb85c;&#x300b;&#xb97c; &#xcd9c;&#xac04;&#xd558;&#xc600;&#xc73c;&#xba70;, 1929&#xb144;&#xc5d0;&#xb294; &#xd398;&#xbbf8;&#xb2c8;&#xc998; &#xc5d0;&#xc138;&#xc774;&#xb85c; &#xc720;&#xba85;&#xd55c; &#x300a;&#xc790;&#xae30;&#xb9cc;&#xc758; &#xbc29;&#x300b;&#xc744; &#xcd9c;&#xac04;&#xd558;&#xc600;&#xb2e4;. &#xc774;&#xd6c4; &#xd2b8;&#xb79c;&#xc2a4;&#xc139;&#xc288;&#xc5bc;&#xc778; &#xc2dc;&#xc778; &#xc62c;&#xb79c;&#xb3c4;&#xb97c; &#xc8fc;&#xc778;&#xacf5;&#xc73c;&#xb85c; &#xd55c; &#xc18c;&#xc124; &#x300a;&#xc62c;&#xb79c;&#xb3c4;&#x300b;(1928),&#xa0;&#x300a;&#xd30c;&#xb3c4;&#x300b;(1931), &#x300a;&#xc138;&#xc6d4;&#x300b;(1937),&#xa0;&#x300a;&#xb9c9;&#xac04;&#x300b;(1941)&#xb4f1;&#xc758; &#xc18c;&#xc124;&#xc744; &#xbc1c;&#xd45c;&#xd558;&#xc600;&#xace0;&#xa0;1939&#xb144; 2&#xcc28; &#xc138;&#xacc4;&#xb300;&#xc804; &#xbc1c;&#xbc1c; &#xd6c4; &#xc2ec;&#xd55c; &#xc2e0;&#xacbd;&#xc99d;&#xc744; &#xc553;&#xb2e4;&#xac00; 1941&#xb144; 3&#xc6d4; 28&#xc77c; &#xc8fc;&#xba38;&#xb2c8;&#xc5d0; &#xb3cc;&#xc744; &#xb123;&#xc740;&#xcc44; &#xc6b0;&#xc2a4; &#xac15;&#xc5d0; &#xd22c;&#xc2e0; &#xc790;&#xc0b4;&#xd558;&#xc5ec; &#xc0dd;&#xc744; &#xb9c8;&#xac10;&#xd588;&#xb2e4;.<br></p><p>&#xa0;</p><p>&#xbc84;&#xc9c0;&#xb2c8;&#xc544; &#xc6b8;&#xd504;&#xb294; &#xc5b4;&#xb9b0; &#xc2dc;&#xc808;&#xbd80;&#xd130; &#xb098;&#xc774;&#xcc28;&#xac00; &#xb9ce;&#xc774; &#xb098;&#xb294; &#xc758;&#xbd93;&#xc624;&#xbe60;&#xc778; &#xc870;&#xc9c0;&#xc640; &#xc81c;&#xb7f4;&#xb4dc;&#xc5d0;&#xac8c; &#xc131;&#xd76c;&#xb871; &#xb4f1;&#xc758; &#xc131;&#xc801; &#xd559;&#xb300;&#xb97c; &#xb2f9;&#xd558;&#xc600;&#xc73c;&#xba70;(&#xc870;&#xc9c0;&#xc640; &#xc81c;&#xb7f4;&#xb4dc;&#xb294; &#xbc84;&#xc9c0;&#xb2c8;&#xc544; &#xbfd0; &#xc544;&#xb2c8;&#xb77c; &#xbc84;&#xc9c0;&#xb2c8;&#xc544;&#xc758; &#xc5b8;&#xb2c8; &#xbc14;&#xb124;&#xc0ac;&#xc5d0;&#xac8c;&#xb3c4; &#xc131;&#xc801; &#xd559;&#xb300;&#xb97c; &#xac00;&#xd588;&#xb2e4;) &#xc774; &#xd559;&#xb300;&#xb294; &#xadf8;&#xb140;&#xc758; &#xc0b6;&#xacfc; &#xc2e0;&#xacbd;&#xc9c8;&#xd658;&#xc5d0; &#xc601;&#xd5a5;&#xc744; &#xc8fc;&#xc5c8;&#xb2e4;. &#xb610;&#xd55c; &#xb3d9;&#xc131;&#xc560;(&#xb808;&#xc988;&#xbe44;&#xc5b8;) &#xc131;&#xd5a5;&#xc744; &#xc9c0;&#xb2c8;&#xace0; &#xc788;&#xb294; &#xc791;&#xac00;&#xb85c;&#xb3c4; &#xc798; &#xc54c;&#xb824;&#xc838; &#xc788;&#xb294;&#xb370;, &#xbc84;&#xc9c0;&#xb2c8;&#xc544; &#xc6b8;&#xd504;&#xb97c; &#xb4f1;&#xc7a5; &#xc778;&#xbb3c; &#xc911; &#xd55c; &#xc0ac;&#xb78c;&#xc73c;&#xb85c; &#xb2e4;&#xb8e8;&#xace0; &#xc788;&#xb294; &#xc601;&#xd654; &lt;&#xb514; &#xc544;&#xc6cc;&#xc2a4;&gt;(1999) &#xb4f1;&#xc5d0;&#xc11c; &#xc798; &#xb4dc;&#xb7ec;&#xb0b4;&#xace0; &#xc788;&#xb2e4;.</p><p>&#xa0;<br></p><h2>&#xc791;&#xd488;</h2><ul><li>&#x300a;&#xcd9c;&#xd56d;&#x300b; (1915&#xb144;)</li><li>&#x300a;&#xbc24;&#xacfc; &#xb0ae;&#x300b; (1919&#xb144;)</li><li>&#x300a;&#xd604;&#xb300; &#xc18c;&#xc124;&#xb860;&#x300b; (1919&#xb144;)</li><li>&#x300a;&#xc81c;&#xc774;&#xcf65;&#xc758; &#xbc29;&#x300b;(1922&#xb144;)</li><li>&#x300a;&#xbca0;&#xb137; &#xc528;&#xc640; &#xbe0c;&#xb77c;&#xc6b4; &#xbd80;&#xc778;&#x300b; (1924&#xb144;)</li><li>&#x300a;&#xb308;&#xb7ec;&#xc6e8;&#xc774; &#xbd80;&#xc778;&#x300b;(1925&#xb144;)</li><li>&#x300a;&#xb4f1;&#xb300;&#xb85c;&#x300b; (1927&#xb144;)</li><li>&#x300a;&#xc62c;&#xb79c;&#xb3c4;&#x300b; (1928&#xb144;)</li><li>&#x300a;<a data-mce-href=\"/wp-d/580e538f97f02f13526b07d4/View\" href=\"/wp-d/580e538f97f02f13526b07d4/View\" class=\"pjax-link\">&#xc790;&#xae30;&#xb9cc;&#xc758; &#xbc29;</a>&#x300b; (1929&#xb144;)</li><li>&#x300a;&#xd30c;&#xb3c4;&#x300b; (1931&#xb144;)</li><li>&#x300a;&#xc138;&#xc6d4;&#x300b; (1937&#xb144;)</li><li>&#x300a;&#xb9c9;&#xac04;&#x300b; (1941&#xb144;)</li></ul><p><br></p><h2>&#xbbf8;&#xb514;&#xc5b4;&#xd654;</h2><ul><li>&#xb514; &#xc544;&#xc6cc;&#xc2a4; The Hours (1999, &#xc2a4;&#xd2f0;&#xbe10; &#xb2ec;&#xb4dc;&#xb9ac; &#xc5f0;&#xcd9c;)</li><li>&#xc62c;&#xb79c;&#xb3c4; Orlando (1993, <a data-mce-href=\"/d