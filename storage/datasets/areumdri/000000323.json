 &#xc8fc;&#xc0ac;&#xc704; &#xb208;&#xae08;&#xc744; &#xbc14;&#xd0d5;&#xc73c;&#xb85c; &#xad00;&#xacc4;/&#xc695;&#xb9dd;/&#xc7a5;&#xc18c;/&#xbb3c;&#xac74;&#xc744; &#xace0;&#xb978;&#xb2e4;. (&#xb098;&#xc624;&#xc9c0; &#xc54a;&#xc740; &#xc22b;&#xc790;&#xb294; &#xace0;&#xb97c; &#xc218; &#xc5c6;&#xb2e4;)&#xa0;</li><li>&#xac01; &#xd50c;&#xb808;&#xc774;&#xc5b4; &#xc0ac;&#xc774;&#xc5d0;&#xb294; &#xad00;&#xacc4;&#xac00; &#xc131;&#xb9bd;&#xd558;&#xba70; &#xc695;&#xb9dd;/&#xc7a5;&#xc18c;/&#xbb3c;&#xac74;&#xc740; &#xadf8; &#xd50c;&#xb808;&#xc774; &#xb0b4;&#xc5d0; &#xcd5c;&#xc18c; &#xd55c;&#xac00;&#xc9c0; &#xc774;&#xc0c1;&#xc529; &#xc874;&#xc7ac;&#xd558;&#xc5ec;&#xc57c; &#xd55c;&#xb2e4;.</li><li>&#xb9c8;&#xc9c0;&#xb9c9; &#xb0a8;&#xc740; &#xc8fc;&#xc0ac;&#xc704;&#xb294; &#xc640;&#xc77c;&#xb4dc; &#xce74;&#xb4dc;&#xb85c;, &#xc6d0;&#xd558;&#xb294; &#xb208;&#xae08;&#xc744; &#xc9c0;&#xc815;&#xd560; &#xc218; &#xc788;&#xb2e4;.&#xa0;</li><li>&#xc804;&#xbd80; &#xc124;&#xc815;&#xd588;&#xb2e4;&#xba74;, &#xc8fc;&#xc0ac;&#xc704;&#xb97c; &#xac00;&#xc6b4;&#xb370;&#xb85c; &#xbaa8;&#xc740;&#xb2e4;.&#xa0;</li></ul><p>&#xa0;</p><div contenteditable=\"false\"><div class=\"alert alert-info\" contenteditable=\"true\">&#xa0;&#xa0; <em>&#xc608;&#xc2dc; ) 3 &#xc778;&#xc758; &#xd50c;&#xb808;&#xc774;&#xc5b4;&#xac00; &#xc8fc;&#xc0ac;&#xc704;&#xb97c; &#xad74;&#xb824; (3*4 = &#xcd1d; 12&#xac1c;) &#xa0;5,2,1,3,3,4,4,1,2,2,1,5 &#xc758; &#xb208;&#xae08;&#xc774; &#xb098;&#xc654;&#xb2e4;.&#xa0;</em> <em>&#xc81c;&#xc784;&#xc2a4;&#xc640; &#xc5d0;&#xc774;&#xbbf8;&#xb294; &#xc9c1;&#xc7a5; &#xad00;&#xacc4;(&#xd070; &#xd2c0;) &#xc774;&#xba70; &#xa0;&#xc804;&#xc5d0; &#xac19;&#xc774; &#xc77c;&#xd588;&#xb358; &#xc0ac;&#xc774;(&#xc138;&#xbd80; &#xc694;&#xc18c;)&#xc774;&#xb2e4;. (&#xc8fc;&#xc0ac;&#xc704; 2, 1&#xc744; &#xc0ac;&#xc6a9;) &#xc81c;&#xc784;&#xc2a4;&#xc640; &#xc5d0;&#xc774;&#xbbf8;&#xb294; &#xc804;&#xbb38;&#xc9c1; &#xad00;&#xacc4;&#xb97c; &#xace0;&#xb974;&#xace0; &#xc2f6;&#xc5c8;&#xc9c0;&#xb9cc; &#xc8fc;&#xc0ac;&#xc704; 6&#xc774; &#xb098;&#xc624;&#xc9c0; &#xc54a;&#xc544; &#xace0;&#xb97c; &#xc218; &#xc5c6;&#xc5c8;&#xb2e4;. &#xb514;&#xd14c;&#xc77c;&#xd558;&#xac8c;, &#xb458;&#xc740; &#xc61b;&#xb0a0;&#xc5d4; &#xac19;&#xc774; &#xc77c;&#xd588;&#xc9c0;&#xb9cc; &#xc9c0;&#xae08;&#xc740; &#xc0ac;&#xc774;&#xac00; &#xd2c0;&#xc5b4;&#xc838; &#xb9e4;&#xc6b0; &#xc548;&#xc88b;&#xb2e4;&#xb294; &#xc774;&#xc57c;&#xae30;&#xb97c; &#xd55c;&#xb2e4;.&#xa0;</em></div></div><p><br></p><h3>1&#xb9c9;</h3><p>&#xc7a5;&#xba74;&#xc744; &#xd50c;&#xb808;&#xc774;&#xd55c;&#xb2e4;. &#xc790;&#xc2e0;&#xc758; &#xc7a5;&#xba74;&#xc5d0;&#xc11c;&#xb294; &#xc790;&#xc2e0;&#xc758; &#xce90;&#xb9ad;&#xd130;&#xac00; &#xc2a4;&#xd3ec;&#xd2b8;&#xb77c;&#xc774;&#xd2b8;&#xb97c; &#xbc1b;&#xb294;&#xb2e4;. &#xd06c;&#xac8c; &#xd574;&#xacb0;&#xacfc; &#xc124;&#xc815;&#xc774; &#xc788;&#xb2e4;.&#xa0;</p><p>&#xa0;<br></p><ul><li>&#xa0;&#xc124;&#xc815; : &#xb0b4;&#xac00; &#xc6d0;&#xd558;&#xb294; &#xc7a5;&#xba74;&#xc744; &#xb9cc;&#xb4e0;&#xb2e4;. &#xd50c;&#xb808;&#xc774; &#xb3c4;&#xc911;, &#xb2e4;&#xb978; &#xd50c;&#xb808;&#xc774;&#xc5b4;&#xb4e4;&#xc774; &#xd751;&#xc774;&#xb098; &#xbc31; &#xc8fc;&#xc0ac;&#xc704;&#xb97c; &#xc918; &#xce90;&#xb9ad;&#xd130;&#xc758; &#xacb0;&#xb9d0;&#xc744; &#xc815;&#xd55c;&#xb2e4;. &#xc774;&#xa0;&#xb2e8;&#xacc4;&#xc5d0;&#xc11c; &#xb2e4;&#xb978; &#xd50c;&#xb808;&#xc774;&#xc5b4;&#xb4e4;&#xc774; &#xc8fc;&#xc0ac;&#xc704;&#xb97c; &#xc904; &#xb54c; &#xc8fc;&#xc0ac;&#xc704;&#xc758; &#xc0c9;&#xc740; &#xd50c;&#xb808;&#xc774;&#xd558;&#xb294; &#xd50c;&#xb808;&#xc774;&#xc5b4;&#xc758; &#xc758;&#xc0ac;&#xb294; &#xbc18;&#xc601;&#xb418;&#xc9c0; &#xc54a;&#xc73c;&#xba70;, &#xd50c;&#xb808;&#xc774;&#xc5b4;&#xb294; &#xc790;&#xc2e0;&#xc774; &#xbc1b;&#xc740; &#xc8fc;&#xc0ac;&#xc704;&#xb97c; &#xd1a0;&#xb300;&#xb85c; &#xce90;&#xb9ad;&#xd130;&#xc758; &#xacb0;&#xb9d0;&#xc744; &#xc815;&#xd558;&#xba74; &#xb41c;&#xb2e4;. (&#xd751;-&#xbd80;&#xc815;&#xc801; / &#xbc31;-&#xae0d;&#xc815;&#xc801;)&#xa0;</li><li>&#xa0;&#xd574;&#xacb0; : &#xb2e4;&#xb978; &#xd50c;&#xb808;&#xc774;&#xc5b4;&#xb4e4;&#xc5d0;&#xac8c; &#xb0b4;&#xac00; &#xb098;&#xc62c; &#xc7a5;&#xba74;&#xc744; &#xc124;&#xc815;&#xd574;&#xb2ec;&#xb77c;&#xace0; &#xd55c;&#xb2e4;. &#xc774; &#xacbd;&#xc6b0; &#xbc1b;&#xc744; &#xce90;&#xb9ad;&#xd130;&#xc758; &#xc8fc;&#xc0ac;&#xc704;&#xc640; &#xacb0;&#xb9d0;&#xc740; &#xc790;&#xae30;&#xc2e0;&#xc774; &#xc815;&#xd55c;&#xb2e4;.<br></li></ul><p>&#xa0;<br></p><p>&#xc774;&#xd6c4; <strong>&#xbc1b;&#xc740; &#xc790;&#xc2e0;&#xc758; &#xc8fc;&#xc0ac;&#xc704;&#xb97c; &#xb2e4;&#xb978; &#xd50c;&#xb808;&#xc774;&#xc5b4;&#xc5d0;&#xac8c; &#xc900;&#xb2e4;</strong>. &#xc774;&#xac83;&#xc744; &#xb450; &#xbc14;&#xd034; &#xb3cc; &#xb3d9;&#xc548; &#xbc18;&#xbcf5;&#xd55c;&#xb2e4;.&#xa0;</p><div contenteditable=\"false\"><div class=\"alert alert-info\" contenteditable=\"true\"><p>&#xc124;&#xc815; &#xc608;&#xc2dc;) &#xa0;&#xc81c;&#xc784;&#xc2a4;&#xc758; &#xc124;&#xc815; &#xb2e8;&#xacc4;&#xc5d0;&#xc11c;, &#xc81c;&#xc784;&#xc2a4;&#xb294; &#xc804;&#xc5d0; &#xac19;&#xc774; &#xc77c;&#xd588;&#xb358; &#xc5d0;&#xc774;&#xbbf8;&#xc5d0;&#xac8c; &#xc0ac;&#xadc0;&#xc790;&#xb294; &#xba58;&#xd2b8;&#xb97c; &#xc9c0;&#xc18d;&#xc801;&#xc73c;&#xb85c; &#xb0a0;&#xb9ac;&#xace0; &#xc788;&#xb2e4;, &#xc5d0;&#xc774;&#xbbf8;&#xb294; &#xc81c;&#xc784;&#xc2a4;&#xb97c; &#xc2eb;&#xc5b4;&#xd558;&#xae30; &#xb54c;&#xbb38;&#xc5d0;, &#xc2dc;&#xd070;&#xb465;&#xd55c; &#xbc18;&#xc751;&#xc774;&#xb2e4;.</p><p>&#xb458;&#xc774; &#xb300;&#xd654;&#xb97c; &#xd558;&#xace0; &#xc788;&#xb294; &#xc640;&#xc911;&#xc5d0;, &#xb2e4;&#xb978; &#xd50c;&#xb808;&#xc774;&#xc5b4;&#xb4e4;&#xc740; &#xd765;&#xbbf8;&#xb86d;&#xac8c; &#xc9c0;&#xcf1c;&#xbcf4;&#xace0; &#xc788;&#xb2e4;&#xac00; &#xc11c;&#xb85c; &#xb208;&#xc9d3;&#xc744; &#xad50;&#xd658;&#xd558;&#xace0; &#xd751; &#xc8fc;&#xc0ac;&#xc704;&#xb97c; &#xc81c;&#xc784;&#xc2a4;&#xc5d0;&#xac8c; &#xac74;&#xb124; &#xc900;&#xb2e4;. &#xc8fc;&#xc0ac;&#xc704;&#xc758; &#xacb0;&#xacfc;&#xc5d0; &#xb530;&#xb77c;, &#xc5d0;&#xc774;&#xbbf8;&#xb294; &#xcc28;&#xac11;&#xac8c;(&#