&#xac00;&#xb2c8;&#xc2a4;&#xd0c4;&#xc758; &#xbc14;&#xb2e4;&#xd06c;&#xc0e8;, &#xce60;&#xb808;&#xc758; &#xc548;&#xb370;&#xc2a4; &#xc0b0;&#xb9e5;, &#xb7ec;&#xc2dc;&#xc544;, &#xc559;&#xace8;&#xb77c;, &#xbbf8;&#xc580;&#xb9c8; &#xb4f1;</li><li><strong>&#xd2b9;&#xc9d5;</strong> : &#xc5ed;&#xc0ac;&#xac00; &#xc624;&#xb798;&#xb41c; &#xb3cc; &#xc911;&#xc5d0; &#xd558;&#xb098;&#xb85c;, &#xadc0;&#xd558;&#xac8c; &#xc5ec;&#xaca8;&#xc84c;&#xb2e4;.&#xa0;</li><li><strong>&#xc758;&#xbbf8;</strong> : &#xc561;&#xb9c9;&#xc774;, &#xd589;&#xc6b4;&#xc744; &#xac00;&#xc838;&#xb2e4;&#xc8fc;&#xba70;, &#xc7ac;&#xd574;&#xb85c;&#xbd80;&#xd130; &#xc790;&#xc2e0;&#xc744; &#xbcf4;&#xd638;.</li></ul></div></div><h2><strong>&#xc131;&#xaca9;</strong></h2><p>&#xa0;<br></p><p>&#xb77c;&#xd53c;&#xc2a4;&#xc758; &#xd604;&#xc7ac; &#xd658;&#xacbd;&#xc774; &#xd2b9;&#xc218;&#xd558;&#xc5ec; &#xbcf8; &#xc131;&#xaca9;&#xc744; &#xc9d0;&#xc791;&#xd558;&#xae30;&#xb294; &#xc5b4;&#xb824;&#xc6b0;&#xb098;, &#xbcf8;&#xb798; &#xc7a5;&#xb09c;&#xc744; &#xc88b;&#xc544;&#xd558;&#xb358; &#xc131;&#xaca9;&#xc73c;&#xb85c; &#xbcf4;&#xc778;&#xb2e4;. &#xb3c5;&#xb9bd;&#xc801;&#xc778; &#xc131;&#xd5a5;&#xb3c4; &#xac00;&#xc9c0;&#xace0; &#xc788;&#xc9c0;&#xb9cc;, &#xb2e8;&#xc21c;&#xd788; &#xd63c;&#xc790; &#xc788;&#xae30;&#xb97c; &#xc88b;&#xc544;&#xd558;&#xb294;&#xc9c0; &#xb610;&#xb294; &#xac19;&#xc774; &#xc788;&#xace0; &#xc2f6;&#xc740; &#xc0ac;&#xb78c;&#xc774; &#xc5c6;&#xc5b4;&#xc11c;&#xc778;&#xc9c0; &#xd655;&#xc2e4;&#xd558;&#xc9c0; &#xc54a;&#xb2e4;.</p><p>&#xd604;&#xc7ac;&#xb294; &#xb77c;&#xd53c;&#xc2a4;&#xb97c; &#xc774;&#xc6a9;&#xd558;&#xb358; &#xc82c;&#xb4e4;&#xc5d0;&#xac8c; &#xbd84;&#xb178;&#xb97c; &#xd488;&#xace0; &#xc788;&#xc73c;&#xba70;, &#xbc30;&#xd0c0;&#xc801;&#xc774;&#xace0; &#xc2dc;&#xb2c8;&#xceec;&#xd55c; &#xd0dc;&#xb3c4;&#xb97c; &#xbcf4;&#xc774;&#xace0; &#xd648;&#xc6d4;&#xb4dc;&#xb85c; &#xb3cc;&#xc544;&#xac00;&#xc9c0; &#xbabb;&#xd558;&#xb294; &#xc790;&#xc2e0;&#xc758; &#xcc98;&#xc9c0;&#xc5d0; &#xace0;&#xd1b5;&#xc2a4;&#xb7ec;&#xc6cc; &#xd558;&#xace0;&#xc788;&#xb2e4;.</p><p>&#xa0;<br></p><p>&#xc7ac;&#xc2a4;&#xd37c;&#xc640; &#xd06c;&#xb9ac;&#xc2a4;&#xd0c8;&#xc82c;&#xc758; &#xb300;&#xce58; &#xc911; &#xb3c4;&#xb9dd;&#xac08; &#xc218;&#xac00; &#xc5c6;&#xb294; &#xc0c1;&#xd669;&#xc774; &#xb418;&#xc790;, &#xc2a4;&#xd2f0;&#xbe10;&#xc744; &#xc704;&#xd574;&#xc11c; &#xc2a4;&#xc2a4;&#xb85c; &#xc801;&#xacfc; &#xd568;&#xaed8; &#xac00;&#xb77c;&#xc549;&#xc544;&#xbc84;&#xb9ac;&#xb294; &#xadf9;&#xb2e8;&#xc801;&#xc778; &#xc790;&#xae30;&#xd76c;&#xc0dd;&#xc758; &#xbc29;&#xbc95;&#xc744; &#xc120;&#xd0dd;&#xd558;&#xae30;&#xb3c4; &#xd588;&#xb2e4;.</p><h2>&#xc791;&#xc911; &#xd589;&#xbcf4;</h2><p>&#xa0;<br></p><p>&#xcd5c;&#xcd08;&#xc5d0; &#xc7a0;&#xc2dc; &#xb4e4;&#xb9ac;&#xb824;&#xb294; &#xbaa9;&#xc801;&#xc73c;&#xb85c; &#xc9c0;&#xad6c;&#xc5d0; &#xc654;&#xc73c;&#xb098;, &#xc9c0;&#xad6c;&#xc640; &#xd648;&#xc6d4;&#xb4dc;&#xac04;&#xc758; &#xc804;&#xc7c1;&#xc5d0; &#xd729;&#xc4f8;&#xb824; &#xd06c;&#xb9ac;&#xc2a4;&#xd0c8; &#xc82c;&#xc758; &#xacf5;&#xaca9;&#xc744; &#xbc1b;&#xc544; &#xbb3c;&#xb9ac;&#xc801; &#xd615;&#xd0dc;&#xac00; &#xae68;&#xc9c4;&#xb2e4;.&#xa0;&#xc774;&#xd6c4; &#xd648;&#xc6d4;&#xb4dc; &#xbcd1;&#xc0ac;&#xc5d0; &#xc758;&#xd574; &#xc8fc;&#xc6cc;&#xc838; &#xac70;&#xc6b8;&#xc5d0; &#xac07;&#xd788;&#xac8c; &#xb418;&#xc5c8;&#xace0;, &#xd06c;&#xb9ac;&#xc2a4;&#xd0c8; &#xc82c;&#xc774;&#xb77c;&#xace0; &#xc624;&#xd574;&#xd55c; &#xd648;&#xc6d4;&#xb4dc; &#xc82c;&#xb4e4;&#xc5d0;&#xac8c; &#xc2ec;&#xbb38;&#xc744; &#xbc1b;&#xc9c0;&#xb9cc; &#xb77c;&#xd53c;&#xc2a4;&#xb294; &#xd56d;&#xbcc0;&#xc744; &#xd558;&#xc9c0; &#xbabb;&#xd55c;&#xcc44; &#xc774;&#xc6a9;&#xb2f9;&#xd55c;&#xb2e4;. &#xc774;&#xd6c4; &#xd648;&#xc6d4;&#xb4dc;&#xc758; &#xc9c0;&#xad6c; &#xcca0;&#xc218; &#xacfc;&#xc815;&#xc5d0;&#xc11c; &#xbc1c;&#xc5d0; &#xbc1f;&#xd600; &#xc82c;&#xc774; &#xac08;&#xb77c;&#xc9c0;&#xace0;, &#xac70;&#xc6b8;&#xc740; &#xac24;&#xb7ed;&#xc2dc; &#xc6cc;&#xd504; &#xd328;&#xb4dc;&#xc5d0; &#xbc84;&#xb824;&#xc9c4;&#xcc44;&#xb85c; &#xc218;&#xcc9c;&#xb144;&#xc774; &#xc9c0;&#xb098; &#xd06c;&#xb9ac;&#xc2a4;&#xd0c8; &#xc82c;&#xc758; &#xd384;&#xc5d0;&#xac8c; &#xc8fc;&#xc6cc;&#xc9c4;&#xb2e4;. &#xd558;&#xc9c0;&#xb9cc;, &#xadf8;&#xb4e4;&#xb3c4; &#xb77c;&#xd53c;&#xc2a4;&#xb97c; &#xac70;&#xc6b8;&#xc5d0;&#xc11c; &#xaebc;&#xb0b4;&#xc9c0; &#xc54a;&#xace0; &#xc774;&#xc6a9;&#xd55c;&#xb2e4;.</p><p>&#xa0;<br></p><p>&#xc774;&#xd6c4; &#xac70;&#xc6b8;&#xc740; &#xc2a4;&#xd2f0;&#xbe10;&#xc758; &#xc190;&#xc5d0; &#xb4e4;&#xc5b4;&#xac04;&#xb2e4;. &#xd558;&#xc9c0;&#xb9cc; &#xc2a4;&#xd2f0;&#xbe10;&#xc740; &#xac70;&#xc6b8;&#xc744; &#xc774;&#xc6a9;&#xd558;&#xc9c0; &#xc54a;&#xace0; &#xb9d0;&#xc744; &#xac78;&#xc5b4;&#xc8fc;&#xace0; &#xc774;&#xc57c;&#xae30;&#xb97c; &#xd558;&#xba70; &#xce5c;&#xad6c;&#xb85c; &#xc5ec;&#xaca8;&#xc8fc;&#xba70;, &#xd384;&#xc758; &#xb9d0;&#xc5d4; &#xb300;&#xb2f5;&#xd558;&#xc9c0; &#xc54a;&#xb358; &#xac70;&#xc6b8;&#xc774; &#xc2a4;&#xd2f0;&#xbe10;&#xc5d0;&#xac90; &#xc2a4;&#xc2a4;&#xb85c; &#xb9d0;&#xb3c4; &#xac78;&#xae30; &#xc2dc;&#xc791;&#xd55c;&#xb2e4;. &#xc2a4;&#xd2f0;&#xbe10;&#xc740; &#xc774; &#xc0ac;&#xc2e4;&#xc744; &#xd06c;&#xb9ac;&#xc2a4;&#xd0c8; &#xc82c;&#xc2a4;&#xb4e4;&#xc5d0;&#xac8c; &#xc54c;&#xb838;&#xb2e4;. &#xd06c;&#xb9ac;&#xc2a4;&#xd0c8; &#xc82c;&#xc2a4;&#xb294; &#xbb54;&#xac00; &#xc774;&#xc0c1;&#xd55c; &#xc77c;&#xc774; &#xbc8c;&#xc5b4;&#xc9c0;&#xace0; &#xc788;&#xc74c;&#xc744; &#xb290;&#xaef4; &#xc2a4;&#xd2f0;&#xbe10;&#xc5d0;&#xac8c;&#xc11c; &#xac70;&#xc6b8;&#xc744; &#xbe8f;&#xc73c;&#xb824;&#xace0; &#xd558;&#xc9c0;&#xb9cc;, &#xac15;&#xd558;&#xac8c; &#xbd80;&#xc778;&#xd558;&#xb294; &#xac70;&#xc6b8;&#xc758; &#xbc18;&#xc751;&#xc744; &#xbcf4;&#xace0; &#xc2a4;&#xd2f0;&#xbe10;&#xc740; &#xd06c;&#xb9ac;&#xc2a4;&#xd0c8; &#xc82c;&#xc2a4;&#xc5d0;&#xac8c;&#xc11c; &#xb3c4;&#xb9dd;&#xac00; &#xac70;&#xc6b8; &#xb4a4;&#xc758; &#xc82c;&#xc744; &#xd574;&#xbc29;&#xd55c;&#xb2e4;. &#xb77c;&#xd53c;&#xc2a4;&#xb294; &#xc218;&#xcc9c;&#xb144;&#xb9cc;&#xc5d0; &#xb2e4;&#xc2dc; &#xbb3c;&#xb9ac;&#xc801; &#xd615;&#xd0dc;&#xb97c; &#xc774;&#xb8e8;&#xc5c8;&#xc73c;&#xb098;, &#xc82c;&#xc774; &#xac08;&#xb77c;&#xc9c4; &#xad00;&#xacc4;&#xb85c; &#xd615;&#xd0dc; &#xb610;&#xd55c; &#xbd88;&#xc548;&#xc815;&#xd588;&#xb2e4;.</p><p>&#xa0;<br></p><p>&#xb77c;&#xd53c;&#xc2a4;&#xb294; &#xb2e4;&#xc2dc; &#xd648;&#xc6d4;&#xb4dc;&#xb85c; &#xb3cc;&#xc544;&#xac00;&#xace0; &#xc2f6;&#xc5c8;&#xc9c0;&#xb9cc;, &#xc82c;&#xc774; &#xac08;&#xb77c;&#xc838; &#xc790;&#xc2e0;&#xc758; &#xbe44;&#xd589;&#xb2a5;&#xb825;&#xc744; &#xaebc;&#xb0b4;&#xc9c0; &#xbabb;&#xd588;&#xae30;&#xc5d0; &#xc9c0;&#xad6c;&#xc758; &#xbc14;&#xb2f7;&#xbb3c;&#xc744; &#xbaa8;&#xc544; &#xd0d1;&#xc744; &#xc62c;&#xb9ac;&#xae30; &#xc2dc;&#xc791;&#xd558;&#xba70;, &#xc774; &#xc601;&#xd5a5;&#xc73c;&#xb85c; &#xbe44;&#xce58;&#xc2dc;&#xd2f0;&#xc758; &#xbc14;&#xb2f7;&#xbb3c;&#xc774; &#xc0ac;&#xb77c;&#xc9c0;&#xac8c; &#xb41c;&#xb2e4;. &#xc774; &#xd604;&#xc0c1;&#xc744; &#xb418;&#xb3cc;&#xb9ac;&#xae30; &#xc704;&#xd574; &#xd06c;&#xb9ac;&#xc2a4;&#xd0c8; &#xc82c;&#xc2a4;&#xb294; &#xb77c;&#xd53c;&#xc2a4;&#xac00; &#xc788;&#xc744;&#xac83;&#xc73c;&#xb85c; &#xcd94;&#xc815;&#xb418;&#xb294; &#xc7a5;&#xc18c;&#xb85c; &#xd5a5;&#xd55c;&#xb2e4;. &#xc811;&#xadfc;&#xc744; &#xac70;&#xbd80;&#xd558;&#xb294; &#xb77c;&#xd53c;&#xc2a4;&#xc640; &#xd55c; &#xcc28;&#xb840; &#xc804;&#xd22c;&#xb97c; &#xbc8c;&#xc778; &#xb4a4;, &#xc2a4;&#xd2f0;&#xbe10;&#xc740; &#xd640;&#xb85c; &#xd0d1;&#xc744; &#xc62c;&#xb77c;&#xac00; &#xb77c;&#xd53c;&#xc2a4;&#xc640; &#xb9c8;&#xc8fc;&#xd55c;&#xb2e4;. &#xb77c;&#xd53c;&#xc2a4;&#xc758; &#xc0ac;&#xc815;&#xc744; &#xc774;&#xd574;&#xd55c; &#xc2a4;&#xd2f0;&#xbe10;&#xc740; &#xc790;&#xc2e0;&#xc758; &#xce58;&#xc720;&#xc758; &#xce68;&#xc73c;&#xb85c; &#xb77c;&#xd53c;&#xc2a4;&#xc758; &#xc82c;&#xc744; &#xace0;&#xccd0;&#xc8fc;&#xace0;, &#xb77c;&#xd53c;&#xc2a4;&#xb294; &#xd648;&#xc6d4;&#xb4dc;&#xb97c; &#xd5a5;&#xd574; &#xb0a0;&#xc544;&#xac04;&#xb2e4;. &#xbe44;&#xce58;&#xc2dc;&#xd2f0;&#xc5d0;&#xb3c4; &#xb2e4;&#xc2dc; &#xbc14;&#xb2f7;&#xbb3c;&#xc774; &#xb3cc;&#xc544;&#xc628;&#xb2e4;.</p><p>&#xa0;<br></p><p>&#xb77c;&#xd53c;&#xc2a4;&#xb294; &#xd648;&#xc6d4;&#xb4dc;&#xc5d0;&#xc11c; &#xc6b8;&#xbd80;&#xc9d6;&#xb294; &#xb3cc;&#xc744; &#xc774;&#xc6a9;&#xd574; &#xc601;&#xc0c1; &#xba54;&#xc2dc;&#xc9c0;&#xb97c; &#xbcf4;&#xb0b8;&#xb2e4;. &#xb0b4;&#xc6a9;&#xc778; &#xc989;&#xc2a8; &#xd648;&#xc6d4;&#xb4dc;&#xc5d0;&#xc11c; &#xc9c0;&#xad6c;&#xb97c; &#xd5a5;&#xd558;&#xace0; &#xc788;&#xc73c;&#xba70;, &#xd648;&#xc6d4;&#xb4dc;&#xb294; &#xc790;&#xc2e0;&#xc774; &#xc788;&#xb358; &#xc2dc;&#xc808;&#xacfc;&#xb294; &#xbe44;&#xad50;&#xac00; &#xc548; &#xb420; &#xc815;&#xb3c4;&#xb85c; &#xc9c4;&#xbcf4;&#xb418;&#xc5b4;&#xc788;&#xace0;, &#xc774;&#xb4e4;&#xacfc; &#xc2f8;&#xc6b0;&#xb294; &#xac83;&#xc740; &#xd30c;&#xba78;&#xb9cc;&#xc744; &#xbd88;&#xb7ec;&#xc62c; &#xbfd0;&#xc774;&#xb2c8; &#xbb34;&#xc870;&#xac74; &#xd56d;&#xbcf5;&#xd558;&#xb77c;&#xb294; &#xb0b4;&#xc6a9;&#xc774;&#xb2e4;. &#xd06c;&#xb9ac;&#xc2a4;&#xd0c8; &#xc82c;&#xc2a4;&#xb294; &#xc9c0;&#xad6c;&#xb85c; &#xb2e4;&#xac00;&#xc624;&#xace0; &#xc788;&#xb294; &#xba85;&#xd655;&#xd55c; &#xc704;&#xd611;&#xc5d0; &#xacf5;&#xd3ec;&#xb97c; &#xb290;&#xb080;&#xb2e4;.</p><p>&#xa0;<br></p><p>&#xb77c;&#xd53c;&#xc2a4;&#xb294; &#xc9c0;&#xad6c;&#xc5d0; &#xb300;&#xd55c; &#xc815;&#xbcf4;&#xb97c; &#xac00;&#xc9c0;&#xace0; &#xc788;&#xb294; &#xc82c;&#xc73c;&#xb85c;&#xc11c; &#xd648;&#xc6d4;&#xb4dc; &#xc82c;&#xb4e4;&#xc5d0; &#xc758;&#xd574; &#xb2e4;&#xc2dc; &#xc9c0;&#xad6c;&#xb85c; &#xb04c;&#xb824;&#xc628;&#xb2e4;. &#xc2a4;&#xd2f0;&#xbe10;&#xc774; &#xd729;&#xc4f8;&#xb9ac;&#xc9c0; &#xc54a;&#xae30;&#xb97c; &#xbc14;&#xb77c;&#xb294; &#xb9c8;&#xc74c;&#xc5d0; &#xd56d;&#xc758;&#xd558;&#xc9c0;&#xb9cc;, &#xacb0;&#xad6d; &#xc804;&#xbd80; &#xc7a1;&#xd600; &#xd648;&#xc6d4;&#xb4dc;&#xb85c; &#xac15;&#xc81c; &#xc774;&#xc1a1;&#xb418;&#xac8c; &#xb41c;&#xb2e4;. &#xc2a4;&#xd2f0;&#xbe10;&#xc740; &#xc6b0;&#xc8fc;&#xc120; &#xb0b4;&#xc5d0;&#xc11c; &#xb77c;&#xd53c;&#xc2a4;&#xc640; &#xb9cc;&#xb098; &#xadf8;&#xb140;&#xb97c; &#xad6c;&#xcd9c;&#xd558;&#xb824; &#xd558;&#xc9c0;&#xb9cc;, &#xb77c;&#