	"postContents": "<div class=\"col-md-12 col-sm-12\">\r\n\t\t\t\t<h2>&#xd30c;&#xc6cc;&#xb4dc;&#xb808;&#xc2f1;</h2><p>1980&#xb144;&#xb300; &#xc5ec;&#xc131;&#xb4e4;&#xc758; &#xc0ac;&#xd68c; &#xc9c4;&#xcd9c;&#xc774; &#xc99d;&#xac00;&#xd558;&#xba74;&#xc11c; &#xc720;&#xd589;&#xd588;&#xb358; &#xd328;&#xc158;&#xd2b8;&#xb80c;&#xb4dc;.&#xa0;&#xc0c1;&#xce35;&#xbd80; &#xb0a8;&#xc131;&#xb4e4;&#xc758; &#xc815;&#xc7a5;&#xc744; &#xc801;&#xadf9;&#xc801;&#xc73c;&#xb85c; &#xbaa8;&#xbc29;&#xd55c; &#xc774; &#xc2a4;&#xd0c0;&#xc77c;&#xc740;, &#xc9c0;&#xae08;&#xae4c;&#xc9c0;&#xb3c4; &#xb9ce;&#xc740; &#xc5ec;&#xc131;&#xb4e4;&#xc774; &apos;&#xacf5;&#xc801;&#xc778; &#xc11d;&#xc0c1;&apos;&#xc5d0;&#xc11c; &#xcd94;&#xad6c;&#xd558;&#xace0; &#xc788;&#xc73c;&#xba70;, &apos;&#xc131;&#xacf5;&#xd55c; &#xc5ec;&#xc131;&#xc758; &#xc637;&#xcc28;&#xb9bc;&apos;&#xc73c;&#xb85c; &#xc18c;&#xac1c;&#xb41c;&#xb2e4;.&#xa0;<br><br></p><h4>&#xc758;&#xc758;</h4><p>1980&#xb144;&#xb300; &#xc5ec;&#xc131;&#xb4e4;&#xc774; &#xc9c1;&#xc7a5;&#xc5d0;&#xc11c;&#xc758; &#xc131;&#xacf5;&#xacfc; &#xad8c;&#xb825;&#xc5d0; &#xb300;&#xd55c; &#xc695;&#xb9dd;&#xc744; &#xbcf4;&#xc5ec;&#xc900;&#xb2e4;. &#xc5ec;&#xc131;&#xc801;&#xc73c;&#xb85c; &#xc139;&#xc288;&#xc5bc;&#xb9ac;&#xd2f0;&#xb97c; &#xac15;&#xc870;&#xd558;&#xae30;&#xb97c; &#xac15;&#xc694;&#xbc1b;&#xb294; &#xc758;&#xbcf5;&#xc744; &#xac70;&#xbd80;&#xd568;&#xc73c;&#xb85c;&#xc368; &#xac00;&#xbd80;&#xc7a5;&#xc81c;&#xc758; &#xc65c;&#xace1;&#xb41c; &#xc5ec;&#xc131; &#xc815;&#xccb4;&#xc131;&#xc744; &#xac70;&#xbd80;&#xd588;&#xb2e4;.<br> &#xa0;</p><h4>&#xd55c;&#xacc4;</h4><blockquote><p>&#xa0; &#xc11c;&#xc591; &#xc815;&#xc7a5;&#xc758; &#xc6d0;&#xce59;&#xc740; &#xb0a8;&#xc131;&#xb4e4;&#xc740; &#xac00;&#xb2a5;&#xd55c; &#xd55c; &#xc637;&#xc73c;&#xb85c; &#xc628;&#xbab8;&#xc744; &#xac00;&#xb9ac;&#xace0; &#xc5ec;&#xc131;&#xb4e4;&#xc740; &#xac00;&#xb2a5;&#xd55c; &#xd55c; &#xc2e0;&#xccb4;&#xb97c; &#xb178;&#xcd9c;&#xd558;&#xb294; &#xac83;&#xc774;&#xb2e4;.&#xa0;(...) &#xadf8;&#xac83;&#xc740; &#xc5ec;&#xc131;&#xb4e4;&#xc774; &#xc0ac;&#xc801;&#xc778; &#xc874;&#xc7ac;&#xb85c;&#xc11c; &#xc624;&#xc9c1; &#xb0a8;&#xc131;&#xb4e4;&#xc758; &#xcf8c;&#xb77d;&#xc744; &#xc704;&#xd574; &apos;&#xaf43;&#xcc98;&#xb7fc; &#xc874;&#xc7ac;&#xd588;&#xb2e4;&apos;&#xb294; &#xc0ac;&#xc2e4;&#xc744; &#xb2e4;&#xc2dc; &#xd55c;&#xbc88; &#xc99d;&#xba85;&#xd558;&#xb294; &#xac83;&#xc774; &#xb41c;&#xb2e4;. &#xadf8;&#xb7ec;&#xb098; &#xc774;&#xc640; &#xb367;&#xbd99;&#xc5ec; &#xbc18;&#xb4dc;&#xc2dc; &#xae30;&#xc5b5;&#xd574;&#xc57c; &#xd560; &#xc0ac;&#xc2e4;&#xc740; &#xc18c;&#xc218;&#xc758; &#xc0c1;&#xb958;&#xce35; &#xc5ec;&#xc131;&#xc774; &#xc544;&#xb2cc; &#xd55c;, &#xb300;&#xbd80;&#xbd84;&#xc758; &#xc5ec;&#xc131;&#xb4e4;&#xc774; &#xb04a;&#xc784;&#xc5c6;&#xc774; &#xc804;&#xadfc;&#xb300;&#xc801;&#xc778; &#xb178;&#xb3d9; &#xd639;&#xc740; &#xc131;&#xbcc4; &#xbd84;&#xc5c5; &#xb178;&#xb3d9;&#xc5d0; &#xc2dc;&#xb2ec;&#xb838;&#xb2e4;&#xb294; &#xac83;&#xc774;&#xb2e4;. &#xadf8;&#xb7f0;&#xb370; &#xacf5;&#xc801; &#xc601;&#xc5ed;&#xc5d0; &#xc9c4;&#xcd9c;&#xd55c; &#xd604;&#xb300; &#xc5ec;&#xc131;&#xc758; &#xc815;&#xc7a5;&#xc774; &#xc804;&#xd1b5;&#xc801;&#xc778; &#xb178;&#xb3d9; &#xacc4;&#xae09; &#xc5ec;&#xc131;&#xb4e4;&#xc758; &#xb178;&#xb3d9;&#xbcf5;&#xc774; &#xc544;&#xb2c8;&#xb77c; &#xb0a8;&#xc131;&#xb4e4;&#xc758; &#xc288;&#xd2b8;&#xb97c; &#xbcf8;&#xb530; &#xb9cc;&#xb4e4;&#xc5b4;&#xc84c;&#xb2e4;&#xb294; &#xac83;&#xc740; &#xace0;&#xc704;&#xc9c1;, &#xc804;&#xbb38;&#xc9c1; &#xc5ec;&#xc131;&#xc774; &#xc2a4;&#xc2a4;&#xb85c; &#xd0c8;&#xc131;&#xbcc4;&#xd654;&#xd568;&#xc73c;&#xb85c;&#xc368; &#xb0a8;&#xc131;&#xb4e4;&#xc758; &#xad8c;&#xb825;&#xc744; &#xcc28;&#xc6a9;&#xd558;&#xb294; &#xd55c;&#xd3b8;, &#xc804;&#xc5c5; &#xc8fc;&#xbd80;&#xc640; &#xc800;&#xc18c;&#xb4dd; &#xb178;&#xb3d9; &#xc5ec;&#xc131; &#xbaa8;&#xb450;&#xb97c; &#xc790;&#xc2e0;&#xacfc; &#xb2e8;&#xc808;&#xd558;&#xb824;&#xb294; &#xc758;&#xb3c4;&#xac00; &#xac15;&#xd558;&#xac8c; &#xb2f4;&#xaca8; &#xc788;&#xb2e4;&#xace0; &#xbcfc; &#xc218; &#xc788;&#xb2e4;.<br></p><p>&#xa0; &#xc5ec;&#xc131; &#xc815;&#xc7a5;&#xc778; &#xb4dc;&#xb808;&#xc2a4;&#xc758; &#xc9c0;&#xb098;&#xce5c; &#xb178;&#xcd9c;&#xacfc; &#xbd88;&#xd3b8;&#xd568;&#xc774; &#xc9c1;&#xc7a5; &#xc0dd;&#xd65c;&#xc5d0; &#xc801;&#xd569;&#xd558;&#xc9c0; &#xc54a;&#xb2e4;&#xba74; &#xc65c; &#xc5ec;&#xc131;&#xc758; &#xb178;&#xb3d9; &#xc804;&#xd1b5;&#xc744; &#xc804;&#xc720;&#xd558;&#xba74;&#xc11c;&#xb3c4; &#xc5ec;&#xc131;&#xc8fc;&#xc758;&#xc801; &#xc0c1;&#xc0c1;&#xb825;&#xc744; &#xbc1c;&#xd718;&#xd55c; &#xc0c8;&#xb85c;&#xc6b4; &#xae30;&#xb2a5;&#xc131; &#xb178;&#xb3d9;&#xbcf5;&#xc744; &#xace0;&#xc548;&#xd558;&#xc9c0; &#xbabb;&#xd588;&#xc744;&#xae4c;?</p><p style=\"text-align: right;\" data-mce-style=\"text-align: right;\"><span style=\"font-size: 8pt;\" data-mce-style=\"font-size: 8pt;\">&#xae40;&#xc8fc;&#xd604;, &lt;&#xc678;&#xbaa8; &#xafb8;&#xbbf8;&#xae30; &#xbbf8;&#xd559;&#xacfc; &#xd398;&#xbbf8;&#xb2c8;&#xc998;&gt; 135-136p &#xc774;&#xc131;&#xd654;&#xc640; &#xb0a8;&#xc131;&#xd654;, 2009.</span></p></blockquote><blockquote><p>&#xd30c;&#xc6cc;&#xb4dc;&#xb808;&#xc2f1;&#xc5d0; &#xad00;&#xd55c; &#xc9c0;&#xce68;&#xc11c;&#xb4e4;&#xc740; &#xc804;&#xbb38;&#xc9c1; &#xc5ec;&#xc131;&#xb4e4;&#xc774; &#xc5ec;&#xc131;&#xc758; &#xc139;&#xc288;&#xc5bc;&#xb9ac;&#xd2f0;&#xad00;&#xd55c; &#xb0a8;&#xc131;&#xc758; &#xc751;&#xc2dc;&#xc5d0; &#xc9c1;&#xba74; &#xd558;&#xc600;&#xc744; &#xb54c; &#xc9c0;&#xcf1c;&#xc57c; &#xd560; &#xbab8;&#xac00;&#xc9d0;&#xacfc; &#xc790;&#xae30;&#xd45c;&#xd604;&#xc5d0; &#xad00;&#xd55c; &#xba87; &#xac00;&#xc9c0; &#xaddc;&#xce59;&#xc744; &#xc124;&#xba85;&#xd55c;&#xb2e4;. &#xadf8;&#xb7ec;&#xb098; &#xc774;&#xb7ec;&#xd55c; &#xc694;&#xad6c;&#xb294; &#xd328;&#xc158;&#xc5d0;&#xc11c; &#xc5ec;&#xc131;&#xc774; &#xb0a8;&#xc131;&#xcc98;&#xb7fc; &#xbc14;&#xc9c0;&#xb97c; &#xcc29;&#xc6a9;&#xd558;&#xae30;&#xbcf4;&#xb2e4;&#xb294; &#xc2a4;&#xcee4;&#xd2b8;&#xb97c; &#xc785;&#xc74c;&#xc73c;&#xb85c;&#xc368; &#xb0a8;&#xc131;&#xc5d0;&#xac8c; &#xb35c; &#xc704;&#xd611;&#xc801;&#xc778; &#xc874;&#xc7ac;&#xb85c; &#xac04;&#xc8fc;&#xb428;&#xc744; &#xc9c0;&#xc801;&#xd55c;&#xb2e4;. &#xc774;&#xb7ec;&#xd55c; &#xbc29;&#xc2dd;&#xc73c;&#xb85c; &#xd30c;&#xc6cc;&#xb4dc;&#xb808;&#xc2f1;&#xc740; &#xc5ec;&#xc131;&#xc758; &#xbab8;&#xc758; &#xc7a0;&#xc7ac;&#xc801; &#xc5d0;&#xb85c;&#xd2f0;&#xc2dc;&#xc998;&#xc744; &#xcd95;&#xc18c;&#xc2dc;&#xd0a4;&#xb824;&#xb294; &#xc695;&#xad6c;&#xc640; &#xc77c;&#xc758; &#xc138;&#xacc4;&#xc5d0;&#xc11c; &#xc5ec;&#xc131;&#xc131;&#xc744; &#xc9c0;&#xc18d;&#xc2dc;&#xd0a4;&#xb824;&#xb294; &#xc695;&#xad6c;&#xc758; &#xade0;&#xd615;&#xc774;&#xb2e4;. (...) &#xadf8;&#xb7ec;&#xb098; Entwistle&#xc740; &#xd30c;&#xc6cc;&#xb4dc;&#xb808;&#xc2f1; &#xb2f4;&#xb860;&#xc774; &#xc5ec;&#xc804;&#xd788; &#xc5ec;&#xc131;&#xc744; &#xc139;&#xc288;&#xc5bc;&#xb9ac;&#xd2f0;&#xc640; &#xc870;&#xd569;&#xc2dc;&#xd0a4;&#xb294; &#xad6c;&#xc2dc;&#xb300;&#xc801; &#xb2f4;&#xb860;&#xc744; &#xc9c0;&#xc18d;&#xd558;&#xace0; &#xc788;&#xc74c;&#xc744; &#xc9c0;&#xc801;&#xd55c;&#xb2e4;. &#xc989; &#xd30c;&#xc6cc;&#xb4dc;&#xb808;&#xc2f1;&#xc740; &#xc5ec;&#xc131;&#xc758; &#xbab8;&#xacfc; &#xc139;&#xc288;&#xc5bc;&#xb9ac;&#xd2f0;&#xc758; &#xacb0;&#xd569; &#xb54c;&#xbb38;&#xc5d0; &#xc5ec;&#xc804;&#xd788; &#xb0a8;&#xc131;&#xbcf4;&#xb2e4; &#xc5ec;&#xc131;&#xc758; &#xbcf5;&#xc2dd;&#xc774; &#xbcf4;&#xb2e4; &#xc8fc;&#xc758;&#xae4a;&#xac8c; &#xd1b5;&#xc81c;&#xb418;&#xc5b4;&#xc57c; &#xd55c;&#xb2e4;&#xb294; &#xc7a0;&#xc7ac;&#xc801; &#xbd80;&#xb2f4;&#xac10;&#xc744; &#xc554;&#xc2dc;&#xd568;&#xc73c;&#xb85c;&#xc368; &#xb610; &#xb2e4;&#xb978; &#xd615;&#xc2dd;&#xc73c;&#xb85c; &#xccb4;&#xacc4;&#xb97c; &#xc7ac;&#xc0dd;&#xc0b0;&#xc744; &#xbc18;&#xbcf5;&#xd558;&#xace0; &#xc788;&#xb2e4;&#xb294; &#xac83;&#xc774;&#xb2e4;.</p><p style=\"text-align: right;\" data-mce-style=\"text-align: right;\"><span style=\"font-size: 8pt;\" data-mce-style=\"font-size: 8pt;\"><a href=\"https://books.google.co.kr/books?id=1EKsAwAAQBAJ&amp;pg=PA84&amp;lpg=PA84&amp;dq=%ED%8C%8C%EC%9B%8C%EB%93%9C%EB%A0%88%EC%8B%B1&amp;source=bl&amp;ots=UAiz4An