&#xac10;&#xc815;&#xc5d0; &#xd729;&#xc4f8;&#xb824; &#xb9d0;&#xc744; &#xbc49;&#xb294; &#xbd80;&#xbd84;&#xb3c4; &#xc788;&#xc5b4; &#xbcf4;&#xc778;&#xb2e4;.</p><p>&#xa0;</p><p>1&#xc138;&#xb300;&#xbcf4;&#xb2e4; &#xb2e4;&#xc6b4;&#xadf8;&#xb808;&#xc774;&#xb4dc; &#xb41c; 2&#xc138;&#xb300;&#xc758; &#xc601;&#xd5a5;&#xc778;&#xc9c0;, &#xb2e4;&#xc774;&#xc544;&#xbaac;&#xb4dc;&#xc758; &#xc758;&#xc790;&#xc5d0; &#xc62c;&#xb77c;&#xac00; &#xc88b;&#xc544;&#xd558;&#xac70;&#xb098; &#xc790;&#xc2e0;&#xc774; &#xd648;&#xc6d4;&#xb4dc;&#xc758; &#xac70;&#xbb3c;&#xc774;&#xb77c;&#xace0; &#xce6d;&#xd558;&#xb294; &#xb4f1; &#xc5d8;&#xb9ac;&#xd2b8;&#xc8fc;&#xc758;&#xc5d0; &#xb300;&#xd55c; &#xc695;&#xb9dd;&#xc774; &#xbcf4;&#xc778;&#xb2e4;.</p><p><br></p><h2>&#xc791;&#xc911; &#xd589;&#xbcf4;</h2><p>&#xd398;&#xb9ac;&#xb3c4;&#xd2b8;&#xb294; &#xd648;&#xc6d4;&#xb4dc;&#xc758; 2&#xc138;&#xb300;&#xb85c; &#xd0dc;&#xc5b4;&#xb0ac;&#xb2e4;. &#xc5b4;&#xb5a4; &#xc774;&#xc720;&#xb85c; &#xd648;&#xc6d4;&#xb4dc;&#xc758; &#xd074;&#xb7ec;&#xc2a4;&#xd130; &#xd504;&#xb85c;&#xc81d;&#xd2b8;&#xc5d0; &#xc9c1;&#xc811; &#xcc38;&#xc5ec;&#xd558;&#xc9c0;&#xb294; &#xbabb;&#xd588;&#xc9c0;&#xb9cc;, &#xc2a4;&#xc2a4;&#xb85c; &#xc218;&#xbc31;&#xb144;&#xce58;&#xc758; &#xbcf4;&#xace0;&#xc11c;&#xb97c; &#xc77d;&#xc5c8;&#xc73c;&#xba70; &#xc774;&#xd6c4; &#xc9c0;&#xad6c;&#xc758; &#xd074;&#xb7ec;&#xc2a4;&#xd130; &#xbc30;&#xc591; &#xd604;&#xd669;&#xc744; &#xd655;&#xc778;&#xd558;&#xb294; &#xc784;&#xbb34;&#xc5d0; &#xbc30;&#xc815;&#xbc1b;&#xac8c; &#xb41c;&#xb2e4;.</p><p>&#xa0;</p><p>&#xcd5c;&#xcd08; &#xb808;&#xb4dc;&#xc544;&#xc774;&#xb97c; &#xc9c0;&#xad6c;&#xb85c; &#xbcf4;&#xb0b4;(ep2) &#xc815;&#xcc30;&#xc744; &#xc2dc;&#xb3c4;&#xd558;&#xc9c0;&#xb9cc; &#xd06c;&#xb9ac;&#xc2a4;&#xd0c8; &#xc82c;&#xc2a4;&#xc5d0;&#xac8c; &#xd30c;&#xad34;&#xb418;&#xace0;, &#xc791;&#xc740; &#xb098;&#xb178; &#xb85c;&#xbd07;&#xc744; &#xc218;&#xc2ed;&#xb300; &#xc9c0;&#xad6c;&#xb85c; &#xbcf4;&#xb0b4; &#xac24;&#xb7ed;&#xc2dc; &#xc6cc;&#xd504; &#xd328;&#xb4dc;&#xb97c; &#xc218;&#xb9ac;&#xd55c; &#xd6c4;&#xc5d0;&#xc57c; &#xc9c0;&#xad6c;&#xb85c; &#xc62c; &#xc218; &#xc788;&#xc5c8;&#xb2e4;. &#xadf8;&#xb7ec;&#xb098; &#xc2a4;&#xd2f0;&#xbe10;&#xc774; &#xc6cc;&#xd504; &#xd328;&#xb4dc;&#xc5d0; &#xbd99;&#xc5ec;&#xb193;&#xc740; &#xc544;&#xce68;&#xc2dd;&#xc0ac; &#xc2a4;&#xd2f0;&#xcee4;&#xb97c; &#xbc1c;&#xacac;&#xd558;&#xace0;, &apos;&#xc774; &#xc7a5;&#xc18c;&#xac00; &#xbc1c;&#xac01;&#xb410;&#xc744; &#xc218; &#xc788;&#xaca0;&#xb2e4;&apos;&#xba70; &#xbe60;&#xb974;&#xac8c; &#xc0ac;&#xb77c;&#xc9c4;&#xb2e4;.</p><p>&#xa0;</p><p>&#xc774;&#xd6c4; &#xac70;&#xb300;&#xd55c; &#xb85c;&#xbd07;&#xc744; &#xc9c0;&#xc18d;&#xc801;&#xc73c;&#xb85c; &#xc9c0;&#xad6c;&#xb85c; &#xbcf4;&#xb0b8;&#xb2e4;. &#xd06c;&#xb9ac;&#xc2a4;&#xd0c8; &#xc82c;&#xc2a4;&#xac00; &#xbc88;&#xbc88;&#xd788; &#xd30c;&#xad34;&#xd558;&#xc9c0;&#xb9cc; &#xb05d;&#xb3c4; &#xc5c6;&#xc774; &#xbcf4;&#xb0b8; &#xb05d;&#xc5d0; &#xb4dc;&#xb514;&#xc5b4; &#xb85c;&#xbd07;&#xc774; &#xc720;&#xce58;&#xc6d0; &#xb0b4; &#xd074;&#xb7ec;&#xc2a4;&#xd130; &#xd504;&#xb85c;&#xd1a0;&#xd0c0;&#xc785; &#xad00;&#xb9ac;&#xc2e4;&#xc5d0; &#xb3c4;&#xcc29;&#xd558;&#xace0;, &#xc6d0;&#xaca9; &#xc870;&#xc885;&#xc744; &#xd1b5;&#xd574; &#xc720;&#xce58;&#xc6d0;&#xacfc; &#xd074;&#xb7ec;&#xc2a4;&#xd130;&#xc758; &#xc0c1;&#xd0dc;&#xb97c; &#xd655;&#xc778;&#xd55c;&#xb2e4;.</p><p>&#xc774; &#xb54c; &#xc2a4;&#xd2f0;&#xbe10;&#xc774; &#xc790;&#xc2e0;&#xc758; &#xbaa8;&#xc2b5;&#xc744; &#xb4dc;&#xb7ec;&#xb0b4;&#xba70; &#xd398;&#xb9ac;&#xb3c4;&#xd2b8;&#xc758; &#xbaa9;&#xc801;&#xc744; &#xbb3b;&#xc9c0;&#xb9cc;, &#xd398;&#xb9ac;&#xb3c4;&#xd2b8;&#xb294; &apos;&#xc2a4;&#xd2f0;&#xbe10;&apos;&#xc774;&#xb77c;&#xb294; &#xc885;&#xc871;&#xc758; &#xbd84;&#xd3ec;&#xb97c; &#xbb3b;&#xb294; &#xb4f1; &#xc778;&#xac04;&#xc5d0; &#xb300;&#xd574; &#xbaa8;&#xb978;&#xb2e4;&#xb294; &#xd589;&#xb3d9;&#xc744; &#xd558;&#xb2e4;, &#xb4a4;&#xc774;&#xc5b4; &#xb098;&#xd0c0;&#xb09c; &#xd06c;&#xb9ac;&#xc2a4;&#xd0c8; &#xc82c;&#xc2a4;&#xc5d0; &#xae5c;&#xc9dd; &#xb180;&#xb780;&#xb2e4;. &#xc790;&#xae3