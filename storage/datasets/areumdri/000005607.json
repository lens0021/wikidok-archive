	"postContents": "<div class=\"col-md-12 col-sm-12\">\r\n\t\t\t\t<p><img width=\"612\" height=\"400\" src=\"/api/File/Real/57bec3f006441c2264e8afe7\" data-mce-src=\"/api/File/Real/57bec3f006441c2264e8afe7\"></p><h2>1.&#xac1c;&#xc694;</h2><p><strong>&#xc9c4;&#xc8fc;(Pearl)</strong>&#xb294; &#xd0c4;&#xc0b0;&#xce7c;&#xc298;&#xc774; &#xc8fc;&#xc131;&#xbd84;&#xc774;&#xba70;, &#xc6b0;&#xc544;&#xd558;&#xace0; &#xc544;&#xb984;&#xb2e4;&#xc6b4; &#xbe5b;&#xae54;&#xc758; &#xad11;&#xd0dd;&#xc774; &#xb098;&#xb294;&#xa0;&#xbcf4;&#xc11d;&#xc758; &#xc77c;&#xc885;&#xc774;&#xb2e4;.&#xa0;&#xc9c4;&#xc8fc;&#xb294; &#xd06c;&#xae30;, &#xbaa8;&#xc591;, &#xc0c9;&#xae54;, &#xad11;&#xd0dd;, &#xd45c;&#xba74;&#xc758; &#xd488;&#xc9c8;, &#xc9c4;&#xc8fc;&#xce35;&#xc758; &#xd488;&#xc9c8; &#xb4f1; &#xb2e4;&#xc591;&#xd55c; &#xc694;&#xc18c;&#xc5d0; &#xb530;&#xb77c; &#xadf8; &#xac00;&#xce58;&#xac00; &#xacb0;&#xc815;&#xb41c;&#xb2e4;.&#xa0;&#xd06c;&#xac8c; &#xcc9c;&#xc5f0; &#xc9c4;&#xc8fc;&#xc640; &#xc591;&#xc2dd; &#xc9c4;&#xc8fc;&#xb85c; &#xb098;&#xb258;&#xba70; &#xcc9c;&#xc5f0; &#xc9c4;&#xc8fc;&#xb294; &#xadf8; &#xc591;&#xc774; &#xac70;&#xc758; &#xc5c6;&#xb2e4;&#xc2dc;&#xd53c; &#xd55c;&#xb2e4;.</p><p><br data-mce-bogus=\"1\"></p><p>&#xc9c4;&#xc8fc;&#xc758;&#xa0;&#xc8fc; &#xc0dd;&#xc0b0;&#xc9c0;&#xb294; &#xb2e4;&#xc74c;&#xacfc; &#xac19;&#xb2e4;.<br></p><p>&#xa0;<br></p><p><img src=\"/api/File/Real/57bec1df06441c2264e8af6e\" data-mce-src=\"/api/File/Real/57bec1df06441c2264e8af6e\"></p><p>&#xa0;&#xc9c4;&#xc8fc;&#xc758; &#xc8fc; &#xc0dd;&#xc0b0;&#xc9c0;&#xa0;(&#xcd9c;&#xcc98; : <a href=\"http://www.sustainablepearls.org/pearl-farming/pearl-farming-world-map\" wk-external=\"true\" data-mce-href=\"http://www.sustainablepearls.org/pearl-farming/pearl-farming-world-map\">http://www.sustainablepearls.org/pearl-farming/pearl-farming-world-map</a><a href=\"http://www.sustainablepearls.org/pearl-farming/pearl-farming-world-map/\" data-mce-href=\"http://www.sustainablepearls.org/pearl-farming/pearl-farming-world-map/\">/</a>)</p><h2>2. &#xc9c4;&#xc8fc;&#xc758; &#xc885;&#xb958;</h2><h3>2.1 &#xcc9c;&#xc5f0; &#xc9c4;&#xc8fc;</h3><p>&#xc870;&#xac1c;&#xc5d0; &#xc790;&#xc5f0;&#xc801;&#xc73c;&#xb85c; &#xc0dd;&#xae34; &#xc9c4;&#xc8fc;&#xb97c; &#xb9d0;&#xd55c;&#xb2e4;.&#xa0;&#xcc9c;&#xc5f0; &#xc9c4;&#xc8fc;&#xb294; &#xc0dd;&#xaca8;&#xb098;&#xb294; &#xbd80;&#xc704;&#xc5d0; &#xb530;&#xb77c; &#xc870;&#xac00;&#xbe44;&#xc5d0;&#xc11c; &#xc720;&#xb9ac;&#xb41c; &apos;&#xc720;&#xb9ac; &#xc9c4;&#xc8fc;(loose pearl)&apos;&#xc640; &#xc870;&#xac00;&#xbe44;&#xc5d0; &#xbd99;&#xc5b4;&#xc788;&#xb294; &apos;&#xbc18;&#xad6c;&#xc0c1;&#xc758; &#xbd80;&#xcc29; &#xc9c4;&#xc8fc;(half pearl)&apos;&#xac00; &#xc788;&#xb2e4;.&#xa0;&#xc774; &#xc911; &#xc720;&#xb9ac; &#xc9c4;&#xc8fc;&#xac00; &#xbcf4;&#xc11d;&#xc73c;&#xb85c;&#xc758; &#xac00;&#xce58;&#xac00; &#xc788;&#xb2e4;.&#xa0;&#xd398;&#xb974;&#xc2dc;&#xc544; &#xb9cc; &#xb4f1;&#xc5d0;&#xc11c; &#xcc9c;&#xc5f0; &#xc9c4;&#xc8fc;&#xac00; &#xcc44;&#xcde8;&#xb418;&#xc5c8;&#xc73c;&#xb098; &#xd574;&#xc591; &#xc624;&#xc5fc; &#xbc0f; &#xb0a8;&#xd68d; &#xb4f1;&#xc5d0; &#xc758;&#xd574; &#xc0dd;&#xc0b0;&#xc774; &#xae09;&#xac10;&#xd588;&#xb2e4;. &#xb2e4;&#xb978; &#xbd84;&#xb958;&#xb85c;&#xb294; &#xbc14;&#xb2e4;&#xc5d0;&#xc11c; &#xb098;&#xb294; &apos;&#xd574;&#xc218; &#xc9c4;&#xc8fc;&apos;&#xc640; &#xd638;&#xc218;&#xb098; &#xac15; &#xac19;&#xc740; &#xbbfc;&#xbb3c;&#xc5d0;&#xc11c; &#xb098;&#xb294; &apos;&#xb2f4;&#xc218; &#xc9c4;&#xc8fc;&apos;&#xb85c; &#xb098;&#xb258;&#xae30;&#xb3c4; &#xd55c;&#xb2e4;.</p><h3>2.2. &#xc591;&#xc2dd; &#xc9c4;&#xc8fc;</h3><p>&#xcc9c;&#xc5f0; &#xc9c4;&#xc8fc;&#xb294; &#xc6b0;&#xc5f0;&#xd788; &#xd615;&#xc131;&#xb418;&#xb294; &#xb370; &#xbc18;&#xd558;&#xc5ec; &#xc591;&#xc2dd; &#xc9c4;&#xc8fc;&#xb294; &#xc9c4;&#xc8fc;&#xc758; &#xbaa8;&#xccb4;&#xc778; &#xc870;&#xac1c;&#xb97c; &#xc778;&#xc704;&#xc801;&#xc73c;&#xb85c; &#xc591;&#xc2dd;&#xd558;&#xc5ec; &#xb9cc;&#xb4e4;&#xc5b4;&#xc9c0;&#xb3c4;&#xb85d; &#xc720;&#xb3c4;&#xd55c; &#xac83;&#xc774;&#xb2e4;.&#xa0;&#xc591;&#xc2dd;&#xd558;&#xae30;&#xc5d0; &#xc801;&#xd569;&#xd55c; &#xc9c4;&#xc8fc; &#xc870;&#xac1c;(Pinctada fucata)&#xc5d0; &#xc870;&#xac1c; &#xb4f1;&#xc758; &#xaecd;&#xb370;&#xae30;&#xb97c; &#xad6c;&#xc2ac; &#xbaa8;&#xc591;&#xc73c;&#xb85c; &#xb2e4;&#xb4ec;&#xc5b4; &#xc778;&#xacf5;&#xd575;&#xc73c;&#xb85c; &#xc0bd;&#xc785;&#xd55c;&#xb2e4;. &#xc0bd;&#xc785; &#xd6c4; 1~3&#xb144;&#xac04; &#xc591;&#xc2dd;&#xd558;&#xc5ec; &#xc9c4;&#xc8fc;&#xb97c; &#xcc44;&#xcde8;&#xd55c;&#xb2e4;.&#xa0;&#xd06c;&#xae30;&#xb294; 3~10mm &#xc815;&#xb3c4;&#xc774;&#xba70; &#xb300;&#xbd80;&#xbd84;&#xc740; &#xad6c;&#xc2ac; &#xbaa8;&#xc591;&#xc774;&#xc9c0;&#xb9cc; &#xbaa8;&#xc591;&#xc774; &#xace0;&#xb974;&#xc9c0; &#xc54a;&#xc740; &#xac83;&#xb3c4; &#xc0dd;&#xae34;&#xb2e4;.</p><h4>2.2.1. &#xc544;&#xcf54;&#xc57c; &#xc9c4;&#xc8fc;</h4><p>&#xb530;&#xb73b;&#xd558;&#xace0; &#xc595;&#xc740; &#xc77c;&#xbcf8; &#xadfc;&#xd574;&#xc5d0;&#xc11c; &#xc11c;&#xc2dd;&#xd558;&#xb294; &#xc544;&#xcf54;&#xc57c; &#xd574;&#xc218; &#xc870;&#xac1c;&#xc5d0;&#xc11c; &#xc0dd;&#xae30;&#xb294; &#xc9c4;&#xc8fc;&#xb97c; &#xb9d0;&#xd55c;&#xb2e4;. &#xc8fc;&#xb85c; &#xc77c;&#xbcf8;&#xacfc; &#xc911;&#xad6d;&#xc5d0;&#xc11c; &#xc0dd;&#xc0b0;&#xb41c;&#xb2e4;. &#xc870;&#xac1c;&#xc758; &#xd06c;&#xae30;&#xac00; &#xc791;&#xc544;&#xc11c; 10mm &#xc774;&#xd558; &#xd06c;&#xae30;&#xc758; &#xc9c4;&#xc8fc;&#xb97c; &#xc0dd;&#xc0b0;&#xd574;&#xb0b8;&#xb2e4;.&#xa0;&#xc544;&#xcf54;&#xc57c; &#xc870;&#xac1c;&#xb294; &#xc77c;&#xbcf8;, &#xc911;&#xad6d;, &#xd55c;&#xad6d;(&#xd1b5;&#xc601;) &#xb4f1;&#xc5d0;&#xc11c; &#xc11c;&#xc2dd;&#xc774; &#xac00;&#xb2a5;&#xd558;&#xb2e4;.&#xa0;&#xd551;&#xd06c;, &#xd654;&#xc774;&#xd2b8;, &#xadf8;&#xb9b0;, &#xace8;&#xb4dc;, &#xbe14;&#xb8e8;, &#xd06c;&#xb9bc; &#xb4f1;&#xc758; &#xc0c9;&#xc744; &#xc9c0;&#xb2cc; &#xc9c4;&#xc8fc;&#xb97c; &#xc0dd;&#xc0b0;&#xd55c;&#xb2e4;.</p><h4>2.2.2. &#xb0a8;&#xc591; &#xc9c4;&#xc8fc;</h4><p>&#xc778;&#xb3c4;&#xb124;&#xc2dc;&#xc544;, &#xd544;&#xb9ac;&#xd540;, &#xd638;&#xc8fc;&#xc5d0;&#xc11c; &#xc591;&#xc2dd;&#xb418;&#xb294; &apos;&#xd574;&#xc218; &#xc870;&#xac1c;&apos;&#xc5d0;&#xc11c; &#xc0dd;&#xc0b0;&#xb41c; &#xc9c4;&#xc8fc;&#xb2e4;.&#xa0;&#xc724;&#xae30;&#xc788;&#xace0; &#xbd80;&#xb4dc;&#xb7ec;&#xc6b4; &#xad11;&#xd0dd;&#xc744; &#xac00;&#xc9c0;&#xace0; &#xc788;&#xb2e4;.</p><h4>2.2.3. &#xd751;&#xc9c4;&#xc8fc;</h4><p>&#xbe14;&#xb799;&#xc774;&#xb098; &#xc2e4;&#xbc84;&#xd1a4;&#xc758; &#xcc44;&#xc0c9;&#xc744; &#xac00;&#xc9c0;&#xace0; &#xc788;&#xb294; &#xd574;&#xc218; &#xc9c4;&#xc8fc;&#xb97c; &#xb73b;&#xd55c;&#xb2e4;.&#xa0;&#xb0a8;&#xd0dc;&#xd3c9;&#xc591;&#xc758; &#xd0c0;&#xd788;&#xd2f0; &#xc9c0;&#xc5ed;&#xc5d0;&#xc11c; &#xd751;&#xc9c4;&#xc8fc;&#xc758; &#xc57d; 80% &#xc815;&#xb3c4;&#xac00; &#xc0dd;&#xc0b0;&#xb418;&#xace0; &#xc788;&#xb2e4;&#xace0; &#xd55c;&#xb2e4;.</p><h4>2.2.4. &#xb2f4;&#xc218; &#xc9c4;&#xc8fc;</h4><p>&#xb2f4;&#xc218;(&#xbbfc;&#xbb3c;) &#xc870;&#xac1c;&#xc5d0;&#xc11c; &#xc0dd;&#xc131;&#xb41c; &#xc9c4;&#xc8fc;&#xb97c; &#xb9d0;&#xd55c;&#xb2e4;. &#xb300;&#xbd80;&#xbd84; &#xc911;&#xad6d;&#xc5d0;&#xc11c; &#xc0dd;&#xc0b0;&#xb418;&#xace0; &#xc788;&#xb2e4;.</p><h3>2.3. &#xbaa8;&#xc870; &#xc9c4;&#xc8fc;</h3><p>&#xc9c4;&#xc8fc;&#xcc98;&#xb7fc; &#xbcf4;&#xc774;&#xac8c;&#xa0;&#xb9cc;&#xb4e0; &#xac00;&#xc9dc; &#xc9c4;&#xc8fc;&#xb97c; &#xb9d0;&#xd55c;&#xb2e4;.</p><h4>2.3.1. &#xd575;&#xc9c4;&#xc8fc;</h4><p>&#xc870;&#xac1c; &#xd575;&#xacfc; &#xc870;&#xac1c;&#xc758; &#xac00;&#xb8e8;&#xb97c; &#xcf54;&#xd305;&#xc6a9;&#xc73c;&#xb85c; &#xc0ac;&#xc6a9;&#xd55c; &#xc9c4;&#xc8fc;&#xb97c; &#xb9d0;&#xd55c;&#xb2e4;.</p><h4>2.3.2. &#xc2a4;&#xc640;&#xb85c;&#xbe0c;&#xc2a4;&#xd0a4; &#xc9c4;&#xc8fc;</h4><p>&#xd06c;&#xb9ac;&#xc2a4;&#xd0c8;&#xb85c; &#xb9cc;&#xb4e4;&#xc5b4;&#xc9c4; &#xd575;&#xc744; &#xc0ac;&#xc6a9;&#xd558;&#xace0; &#xac89;&#xc744; &#xc9c4;&#xc8fc; &#xac00;&#xb8e8;&#xb85c; &#xac10;&#xc2fc; &#xac83;&#xc774;&#xb2e4;.</p><h2>3. &#xc9c4;&#xc8fc;&#xc758; &#xad00;&#xb9ac;&#xbc95;</h2><ol><li>&#xacfc;&#xc77c;&#xc999;, &#xb540;, &#xc2dd;&#xcd08;, &#xd654;&#xc7a5;&#xd488;, &#xd5e4;&#xc5b4;&#xc2a4;&#xd504;&#xb808;&#xc774;, &#xd5a5;&#xc218;, &#xc2a4;&#xd0a8;, &#xc624;&#xc77c; &#xac19;&#xc740; &#xbb3c;&#xc9c8;&#xc740; &#xad11;&#xd0dd;&#xc5d0; &#xc601;&#xd5a5;&#xc744; &#xbbf8;&#xce5c;&#xb2e4;.</li><li>&#xc790;&#xc678;&#xc120;(&#xc77c;&#xad11;)&#xc5d0; &#xc624;&#xb7ab;&#xb3d9;&#xc548; &#xb178;&#xcd9c;&#xb418;&#xc9c0; &#xc54a;&#xb3c4;&#xb85d; &#xc8fc;&#xc758;&#xd55c;&#xb2e4;.</li><li>&#xc9c0;&#xb098;&#xce58;&#xac8c; &#xac74;&#xc870;&#xd558;&#xac70;&#xb098; &#xc2b5;&#xd55c; &#xc7a5;&#xc18c;&#xc5d0; &#xbcf4;&#xad00;&#xd558;&#xc9c0; &#xc54a;&#xb294;&#xb2e4;.</li><li>&#xc218;&#xb3d7;&#xbb3c;&#xc740; &#xd45c;&#xbc31;&#xc81c; &#xc5fc;&#xc18c; &#xb4f1;&#xc744; &#xd568;&#xc720;&#xd558;&#xace0; &#xc788;&#xc5b4; &#xad11;&#xd0dd;&#xc5d0; &#xc601;&#xd5a5;&#xc774; &#xac00;&#xbbc0;&#xb85c;, &#xc774;&#xbb3c;&#xc9c8;&#xc774; &#xbb3b;&#xc5c8;&#xc744; &#xacbd;&#xc6b0; &#xbd80;&#xb4dc;&#xb7ec;&#xc6b4; &#xcc9c;&#xc73c;&#xb85c; &#xb2e6;&#xb294; &#xac83;&#xc774; &#xc88b;&#xb2e4;.</li><li>&#xace0;&#xc628;&#xc5d0; &#xc57d;&#xd558;&#xbbc0;&#xb85c; &#xbaa9;&#xc695;&#xd0d5;, &#xc628;&#xcc9c;, &#xc0ac;&#xc6b0;&#xb098;&#xa0;&#xb4f1;&#xc758; &#xc7a5;&#xc18c;&#xc5d0;&#xc11c; &#xcc29;&#xc6a9;&#xc744; &#xd53c;&#xd55c;&#xb2e4;.<br></li><li>&#xd45c;&#xba74;&#xc774; &#xc57d;&#xd558;&#xbbc0;&#xb85c; &#xb2e4;&#xb978; &#xbcf4;&#xc11d;&#xc774;&#xb098; &#xadc0;&#xae08;&#xc18d;&#xc5d0; &#xb2ff;&#xc9c0;&#xc54a;&#xac8c; &#xb530;&#xb85c; &#xbcf4;&#xad00;&#xd55c;&#xb2e4;.</li></ol><h2>4. &#xc0c1;&#xc9d5;</h2><p>&#xc21c;&#xacb0;, &#xc6b0;&#xc544;&#xd568;, &#xbd80;&#xadc0;&#xb97c; &#xc0c1;&#xc9d5;&#xd55c;&#xb2e4;.&#xa0;&#xc2ac;&#xd514;&#xc744; &#xc0c1;&#xc9d5;&#xd558;&#xae30;&#xb3c4; &#xd55c;&#xb2e4;.&#xa0;&#xc9c4;&#xc8fc;&#xb294; &#xbc14;&#xb2e4; &#xb610;&#xb294; &#xb2ec;&#xc758; &#xb208;&#xbb3c;&#xc774;&#xb098; &#xc778;&#xc5b4;&#xc758; &#xb208;&#xbb3c;&#xc774;&#xb77c;&#xace0; &#xbd88;&#xb9ac;&#xae30;&#xb3c4; &#xd55c;&#xb2e4;. <span style=\"text-decoration: line-through;\" data-mce-style=\"text-decoration: line-through;\">&#xc2e4;&#xc0c1;&#xc740; &#xc870;&#xac1c;&#xac00; &#xd758;&#xb9ac;&#xb294;&#xa0;&#xace0;&#xd1b5;&#xc758; &#xb208;&#xbb3c;</span>&#xa0;&#xb208;&#xbb3c;, &#xc2ac;&#xd514;&#xc774;&#xb77c;&#xb294; &#xc774;&#xbbf8;&#xc9c0;&#xc640; &#xacb0;&#xd569;&#xd558;&#xc5ec; &#xc7a5;&#xb840;&#xc2dd;&#xc5d0;&#xc11c; &#xc720;&#xc77c;&#xd558;&#xac8c; &#xd5c8;&#xc6a9;&#xb418;&#xb294; &#xbcf4;&#xc11d;&#xc774;&#xae30;&#xb3c4; &#xd558;&#xb2e4;.</p><h2>5. &#xc720;&#xba85;&#xd55c; &#xc9c4;&#xc8fc;</h2><p><a href=\"http://imnews.imbc.com/replay/2016/nwtoday/article/4098222_19847.html\" wk-external=\"true\" data-mce-href=\"http://imnews.imbc.com/replay/2016/nwtoday/article/4098222_19847.html\">&#xd544;&#xb9ac;&#xd540;&#xc11c; &#xc138;&#xacc4; &#xcd5c;&#xb300; 34kg &#xc9c4;&#xc8fc; &#xacf5;&#xac1c;</a>.&#xa0;&#xd544;&#xb9ac;&#xd540;&#xc5d0;&#xc11c; &#xbc1c;&#xacac;&#xb41c; &#xcd08;&#xb300;&#xd615; &#xc9c4;&#xc8fc;&#xb294; &#xae38;&#xc774; 67cm, &#xbb34;&#xac8c; 34kg&#xc774;&#xb2e4;.&#xa0;&#xac00;&#xaca9;&#xc740; 1&#xcc9c;&#xc5b5;&#xc6d0;&#xc774; &#xb118;&#xc744; &#xac83;&#xc73c;&#xb85c; &#xc608;&#xc0c1;&#xb418;&#xace0; &#xc788;&#xb2e4;. &#xc774;&#xac83;&#xc744; &#xbcf8; &#xd55c;&#xad6d; &#xc0ac;&#xb78c;&#xb4e4;&#xc758; &#xbc18;&#xc751; : &#xb9cc;&#xb450;...?</p><h2>6. &#xd2b8;&#xb9ac;&#xbe44;&#xc544;</h2><ul><li>&#xd074;&#xb808;&#xc624;&#xd30c;&#xd2b8;&#xb77c;&#xc640; &#xc9c4;&#xc8fc;</li><li>&#xc624;&#xb4dc;&#xb9ac; &#xd5f5;&#xbc88;&#xacfc; &#xc9c4;&#xc8fc; : &#xc601;&#xd654; &lt;&#xd2f0;&#xd30c;&#xb2c8;&#xc5d0;&#xc11c; &#xc544;&#xce68;&#xc744;&gt;&#xc5d0;&#xc11c; &#xc8fc;&#xc778;&#xacf5; &#xc5ed;&#xc744; &#xb9e1;&#xc740; &#xc624;&#xb4dc;&#xb9ac; &#xd5f5;&#xbc88;&#xc774; &#xcee4;&#xb2e4;&#xb780; &#xc9c4;&#xc8fc; &#xbaa9;&#xac78;&#xc774;&#xb97c; &#xcc29;&#xc6a9;&#xd558;&#xace0; &#xb098;&#xc640; &#xd654;&#xc81c;&#xac00; &#xb418;&#xc5c8;&#xb2e4;. &#xadf8;&#xc758; &#xb4dc;&#xb808;&#xc2a4;&#xc5c5; &#xd5e4;&#xc5b4;+&#xc9c4;&#xc8fc;&#xbaa9;&#xac78;&#xc774;+&#xd0c0;&#xc774;&#xd2b8;&#xd55c; &#xb9ac;&#xd2c0; &#xbe14;&#xb799; &#xb4dc;&#xb808;&#xc2a4;&#xb294; &#xd5f5;&#xbc88;&#xc758; &#xc774;&#xbbf8;&#xc9c0;&#xb97c; &#xac00;&#xc7a5; &#xc798; &#xb098;&#xd0c0;&#xb0b8; &#xcf54;&#xb514;&#xb85c; &apos;&#xd5f5;&#xbc88; &#xb8e9;&apos;&#xc774;&#xb77c;&#xace0;&#xb3c4; &#xbd88;&#xb9b0;&#xb2e4;.</li><li>&#xcf54;&#xcf54; <a class=\"pjax-link\" href=\"/wp-d/57989b7be1db80c0295ee2e9/View\" data-mce-href=\"/wp-d/57989b7be1db80c0295ee2e9/View\">&#xc0e4;&#xb12c;</a>&#xacfc; &#xc9c4;&#xc8fc; : &#xcf54;&#xcf54; &#xc0e4;&#xb12c;&#xc740; &#xc80a;&#xc740; &#xc2dc;&#xc808;&#xbd80;&#xd130; &#xb178;&#xb144;&#xc5d0; &#xc774;&#xb974;&#xae30;&#xae4c;&#xc9c0; &#xb2e4;&#xc591;&#xd55c; &#xc9c4;&#xc8fc; &#xbaa9;&#xac78;&#xc774;&#xb97c; &#xb808;&#xc774;&#xc5b4;&#xb4dc;&#xd558;&#xb294; &#xac83;&#xc744; &#xc990;&#xacbc;&#xb2e4;.&#xa0;&#xadf8;&#xac00; &#xb9cc;&#xb4e4;&#xc5b4;&#xb0b8; &#xd328;&#xc158; &#xbe0c;&#xb79c;&#xb4dc; &apos;&#xc0e4;&#xb12c;&apos;&#xc740; &#xc544;&#xc9c1;&#xae4c;&#xc9c0;&#xb3c4;&#xa0;&#xc9c4;&#xc8fc; &#xc8fc;&#xc5bc;&#xb9ac;&#xac00; &#xd568;&#xaed8; &#xb9e4;&#xce58;&#xb418;&#xc5b4; &#xd328;&#xc158;&#xc1fc;&#xc5d0; &#xb098;&#xc624;&#xb294; &#xacbd;&#xc6b0;&#xac00; &#xb9ce;&#xb2e4;. &#xcf54;&#xcf54; &#xc0e4;&#xb12c;&#xc740; &quot;&#xc9c4;&#xc8fc; &#xbaa9;&#xac78;&#xc774;&#xb294; &#xbaa8;&#xb4e0; &#xc637;&#xc744; &#xb4dc;&#xb808;&#xc2a4;&#xc5c5; &#xd55c; &#xac83;&#xcc98;&#xb7fc; &#xb9cc;&#xb4e4;&#xc5b4;&#xc8fc;&#xb294; &#xbb18;&#xd55c; &#xb9e4;&#xb825;&#xc744; &#xc9c0;&#xb154;&#xb2e4;&quot;&#xace0; &#xb9d0;&#xd588;&#xb2e4;.</li></ul><h2>7. &#xc9c4;&#xc8fc;&#xc640; &#xad00;&#xb828;&#xb41c; &#xb9d0;</h2><ul><li>&#xb3fc;&#xc9c0; &#xbaa9;&#xc5d0; &#xc9c4;&#xc8fc; &#xbaa9;&#xac78;&#xc774;: &#xc544;&#xbb34;&#xb9ac; &#xc88b;&#xc740; &#xac83;&#xc774;&#xb77c;&#xb3c4; &#xc218;&#xc900;&#xc5d0; &#xb9de;&#xc9c0; &#xc54a;&#xc73c;&#xba74; &#xc4f8;&#xbaa8;&#xac00; &#xc5c6;&#xb2e4;&#xb294; &#xb73b;.<br></li></ul>\r\n\t\t\t</div>"
