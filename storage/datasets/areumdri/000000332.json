	"postContents": "<div class=\"col-md-12 col-sm-12\">\r\n\t\t\t\t<h2>&#xac1c;&#xc694;</h2><p style=\"text-align: center;\" data-mce-style=\"text-align: center;\"><img src=\"/api/File/Real/57a346dcd4bf20eb187e53b4\" data-mce-src=\"/api/File/Real/57a346dcd4bf20eb187e53b4\"></p><p><strong>Bioshock.</strong></p><h2>&#xc904;&#xac70;&#xb9ac;</h2><p>&#xc8fc;&#xc778;&#xacf5; &#xc7ad;Jack&#xc774; &#xd0c0;&#xace0; &#xc788;&#xb358; &#xbe44;&#xd589;&#xae30;&#xac00; &#xb300;&#xc11c;&#xc591;&#xc5d0; &#xcd94;&#xb77d;&#xd55c;&#xb2e4;. &#xcd94;&#xb77d; &#xc0ac;&#xace0;&#xc5d0;&#xc11c; &#xc6a9;&#xcf00;&#xb3c4; &#xc0b4;&#xc544;&#xb0a8;&#xc740; &#xc7ad;&#xc740;, &#xbc14;&#xb2e4; &#xd55c; &#xac00;&#xc6b4;&#xb370;&#xc5d0; &#xc704;&#xce58;&#xd55c; &#xc815;&#xccb4; &#xbd88;&#xba85;&#xc758; &#xb4f1;&#xb300;&#xb97c; &#xbc1c;&#xacac;&#xd55c;&#xb2e4;. &#xb4f1;&#xb300; &#xc9c0;&#xd558;&#xc5d0;&#xb294; &#xc5b4;&#xb518;&#xac00;&#xb85c; &#xc774;&#xc5b4;&#xc9c0;&#xb294; &#xc7a0;&#xc218;&#xc815;&#xc774; &#xc788;&#xc5c8;&#xace0;, &#xc7ad;&#xc740; &#xd560; &#xc218; &#xc5c6;&#xc774; &#xadf8; &#xc7a0;&#xc218;&#xc815;&#xc5d0; &#xd0d1;&#xc2b9;&#xd558;&#xace0;, &#xc2ec;&#xd574; &#xb3c4;&#xc2dc; &#xb7a9;&#xccd0;(Rapture)&#xc5d0; &#xb3c4;&#xb2ec;&#xd558;&#xac8c; &#xb41c;&#xb2e4;. &#xc544;&#xb974;&#xb370;&#xcf54; &#xc591;&#xc2dd;&#xc73c;&#xb85c; &#xd654;&#xb824;&#xd558;&#xac8c; &#xbed7;&#xc5b4; &#xc788;&#xb294; &#xb9c8;&#xcc9c;&#xb8e8;&#xac00; &#xac00;&#xb4dd;&#xd55c; &#xb7a9;&#xccd0;&#xb294;, &#xadf8;&#xb7ec;&#xb098; &#xc5b4;&#xb518;&#xc9c0; &#xc1e0;&#xb77d;&#xd55c; &#xbaa8;&#xc2b5;&#xc774;&#xb2e4;.&#xa0;<br></p><p>&apos;&#xb7a9;&#xccd0;&#xc758; &#xd658;&#xc601;&apos; &#xad6c;&#xc5ed;&#xc5d0; &#xb3c4;&#xcc29;&#xd55c; &#xc7ad;&#xc740; &#xc2a4;&#xd50c;&#xb77c;&#xc774;&#xc11c;(splicer) &#xb85c;&#xc988;&#xc758; &#xacf5;&#xaca9;&#xc744; &#xbc1b;&#xac8c; &#xb418;&#xb098;, &#xc790;&#xc2e0;&#xc744; &#xc544;&#xd2c0;&#xb77c;&#xc2a4;(Atlas)&#xb77c;&#xace0; &#xc18c;&#xac1c;&#xd558;&#xb294; &#xb0a8;&#xc790;&#xc5d0;&#xac8c; &#xb3c4;&#xc6c0;&#xc744; &#xbc1b;&#xc544; &#xc8fd;&#xc74c;&#xc758; &#xc704;&#xae30;&#xb97c; &#xbc97;&#xc5b4;&#xb09c;&#xb2e4;.&#xa0;&#xc544;&#xd2c0;&#xb77c;&#xc2a4;&#xb294; &#xc7ad;&#xc5d0;&#xac8c; &#xc790;&#xc2e0;&#xc758; &#xc544;&#xb0b4; &#xbaa8;&#xc774;&#xb77c;&#xc640; &#xc544;&#xb4e4;&#xc758; &#xad6c;&#xc870;&#xb97c; &#xbd80;&#xd0c1;&#xd55c;&#xb2e4;.</p><p>&#xa0;&#xc774;&#xd6c4; &#xc7ad;&#xc740; &#xbb34;&#xc804;&#xae30;&#xb85c; &#xc544;&#xd2c0;&#xb77c;&#xc2a4;&#xc758; &#xc9c0;&#xc2dc;&#xb97c; &#xbc1b;&#xc73c;&#xba70;, &#xb7a9;&#xccd0;&#xb97c; &#xac10;&#xc2fc; &#xac70;&#xb300;&#xd55c; &#xc218;&#xc218;&#xaed8;&#xb07c; &#xc18d;&#xc73c;&#xb85c; &#xd5a5;&#xd558;&#xac8c; &#xb418;&#xb294;&#xb370;......</p><p>&#xa0;<br></p><h2><span style=\"letter-spacing: 2px; font-size: 23px; font-weight: 600; -webkit-text-stroke-width: 0.46000000834465027px;\" data-mce-style=\"font-size: 23px; font-weight: 600; letter-spacing: 2px; -webkit-text-stroke-width: 0.46000000834465027px;\">&#xb4f1;&#xc7a5;&#xc778;&#xbb3c;</span></h2><p><img width=\"161\" height=\"209\" src=\"/api/File/Real/57a341c8d4bf20eb187e52db\" data-mce-src=\"/api/File/Real/57a341c8d4bf20eb187e52db\">&#xa0;</p><p><strong>&#xc7ad;(Jack)</strong></p><p>&#xc8fc;&#xc778;&#xacf5;. &#xc791;&#xc911; &#xd50c;&#xb808;&#xc774;&#xc5b4;&#xb294; &#xc7ad;&#xc758; &#xc2dc;&#xc810;&#xc5d0;&#xc11c; &#xac8c;&#xc784;&#xc744; &#xd50c;&#xb808;&#xc774;&#xd55c;&#xb2e4;. &#xac04;&#xac04;&#xc774; &#xb5a0;&#xc624;&#xb974;&#xb294; &#xd50c;&#xb798;&#xc2dc;&#xbc31;&#xc73c;&#xb85c; &#xbcf4;&#xc544;, &#xcd94;&#xb77d;&#xc0ac;&#xace0; &#xc804;&#xc5d0;&#xb294; &#xb18d;&#xc7a5;&#xc5d0;&#xc11c; &#xbd80;&#xbaa8;&#xb2d8;&#xacfc; &#xb2e8;&#xb780;&#xd558;&#xac8c; &#xc0b4;&#xc558;&#xb358; &#xb4ef;.&#xa0;</p><p>19&#xc138; &#xc778;&#xac04;&#xc774;&#xb77c;&#xace0;&#xb294; &#xbbff;&#xae30;&#xc9c0; &#xc54a;&#xc744; &#xc815;&#xb3c4;&#xb85c; &#xac15;&#xd55c; &#xc644;&#xb825;&#xc744; &#xc790;&#xb791;&#xd55c;&#xb2e4;.&#xa0;</p><p>&#xc544;&#xd2c0;&#xb77c;&#xc2a4;&#xc758; &#xbd80;&#xd0c1;&#xc73c;&#xb85c; &#xadf8;&#xc758; &#xcc98;&#xc790;&#xc2dd;&#xc744; &#xad6c;&#xd558;&#xb294; &#xc77c;&#xc5d0; &#xb3d9;&#xc6d0;&#xb418;&#xace0;, &#xc774;&#xd6c4; &#xbcf5;&#xc218;&#xc2ec;&#xc5d0; &#xbd88;&#xd0c0;&#xb294; &#xc544;&#xd2c0;&#xb77c;&#xc2a4;&#xc5d0; &#xc758;&#xd574; &#xc564;&#xb4dc;&#xb8e8; &#xb77c;&#xc774;&#xc5b8;&#xc744; &#xc8fd;&#xc774;&#xb294; &#xc77c;&#xc5d0; &#xb3d9;&#xcc38;&#xd558;&#xac8c; &#xb41c;&#xb2e4;.&#xa0;</p><p>&#xa0;<br></p><p>&#xa0;<img width=\"153\" height=\"191\" src=\"/api/File/Real/57a34170d4bf20eb187e52bf\" data-mce-src=\"/api/File/Real/57a34170d4bf20eb187e52bf\"><br></p><p><strong>&#xc544;&#xd2c0;&#xb77c;&#xc2a4;(Atlas)</strong></p><p>&#xc8fc;&#xc778;&#xacf5;&#xc758; &#xc870;&#xb825;&#xc790;&#xc774;&#xc790;, &#xc564;&#xb4dc;&#xb8e8; &#xb77c;&#xc774;&#xc5b8;&#xc5d0;&#xac8c; &#xb300;&#xc801;&#xd558;&#xb294; &#xc138;&#xb825;&#xc758; &#xc218;&#xc7a5;. &#xc8fc;&#xc778;&#xacf5;&#xc774; &#xcc98;&#xc74c;&#xc73c;&#xb85c; &#xc2a4;&#xd50c;&#xb77c;&#xc774;&#xc11c;&#xb97c; &#xb9de;&#xb2e5;&#xb728;&#xb838;&#xc744; &#xb54c;, &#xbcf4;&#xc548; &#xb85c;&#xbd07;&#xc744; &#xc774;&#xc6a9;&#xd574; &#xadf8;&#xb97c; &#xad6c;&#xd574; &#xc8fc;&#xc5c8;&#xb2e4;.&#xa0;</p><p>&#xc774;&#xd6c4; &#xc7ad;&#xc5d0;&#xac8c; &apos;&#xb135;&#xd2a0;&#xc758; &#xc740;&#xd61c;&apos;&#xc758; &#xc7a0;&#xc218;&#xc815; &#xc548;&#xc5d0; &#xc788;&#xb294; &#xc790;&#xc2e0;&#xc758; &#xc544;&#xb0b4; &#xbaa8;&#xc774;&#xb77c;&#xc640; &#xadf8;&#xb4e4;&#xc758; &#xc5b4;&#xb9b0; &#xc544;&#xb4e4;&#xc744; &#xad6c;&#xcd9c;&#xd574; &#xb2ec;&#xb77c;&#xace0; &#xbd80;&#xd0c1;&#xd55c;&#xb2e4;.&#xa0;</p><p>&#xadf8;&#xb7ec;&#xb098; &#xadf8;&#xb4e4;&#xc774; &#xd0c0;&#xace0; &#xc788;&#xb294; &#xc7a0;&#xc218;&#xc815;&#xc774; &#xc8fc;&#xc778;&#xacf5;&#xc758; &#xb208; &#xc55e;&#xc5d0;&#xc11c; &#xd3ed;&#xbc1c;&#xd558;&#xc790;, &#xc544;&#xd2c0;&#xb77c;&#xc2a4;&#xb294; &#xbcf5;&#xc218;&#xc2ec;&#xc5d0; &#xbd88;&#xd0c0; &#xadf8;&#xb4e4;&#xc774; &#xc564;&#xb4dc;&#xb8e8; &#xb77c;&#xc774;&#xc5b8;&#xc744; &#xbc18;&#xb4dc;&#xc2dc; &#xc8fd;&#xc5ec;&#xc57c; &#xd55c;&#xb2e4;&#xace0; &#xd558;&#xace0;, &#xadf8;&#xc5d0; &#xb530;&#xb77c; &#xc7ad;&#xc740; &#xb77c;&#xc774;&#xc5b8;&#xc744; &#xcc98;&#xce58;&#xd558;&#xb294; &#xc5ec;&#xc815;&#xc744; &#xb5a0;&#xb098;&#xac8c; &#xb41c;&#xb2e4;.&#xa0;&#xadf8;&#xb7ec;&#xb098; &#xadf8;&#xc758; &#xc9c4;&#xc9dc; &#xc815;&#xccb4;&#xb294;......</p><p>&#xa0;<br></p><p>&#xa0;<img width=\"153\" height=\"191\" src=\"/api/File/Real/57a34133d4bf20eb187e52b0\" data-mce-src=\"/api/File/Real/57a34133d4bf20eb187e52b0\"><br></p><p><strong>&#xc564;&#xb4dc;&#xb8e8; &#xb77c;&#xc774;&#xc5b8;(Andrew Ryan)</strong></p><p>&#xb7a9;&#xccd0;&#xc758; &#xcc3d;&#xc2dc;&#xc790;. &#xbcf8;&#xba85;&#xc740; &#xc548;&#xb4dc;&#xb808;&#xc774; &#xb77c;&#xc774;&#xb189;&#xc2a4;&#xd0a4;&#xb85c;, &#xc18c;&#xb828;&#xc758; &#xc219;&#xccad;&#xc744; &#xd53c;&#xd574; &#xbbf8;&#xad6d;&#xc73c;&#xb85c; &#xc774;&#xbbfc;&#xd588;&#xb2e4;.</p><p>&#xc644;&#xc804;&#xd55c; &#xc790;&#xc720;&#xc8fc;&#xc758; &#xc2dc;&#xc7a5; &#xacbd;&#xc81c;&#xb97c; &#xc9c0;&#xd5a5;&#xd558;&#xb294; &#xae30;&#xc5c5;&#xc778;&#xc73c;&#xb85c;, &#xbbf8;&#xad6d; &#xc815;&#xbd80;&#xc758; &#xc815;&#xcc45;&#xc5d0; &#xbd88;&#xb9cc;&#xc744; &#xd488;&#xace0;, &#xc790;&#xc2e0;&#xc758; &#xc774;&#xc0c1;&#xc744; &#xb7a9;&#xccd0;&#xb77c;&#xb294; &#xb3c4;&#xc2dc;&#xb97c; &#xd1b5;&#xd574; &#xc2e4;&#xd604;&#xd558;&#xace0;&#xc790; &#xd558;&#xc600;&#xb2e4;.&#xa0;</p><p>&#xadf8;&#xb294; &#xc790;&#xc2e0;&#xc758; &#xc774;&#xc0c1;&#xc0ac;&#xd68c;&#xb294; &apos;&#xc704;&#xb300;&#xd55c; &#xc0ac;&#xc2ac;(The Great Chain)&apos;&#xc5d0; &#xc758;&#xd574; &#xc6b4;&#xc601;&#xb41c;&#xb2e4;&#xace0; &#xc5ec;&#xacbc;&#xb294;&#xb370;, &#xc5ec;&#xae30;&#xc11c; &#xc704;&#xb300;&#xd55c; &#xc0ac;&#xc2ac;&#xc774;&#xb780; &#xc27d;&#xac8c; &#xb9d0;&#xd574; &#xc560;&#xb364; &#xc2a4;&#xbbf8;&#xc2a4;&#xc758; &apos;&#xbcf4;&#xc774;&#xc9c0; &#xc54a;&#xb294; &#xc190;&apos;&#xacfc; &#xbe44;&#xc2b7;&#xd55c; &#xac1c;&#xb150;&#xc774;&#xb2e4;.&#xa0;</p><p>&#xc564;&#xb4dc;&#xb8e8; &#xb77c;&#xc774;&#xc5b8;&#xc740; &#xc774; &#xc704;&#xb300;&#xd55c; &#xc0ac;&#xc2ac;&#xc758; &#xc791;&#xb3d9; &#xacfc;&#xc815;&#xc5d0;&#xc11c; &#xd544;&#xc5f0;&#xc801;&#xc73c;&#xb85c; &#xbc1c;&#xc0dd;&#xd558;&#xb294; &#xb099;&#xc624;&#xc790;&#xb4e4;&#xc744; &#xae30;&#xc0dd;&#xcda9;(parasite) &#xb77c;&#xace0; &#xbd80;&#xb974;&#xba70; &#xc5b4;&#xb5a4; &#xc0ac;&#xd68c;&#xc801; &#xad6c;&#xc81c; &#xc0ac;&#xc5c5;&#xb3c4; &#xd3bc;&#xce58;&#xc9c0; &#xc54a;&#xc558;&#xb2e4;.&#xa0;&#xadf8;&#xb7ec;&#xb098; &#xc774;&#xac83;&#xc774; &#xc624;&#xd788;&#xb824; &#xadf8;&#xc758; &#xbab0;&#xb77d;&#xc758; &#xb3c4;&#xd654;&#xc120;&#xc774; &#xb41c;&#xb2e4;.</p><p>&#xa0;<br></p><p><img width=\"151\" height=\"189\" src=\"/api/File/Real/57a34237d4bf20eb187e52f9\" data-mce-src=\"/api/File/Real/57a34237d4bf20eb187e52f9\"></p><p><strong>&#xd504;&#xb7ad;&#xd06c; &#xd3f0;&#xd14c;&#xc778;(Frank Fontaine)</strong></p><p><em>&quot;&#xc774; &#xd3f0;&#xd14c;&#xc778;&#xc774;&#xb77c;&#xb294; &#xce5c;&#xad6c;&#xb97c; &#xd56d;&#xc0c1; &#xc8fc;&#xc2dc;&#xd574;&#xc57c; &#xd574;.&#xa0;&#xd55c;&#xb54c; &#xadf8;&#xb294;&#xa0;&#xadf8;&#xc800; &#xc720;&#xc8c4; &#xc120;&#xace0;&#xb97c; &#xb0b4;&#xb824; &#xad50;&#xc218;&#xd615;&#xc5d0; &#xcc98;&#xd560; &#xace8;&#xce6b;&#xac70;&#xb9ac;&#xc5d0; &#xbd88;&#xacfc;&#xd588;&#xc9c0;.&#xa0;&#xa0;&#xd558;&#xc9c0;&#xb9cc; &#xadf8;&#xb294; &#xc808;&#xb300; &#xc99d;&#xac70;&#xb97c; &#xb0a8;&#xae30;&#xc9c0; &#xc54a;&#xc544;. &#xadf8; &#xae61;&#xd328; &#xc790;&#xc2dd;&#xc774; &#xac00;&#xc7a5; &#xc704;&#xd5d8;&#xd55c; &#xcd95;&#xc5d0; &#xb4dc;&#xb294; &#xc774;&#xc720;&#xb294; ...... &#xadf8;&#xac00; &#xbe44;&#xc804;&#xc744; &#xac16;&#xace0; &#xc788;&#xb294; &#xbd80;&#xb958;&#xc774;&#xae30; &#xb54c;&#xbb38;&#xc774;&#xc57c;.&quot;</em></p><p style=\"text-align: right;\" data-mce-style=\"text-align: right;\"><em>-&#xc564;&#xb4dc;&#xb8e8; &#xb77c;&#xc774;&#xc5b8;</em></p><p>&#xb7a9;&#xccd0;&#xc758; &#xc0ac;&#xc5c5;&#xac00;/&#xbc94;&#xc8c4;&#xc790;. &#xa0;&#xb7a9;&#xccd0;&#xc5d0;&#xc11c; &#xc5c4;&#xaca9;&#xd558;&#xac8c; &#xae08;&#xc9c0;&#xb418;&#xc5b4; &#xc788;&#xb294; &#xbc00;&#xc218; &#xb4f1;&#xc758; &#xbd88;&#xbc95; &#xc0ac;&#xc5c5;&#xc73c;&#xb85c; &#xbd80;&#xb97c; &#xcd95;&#xc801;&#xd588;&#xc744; &#xbfd0;&#xb9cc; &#xc544;&#xb2c8;&#xb77c;, &#xace0;&#xc544;&#xc6d0; &#xb4f1;&#xc758; &#xad6c;&#xc81c; &#xc0ac;&#xc5c5;&#xc744; &#xd1b5;&#xd574; &#xbe48;&#xbbfc;&#xce35;&#xc758; &#xbbfc;&#xc2ec;&#xc744; &#xc5bb;&#xb294; &#xb370; &#xc131;&#xacf5;&#xd588;&#xb2e4;. &#xadf8;&#xb7ec;&#xb098; &#xadf8;&#xc758; &#xc0ac;&#xc5c5;&#xc774; &#xb77c;&#xc774;&#xc5b8; &#xacf5;&#xc5c5;&#xc744; &#xc704;&#xd611;&#xd560; &#xc815;&#xb3c4;&#xb85c; &#xcee4;&#xc9c4; &#xac83;&#xc740; &#xc720;&#xc804;&#xc790; &#xc870;&#xc791; &#xd50c;&#xb77c;&#xc2a4;&#xbbf8;&#xb4dc; &#xc5f0;&#xad6c;&#xc758; &#xacf5;&#xc774; &#xc9c0;&#xb300;&#xd588;&#xb2e4;. &#xb610;&#xd55c; &#xc774; &#xc0ac;&#xc5c5;&#xc740; &#xb7a9;&#xccd0;&#xc758; &#xbab0;&#xb77d;&#xc744; &#xac00;&#xc838;&#xc624;&#xb294; &#xacb0;&#xc815;&#xc801;&#xc778; &#xc5ed;&#xd560;&#xc744; &#xd55c;&#xb2e4;.&#xa0;</p><p>1958&#xb144; &#xb77c;&#xc774;&#xc5b8;&#xc774; &#xbcf4;&#xb0b8; &#xc790;&#xac1d;&#xc5d0; &#xc758;&#xd574; &#xc0ac;&#xb9dd;&#xd588;&#xb2e4;&#xace0; &#xc54c;&#xb824;&#xc838; &#xc788;&#xb2e4;.</p><p>&#xa0;<br></p><p><img width=\"152\" height=\"189\" src=\"/api/File/Real/57a3428fd4bf20eb187e5311\" data-mce-src=\"/api/File/Real/57a3428fd4bf20eb187e5311\"></p><p><strong>&#xbe0c;&#xb9ac;&#xae43; &#xd14c;&#xb128;&#xbc14;&#xc6c0;(Brigid Tenenbaum)</strong></p><p>&#xb7a9;&#xccd0; &#xbab0;&#xb77d;&#xc758; &#xc9c1;&#xc811;&#xc801;&#xc778; &#xc6d0;&#xc778;&#xc778; &#xc544;&#xb2f4;(ADAM)&#xc744; &#xbc1c;&#xacac;&#xd558;&#xace0; &#xc5f0;&#xad6c;&#xd55c; &#xacfc;&#xd559;&#xc790;. &#xbca8;&#xb77c;&#xb8e8;&#xc2a4;&#xc5d0;&#xc11c; &#xd0dc;&#xc5b4;&#xb098;, &#xc5b4;&#xb9b0; &#xc2dc;&#xc808; &#xb098;&#xce58;&#xc758; &#xc720;&#xb300;&#xc778; &#xc218;&#xc6a9;&#xc18c;&#xc5d0; &#xb04c;&#xb824;&#xac14;&#xb2e4;. &#xadf8;&#xb7ec;&#xb098; &#xb180;&#xb77c;&#xc6b4; &#xc218;&#xc900;&#xc758; &#xc9c0;&#xc801; &#xb2a5;&#xb825; &#xb355;&#xbd84;&#xc5d0; &#xadf8;&#xacf3;&#xc5d0;&#xc11c; &#xc8fd;&#xc74c;&#xc744; &#xba74;&#xd558;&#xace0;, &#xadf8; &#xb300;&#xc2e0; &#xb098;&#xce58;&#xc758; &#xc0dd;&#xccb4; &#xc2e4;&#xd5d8;&#xc758; &#xbcf4;&#xc870;&#xb85c;&#xc11c; &#xb3d9;&#xc6d0;&#xb418;&#xc5c8;&#xb2e4;. &#xc774;&#xb807;&#xb4ef; &#xb9dd;&#xac00;&#xc9c4; &#xd658;&#xacbd;&#xc5d0;&#xc11c; &#xc131;&#xc7a5;&#xd55c; &#xd0d3;&#xc778;&#xc9c0;, (&#xd3f0;&#xd14c;&#xc778;&#xc758; &#xd3c9;&#xac00;&#xc5d0; &#xc758;&#xd558;&#xba74;)&#xa0;&#xbcf8;&#xc778;&#xc758; &#xc778;&#xaca9;&#xb3c4; &#xb9dd;&#xac00;&#xc838; &#xc788;&#xb2e4;. &#xc774;&#xc218;&#xc885;&#xacfc; &#xb9c8;&#xcc2c;&#xac00;&#xc9c0;&#xb85c;, &#xc5f0;&#xad6c; &#xc724;&#xb9ac;&#xb97c; &#xc800;&#xbc84;&#xb9b0; &#xb9e4;&#xb4dc; &#xc0ac;&#xc774;&#xc5b8;&#xd2f0;&#xc2a4;&#xd2b8;.</p><p>&#xc6b0;&#xc5f0;&#xd55c; &#xacc4;&#xae30;&#xb85c;, &#xc544;&#xb2f4;&#xc744; &#xc0dd;&#xc131;&#xd558;&#xb294; &#xbc14;&#xb2e4; &#xbbfc;&#xb2ec;&#xd33d;&#xc774;(Sea Slug)&#xc758; &#xc874;&#xc7ac;&#xb97c; &#xc54c;&#xac8c; &#xb41c;&#xb2e4;. &#xc544;&#xb2f4;&#xc5d0; &#xc190;&#xc0c1;&#xb41c; &#xc138;&#xd3ec;&#xb97c; &#xbcf5;&#xad6c;&#xc2dc;&#xd0a4;&#xb294; &#xb2a5;&#xb825; &#xbfd0;&#xb9cc; &#xc544;&#xb2c8;&#xb77c; &#xc8fd;&#xc740; &#xc138;&#xd3ec;&#xae4c;&#xc9c0; &#xc18c;&#xc0dd;&#xc2dc;&#xd0a4;&#xb294; &#xb2a5;&#xb825;&#xc774; &#xc788;&#xb2e4;&#xace0; &#xd655;&#xc2e0;&#xd55c; &#xadf8;&#xb140;&#xb294; &#xc790;&#xc2e0;&#xc758; &#xc5f0;&#xad6c;&#xb97c; &#xc9c0;&#xc6d0;&#xd574; &#xc904; &#xd6c4;&#xc6d0;&#xc790;&#xb97c; &#xcc3e;&#xc9c0;&#xb9cc;, &#xd3f0;&#xd14c;&#xc778;&#xc744; &#xc81c;&#xc678;&#xd55c; &#xbaa8;&#xb4e0; &#xc0ac;&#xb78c;&#xb4e4;&#xc5d0;&#xac8c;&#xc11c; &#xac70;&#xbd80;&#xb2f9;&#xd55c;&#xb2e4;. &#xd3f0;&#xd14c;&#xc778;&#xc740; &#xc544;&#xb2f4; &#xc0dd;&#xc0b0;/&#xc751;&#xc6a9; &#xc0ac;&#xc5c5;&#xc5d0; &#xc7a0;&#xc7ac;&#xd55c; &#xc5c4;&#xccad;&#xb09c; &#xae08;&#xc804;&#xc801;