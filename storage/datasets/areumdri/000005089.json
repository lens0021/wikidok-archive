, &#xd50c;&#xb808;&#xc774;&#xc5b4;&#xb4e4;&#xc774; &#xac8c;&#xc784;&#xc758; &#xbcf8;&#xd3b8;&#xc774;&#xb77c;&#xace0; &#xc0dd;&#xac01;&#xd558;&#xb294; &#xbd80;&#xbd84;&#xc740; &#xc0ac;&#xc2e4; &#xc8fc;&#xc778;&#xacf5;&#xc774; &#xc560;&#xb2c8;&#xba38;&#xc2a4;&#xb97c; &#xd1b5;&#xd574; &#xc7ac;&#xd604;&#xd558;&#xb294;, &#xacfc;&#xac70;&#xc5d0; &#xc774;&#xbbf8; &#xc2e4;&#xc81c;&#xb85c; &#xc788;&#xc5c8;&#xb358; &#xc0ac;&#xac74;&#xc778; &#xc148;&#xc774;&#xb2e4;. &quot;&#xb3d9;&#xae30;&#xd654;&quot;&#xb77c;&#xb294; &#xac1c;&#xb150;&#xc774;&#xa0;&#xac8c;&#xc784; &#xb0b4;&#xc5d0;&#xc11c; &#xc2ec;&#xc2ec;&#xcc2e;&#xac8c; &#xb098;&#xc624;&#xb294;&#xb370;, &#xc774;&#xb294; &#xc120;&#xc870;&#xc758; &#xae30;&#xc5b5;&#xacfc; &#xc560;&#xb2c8;&#xba38;&#xc2a4;&#xb97c; &#xd1b5;&#xd55c; &#xd50c;&#xb808;&#xc774;&#xc5b4;&#xc758; &#xc0ac;&#xac74; &#xc7ac;&#xd604;&#xc774; &#xc5bc;&#xb9c8;&#xb098; &#xc774;&#xb8e8;&#xc5b4;&#xc84c;&#xb294;&#xc9c0;&#xb97c; &#xb530;&#xc9c0;&#xb294; &#xac1c;&#xb150;&#xc774;&#xb2e4;. &#xc560;&#xb2c8;&#xba38;&#xc2a4; &#xc548;&#xc5d0;&#xc11c; &#xc8fc;&#xc778;&#xacf5;&#xc758; &#xccb4;&#xb825;&#xc774; &quot;&#xb3d9;&#xae30;&#xd654;&#xc728;&quot;&#xb85c; &#xd45c;&#xc2dc;&#xb418;&#xba70;(&#xb2e4;&#xb9cc; &#xc774;&#xb294; &#xc5b4;&#xc314;&#xc2e0; &#xd06c;&#xb9ac;&#xb4dc; 1&#xd3b8;&#xc5d0;&#xc11c;&#xc758; &#xac1c;&#xb150;&#xc774;&#xace0;, &#xc774;&#xd6c4; &#xc2dc;&#xb9ac;&#xc988;&#xc5d0;&#xc11c;&#xb294; &#xadf8; &#xac1c;&#xb150;&#xc774; &#xd76c;&#xbc15;&#xd574;&#xc838; &#xd3c9;&#xbc94;&#xd558;&#xac8c; &quot;&#xccb4;&#xb825;&quot; &#xc73c;&#xb85c; &#xbcf4;&#xac8c; &#xb41c;&#xb2e4;.), &#xc8fc;&#xc778;&#xacf5;&#xc774; &#xc8fd;&#xac70;&#xb098; &#xbbf8;&#xc158;&#xc5d0;&#xc11c; &#xc2e4;&#xd328;&#xd55c; &#xacbd;&#xc6b0;&#xb97c; &quot;&#xbe44;&#xb3d9;&#xae30;&#xd654;&quot;&#xb85c; &#xd45c;&#xc2dc;&#xd558;&#xb294; &#xac83;&#xc740; &#xc2dc;&#xb9ac;&#xc988;&#xc758; &#xc804;&#xd1b5;&#xc774;&#xc790; &#xc0c1;&#xc9d5;&#xc774;&#xb77c;&#xace0; &#xbcfc; &#xc218; &#xc788;&#xc744; &#xc815;&#xb3c4;&#xb2e4;.</p><p>&#xa0;</p><h3>&#xd604;&#xb300; &#xd30c;&#xd2b8;</h3><p>&#xacfc;&#xac70; &#xc2a4;&#xd1a0;&#xb9ac;&#xc640;&#xb294; &#xbcc4;&#xac1c;&#xb85c; &#xd604;&#xb300; &#xd30c;&#xd2b8;&#xc5d0;&#xc11c;&#xb3c4; &#xb098;&#xb984;&#xb300;&#xb85c;&#xc758; &#xc774;&#xc57c;&#xae30; &#xc9c4;&#xd589;&#xc774; &#xc788;&#xc73c;&#xba70;, &#xc8fc;&#xc778;&#xacf5;&#xc774; &#xc560;&#xb2c8;&#xba38;&#xc2a4;&#xb97c; &#xd1b5;&#xd574; &#xc120;&#xc870;&#xc758; &#xae30;&#xc5b5;&#xb4e4;&#xc744; &#xc5f4;&#xb78c;&#xd574;&#xc57c;&#xb9cc; &#xd558;&#xb294; &#xb3d9;&#xae30;&#xb97c; &#xc81c;&#xacf5;&#xd574;&#xc8fc;&#xb294; &#xc5ed;&#xd560;&#xc744; &#xd55c;&#xb2e4;. &#xc77c;&#xc885;&#xc758; &#xc561;&#xc790;&#xd615; &#xc11c;&#xc0ac; &#xad6c;&#xc870;&#xb85c; &#xbcfc; &#xc218;&#xb3c4; &#xc788;&#xb2e4;.</p><p>&#xa0;</p><p>&#xb2e4;&#xb9cc; &#xbcf8;&#xd3b8;&#xc5d0; &#xbe44;&#xd574;&#xc11c;&#xb294; &#xbe44;&#xc911;&#xc774; &#xb0ae;&#xace0;, &#xb9e4; &#xc2dc;&#xb9ac;&#xc988;&#xb9c8;&#xb2e4; &#xacc4;&#xc18d; &#xadf8; &#xbaa8;&#xc2b5;&#xc774; &#xbcc0;&#xd558;&#xae30;&#xb3c4; &#xd588;&#xb2e4;. &#xc5b4;&#xc314;&#xc2e0; &#xd06c;&#xb9ac;&#xb4dc; 3&#xd3b8;&#xae4c;&#xc9c0;&#xb294; &#xd604;&#xb300; &#xd30c;&#xd2b8;&#xc758; &#xc8fc;&#xc778;&#xacf5;&#xc778; &#xb370;&#xc2a4;&#xbaac;&#xb4dc; &#xb9c8;&#xc77c;&#xc988;&#xb97c; 3&#xc778;&#xce6d; &#xad00;&#xc810;&#xc5d0;&#xc11c; &#xc870;&#xc791;&#xd560; &#xc218; &#xc788;&#xc73c;&#xba70; 3&#xd3b8;&#xc5d0; &#xc774;&#xb974;&#xb7ec;&#xc11c;&#xb294; &#xb370;&#xc2a4;&#xbaac;&#xb4dc;&#xac00; &#xacfc;&#xac70; &#xd30c;&#xd2b8;&#xcc98;&#xb7fc; &#xc9c1;&#xc811; &#xbbf8;&#xc158;&#xc744; &#xc218;&#xd589;&#xd558;&#xae30;&#xb3c4; &#xd558;&#xc9c0;&#xb9cc;, 4&#xd3b8;&#xbd80;&#xd130;&#xb294; &#xd50c;&#xb808;&#xc774;&#xc5b4;&#xac00; &#xc9c1;&#xc811; 1&#xc778;&#xce6d; &#xad00;&#xc810;&#xc73c;&#xb85c; &#xc870;&#xc791;&#xd558;&#xba70;, &#xc720;&#xb2c8;&#xd2f0;&#xc5d0; &#xc774;&#xb974;&#xb7ec;&#xc11c;&#xb294; &#xc544;&#xc608; &#xc8fc;&#xc778;&#xacf5; &#xad00;&#xc810;&#xc5d0;&#xc11c; &#xc9c1;&#xc811; &#xc870;&#xc791;&#xd558;&#xb294; &#xd604;&#xb300; &#xd30c;&#xd2b8; &#xbd80;&#xbd84;&#xc774; &#xc5c6;&#xc5b4;&#xc9c0;&#xace0; &#xc911;&#xac04;&#xc911;&#xac04; &#xd604;&#xb300; &#xd30c;&#xd2b8;&#xc758; &#xc774;&#xc57c;&#xae30; &#xd750;&#xb984;&#xc744; &#xc54c; &#xc218; &#xc788;&#xb294; &#xc601;&#xc0c1; &#xb4f1;&#xc73c;&#xb85c; &#xb300;&#xccb4;&#xd558;&#xac8c; &#xb418;&#xc5c8;&#xb2e4;.</p><p>&#xa0;</p><h3>&#xacfc;&#xac70; (&#xc560;&#xb2c8;&#xba38;&#xc2a4;) &#xd30c;&#xd2b8;</h3><p>&#xc5b4;&#xc314;&#xc2e0; &#xd06c;&#xb9ac;&#xb4dc; &#xac8c;&#xc784;&#xc758; &#xbcf8;&#xd3b8;&#xc774;&#xb77c;&#xace0; &#xbcfc; &#xc218; &#xc788;&#xb294; &#xbd80;&#xbd84;&#xc73c;&#xb85c;, &#xc8fc;&#xb85c; &#xc120;&#xc870;&#xb4e4;&#xc758; &#xae30;&#xc5b5;&#xc5d0; &#xd574;&#xb2f9;&#xd558;&#xb294; &#xad6c;&#xac04;&#xc774;&#xb2e4;. &#xb370;&#xc2a4;&#xbaac;&#xb4dc; &#xc0ac;&#xac00;&#xc5d0; &#xc18d;&#xd558;&#xb294; &#xc5b4;&#xc314;&#xc2e0; &#xd06c;&#xb9ac;&#xb4dc; 3&#xd3b8;&#xacfc; &#xbe14;&#xb799;&#xd50c;&#xb798;&#xadf8;&#xae4c;&#xc9c0;&#xb294; &#xb370;&#xc2a4;&#xbaac;&#xb4dc; &#xb9c8;&#xc77c;&#xc988;&#xc758; &#xc120;&#xc870;&#xc778; &#xc54c;&#xd0c0;&#xc774;&#xb974; &#xc774;&#xbe10; &#xb77c; &#xc544;&#xd558;&#xb4dc;, &#xc5d0;&#xc9c0;&#xc624; &#xc544;&#xc6b0;&#xb514;&#xd1a0;&#xb808;, &#xb77c;&#xb454;&#xd558;&#xac8c;&#xb454;(&#xcf54;&#xb108;), &#xc5d0;&#xb4dc;&#xc6cc;&#xb4dc; &#xcf04;&#xc6e8;&#xc774; &#xb4f1;&#xc758; &#xae30;&#xc5b5;&#xc744; &#xd0d0;&#xc0ac;&#xd558;&#xac8c; &#xb418;&#xba70;, &#xc720;&#xb2c8;&#xd2f0;&#xbd80;&#xd130;&#xb294; &#xd604;&#xb300; &#xd30c;&#xd2b8;&#xc758; &#xd50c;&#xb808;&#xc774;&#xc5b4;&#xac00; &#xbd88;&#xd2b9;&#xc815; &#xb2e4;&#xc218;&#xc758; &#xac1c;&#xc778;&#xc73c;&#xb85c; &#xbcc0;&#xd558;&#xba74;&#xc11c; &#xad73;&#xc774; &#xc790;&#xc2e0;&#xc758; &#xc120;&#xc870;&#xac00; &#xc544;&#xb2c8;&#xc5b4;&#xb3c4; &#xc720;&#xc804;&#xc790; &#xb370;&#xc774;&#xd130;&#xbca0;&#xc774;&#xc2a4;&#xb97c; &#xd1b5;&#xd574; &#xae30;&#xc5b5;&#xc744; &#xd0d0;&#xc0ac;&#xd560; &#xc218; &#